					<Not>
						<IfExternalCondition ConditionName="Has_Valid_Current_Offhand"/>
					</Not>
					<Not>
						<IfCooldownTimer AbilityID="Ability_Hatchet_InfectedThrow" SlotToCheckAlias="ACTIVE_MAIN_HAND_WEAPON"/>
