										<Conditional Name="Bow_Hipfire_Movespeed" ForwardInput="ForwardNav" BackwardInput="moveback" LeftInput="moveleft" RightInput="moveright" UpInput="" DownInput="" ForcedMotion="false" UseInitialCameraRotation="false" AllowVerticalMotion="false" Speed="2.25,2.25,2.25" Accel="25.00,25.00,25.00" Decel="25.00,25.00,25.00" InheritVelocity="false" BackwardsMultiplier="0.7" AnalogInputOverride="LeftAnalog" UseAnalogInputToModulateSpeed="true" MinSpeed="1.40,1.40,0.00" MinAnalogInputMag="0.20" MaxAnalogInputMag="0.80" MovementAccuracyBonus="-7.0"/>
										<Conditional Name="Combat_Nav_Stand" ForwardInput="ForwardNav" BackwardInput="moveback" LeftInput="moveleft" RightInput="moveright" UpInput="" DownInput="" ForcedMotion="false" UseInitialCameraRotation="false" AllowVerticalMotion="false" Speed="3.25,3.25,3.25" Accel="25.00,25.00,25.00" Decel="25.00,25.00,25.00" InheritVelocity="false" BackwardsMultiplier="0.7" AnalogInputOverride="LeftAnalog" UseAnalogInputToModulateSpeed="true" MinSpeed="1.40,1.40,0.00" MinAnalogInputMag="0.20" MaxAnalogInputMag="0.80" MovementAccuracyBonus="-7.0"/>
										<Conditional Name="Combat_Nav_Stand_Slow" ForwardInput="ForwardNav" BackwardInput="moveback" LeftInput="moveleft" RightInput="moveright" UpInput="" DownInput="" ForcedMotion="false" UseInitialCameraRotation="false" AllowVerticalMotion="false" Speed="3.25,3.25,3.25" Accel="25.00,25.00,25.00" Decel="25.00,25.00,25.00" InheritVelocity="false" BackwardsMultiplier="0.7" AnalogInputOverride="LeftAnalog" UseAnalogInputToModulateSpeed="true" MinSpeed="1.40,1.40,0.00" MinAnalogInputMag="0.20" MaxAnalogInputMag="0.80" MovementAccuracyBonus="-7.0"/>
										<Conditional Name="Combat_Nav_Stand_Fast" ForwardInput="ForwardNav" BackwardInput="moveback" LeftInput="moveleft" RightInput="moveright" UpInput="" DownInput="" ForcedMotion="false" UseInitialCameraRotation="false" AllowVerticalMotion="false" Speed="3.25,3.25,3.25" Accel="25.00,25.00,25.00" Decel="25.00,25.00,25.00" InheritVelocity="false" BackwardsMultiplier="0.7" AnalogInputOverride="LeftAnalog" UseAnalogInputToModulateSpeed="true" MinSpeed="1.40,1.40,0.00" MinAnalogInputMag="0.20" MaxAnalogInputMag="0.80" MovementAccuracyBonus="-7.0"/>
										<Conditional Name="Combat_Nav_Crouch" ForwardInput="ForwardNav" BackwardInput="moveback" LeftInput="moveleft" RightInput="moveright" UpInput="" DownInput="" ForcedMotion="false" UseInitialCameraRotation="false" AllowVerticalMotion="false" Speed="1.25,1.25,1.25" Accel="25.00,25.00,25.00" Decel="25.00,25.00,25.00" InheritVelocity="false" BackwardsMultiplier="0.7" AnalogInputOverride="LeftAnalog" UseAnalogInputToModulateSpeed="true" MinSpeed="1.40,1.40,0.00" MinAnalogInputMag="0.20" MaxAnalogInputMag="0.80" MovementAccuracyBonus="-3.0"/>
										<Conditional Name="Stand_Walk" ForwardInput="ForwardNav" BackwardInput="moveback" LeftInput="moveleft" RightInput="moveright" UpInput="" DownInput="" ForcedMotion="false" UseInitialCameraRotation="false" AllowVerticalMotion="false" Speed="1.65,1.65,1.65" Accel="25.00,25.00,25.00" Decel="25.00,25.00,25.00" InheritVelocity="false" BackwardsMultiplier="1.00" AnalogInputOverride="LeftAnalog" UseAnalogInputToModulateSpeed="true" MinSpeed="1.40,1.40,0.00" MinAnalogInputMag="0.20" MaxAnalogInputMag="0.80" MovementAccuracyBonus="-4.0"/>
										<Conditional Name="Crouch_Walk" ForwardInput="ForwardNav" BackwardInput="moveback" LeftInput="moveleft" RightInput="moveright" UpInput="" DownInput="" ForcedMotion="false" UseInitialCameraRotation="false" AllowVerticalMotion="false" Speed="0.8250,0.8250,0.8250" Accel="2.50,2.50,2.50" Decel="2.50,2.50,2.50" InheritVelocity="false" BackwardsMultiplier="1.00" AnalogInputOverride="LeftAnalog" UseAnalogInputToModulateSpeed="true" MinSpeed="0.80,0.80,0.00" MinAnalogInputMag="0.20" MaxAnalogInputMag="0.80" MovementAccuracyBonus="-3.0"/>
										<Conditional Name="Prone_Walk" ForwardInput="ForwardNav" BackwardInput="moveback" LeftInput="moveleft" RightInput="moveright" UpInput="" DownInput="" ForcedMotion="false" UseInitialCameraRotation="false" AllowVerticalMotion="false" Speed="0.1875,0.1875,0.00" Accel="1.00,1.00,1.00" Decel="5.00,5.00,5.00" InheritVelocity="false" BackwardsMultiplier="1.00" AnalogInputOverride="LeftAnalog" UseAnalogInputToModulateSpeed="true" MinSpeed="0.18,0.18,0.00" MinAnalogInputMag="0.20" MaxAnalogInputMag="0.80" MovementAccuracyBonus="-2.0"/>
										<Conditional Name="Crouch" ForwardInput="ForwardNav" BackwardInput="moveback" LeftInput="moveleft" RightInput="moveright" UpInput="" DownInput="" ForcedMotion="false" UseInitialCameraRotation="false" AllowVerticalMotion="false" Speed="1.65,1.65,1.65" Accel="10.00,10.00,10.00" Decel="10.00,10.00,10.00" InheritVelocity="false" BackwardsMultiplier="1.00" AnalogInputOverride="LeftAnalog" UseAnalogInputToModulateSpeed="true" MinSpeed="0.80,0.80,0.00" MinAnalogInputMag="0.20" MaxAnalogInputMag="0.80" MovementAccuracyBonus="-4.0"/>
										<Conditional Name="Prone" ForwardInput="ForwardNav" BackwardInput="moveback" LeftInput="moveleft" RightInput="moveright" UpInput="" DownInput="" ForcedMotion="false" UseInitialCameraRotation="false" AllowVerticalMotion="false" Speed="0.3750,0.3750,0.00" Accel="1.00,1.00,1.00" Decel="5.00,5.00,5.00" InheritVelocity="false" BackwardsMultiplier="1.00" AnalogInputOverride="LeftAnalog" UseAnalogInputToModulateSpeed="true" MinSpeed="0.18,0.18,0.00" MinAnalogInputMag="0.20" MaxAnalogInputMag="0.80" MovementAccuracyBonus="-7.0"/>
										<Conditional Name="NavStart" ForwardInput="ForwardNav" BackwardInput="moveback" LeftInput="moveleft" RightInput="moveright" UpInput="" DownInput="" ForcedMotion="false" UseInitialCameraRotation="false" AllowVerticalMotion="false" Speed="4,4,0" Accel="12.00,12.00,12.00" Decel="35.00,35.00,35.00" InheritVelocity="false" BackwardsMultiplier="0.7" AnalogInputOverride="LeftAnalog" UseAnalogInputToModulateSpeed="true" MinSpeed="1.40,1.40,0.00" MinAnalogInputMag="0.20" MaxAnalogInputMag="0.80" MovementAccuracyBonus="-7.0"/>
									</CameraRelativeMotion>
								</ThenActivity>
								<ElseActivity>
