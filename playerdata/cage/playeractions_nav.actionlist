<ActionList>
	<Actions>
		<Action Name="Nav_Start" FragmentName="Strafe">
			<ActivationCondition>
				<And Comment="AnyNavInput Conditional">
					<IfExternalCondition ConditionName="AnyNavInput"/>
					<Not>
						<IfExternalCondition ConditionName="AllNavInput"/>
					</Not>
					<Not>
						<IfExternalCondition ConditionName="LeftRight_NavCancel"/>
					</Not>
					<Not>
						<IfExternalCondition ConditionName="ForwardBack_NavCancel"/>
					</Not>
					<Not>
						<IfMannequinTag TagName="Walk"/>
					</Not>
					<Not>
						<IfMannequinTag TagName="Run"/>
					</Not>
					<Not>
						<IfMannequinTag TagName="Sprint"/>
					</Not>
					<Not>
						<IfMannequinMarker MarkerName="PreventNav"/>
					</Not>
					<Not>
						<IfMannequinMarker MarkerName="ResumeNav"/>
					</Not>
					<Not>
						<IfFragmentPlaying UseHostActionFragment="false" FragmentName="Strafe"/>
					</Not>
					<Not>
						<IfMannequinMarker MarkerName="ForceStop"/>
					</Not>
					<Not>
						<IfEncumbered immobilized="true"/>
					</Not>
					<Not>
						<IfP2PTrading/>
					</Not>
					<Or Comment="Only if backwards input while Haste Elixir is active">
						<Not>
							<IfHasStatusEffect StatusEffect="PotionHaste"/>
						</Not>
						<And>
							<IfHasStatusEffect StatusEffect="PotionHaste"/>
							<Or Comment="Pressing any Backward direction key">
								<And Comment="Backward input during strafe sprint">
									<IfPlayerSetting PlayerSettingName="SprintStrafeMovement"/>
									<Or>
										<IfPlayerSetting PlayerSettingName="LockedStrafeMovement"/>
										<IfCameraLockActive/>
										<IfExternalCondition ConditionName="CombatNav"/>
									</Or>
									<IfExternalCondition ConditionName="Backward_180_Input"/>
								</And>
								<And Comment="Any input during non-strafe sprint movement">
									<Not>
										<IfCameraLockActive/>
									</Not>
									<Not>
										<IfExternalCondition ConditionName="CombatNav"/>
									</Not>
									<Or Comment="SprintStrafeMovement or LockedStrafeMovement">
										<Not>
											<IfPlayerSetting PlayerSettingName="SprintStrafeMovement"/>
										</Not>
										<Not>
											<IfPlayerSetting PlayerSettingName="LockedStrafeMovement"/>
										</Not>
									</Or>
									<IfExternalCondition ConditionName="Any_Nav_Input"/>
								</And>
							</Or>
						</And>
					</Or>
					<IfCanNav/>
					<Or>
						<And>
							<Not>
								<IfFragmentPlaying UseHostActionFragment="false" FragmentName="Ability_Musket_PlayDead"/>
							</Not>
							<Not>
								<IfFragmentPlaying UseHostActionFragment="false" FragmentName="Ability_Musket_PlayDead_Loop"/>
							</Not>
						</And>
						<IfAbilityActive AbilityName="PlayAction"/>
						<And>
							<IfFragmentPlaying UseHostActionFragment="false" FragmentName="Ability_Musket_PlayDead_Loop"/>
							<Not>
								<IfMannequinTag TagName="Rifle"/>
							</Not>
						</And>
					</Or>
				</And>
			</ActivationCondition>
			<DeactivationCondition>
				<Not>
					<True/>
				</Not>
			</DeactivationCondition>
			<Activities>
				<ApplyMoveSpeedMultiplier>
					<Default MultiplierType="MoveSpeedMod"/>
				</ApplyMoveSpeedMultiplier>
				<SetWeaponAccuracyBonus ResetWhenReinstalled="true">
					<Default StanceBonus="12.0" MovementBonus="0.0"/>
					<Conditional Name="Leviathon" StanceBonus="12.0" MovementBonus="0.0"/>
					<Conditional Name="LeviathonIn" StanceBonus="25.0" MovementBonus="0.0"/>
					<Conditional Name="AimStand" StanceBonus="25.0" MovementBonus="0.0"/>
					<Conditional Name="AimCrouch" StanceBonus="25.0" MovementBonus="0.0"/>
					<Conditional Name="AimProne" StanceBonus="25.0" MovementBonus="0.0"/>
					<Conditional Name="Crouch" StanceBonus="14.0" MovementBonus="0.0"/>
					<Conditional Name="Prone" StanceBonus="18.0" MovementBonus="0.0"/>
				</SetWeaponAccuracyBonus>
				<IfThenElse Comment="Navigation audio switch IF aiming: walk else: if stamina&gt; 0 AND not eating or drinking AND not sprinting then:run" ResetWhenReinstalled="true">
					<IfCondition>
						<Or Comment="Aim">
							<IfMannequinTag TagName="Aim"/>
							<IfMannequinTag TagName="ChargedAim"/>
							<IfMannequinTag TagName="ChargedShakeAim"/>
						</Or>
					</IfCondition>
					<ThenActivity>
						<SetAudioSwitchState Comment="Walk" ResetWhenReinstalled="true" RunOnRemoteClients="true" IgnoreDataChangesAfterInstall="false">
							<Default ForceStopFxOnDataChange="false" SwitchName="Navigation" StateName="Nav_Idle" SetOnStop="false"/>
						</SetAudioSwitchState>
					</ThenActivity>
					<ElseActivity>
						<IfThenElse Comment="If Stamina &gt; 0 and not drinking eating or sprinting then: Run" ResetWhenReinstalled="true">
							<IfCondition>
								<And>
									<IfStaminaValue Comment="GreaterThan 0" ComparisonValue="0.00" ComparisonValueIsPercentage="true" ComparisonType="GreaterThan"/>
									<Not Comment="Drinking or Eating">
										<Or>
											<IfMannequinTag TagName="Drink"/>
											<IfMannequinTag TagName="Eat"/>
											<IfMannequinTag TagName="Potion"/>
										</Or>
									</Not>
									<Not Comment="Not Pressing Sprint Key">
										<IfInput InputName="Sprint"/>
									</Not>
								</And>
							</IfCondition>
							<ThenActivity>
								<SetAudioSwitchState Comment="Run" ResetWhenReinstalled="true" RunOnRemoteClients="true" IgnoreDataChangesAfterInstall="false">
									<Default ForceStopFxOnDataChange="false" SwitchName="Navigation" StateName="Nav_Run" SetOnStop="false"/>
								</SetAudioSwitchState>
							</ThenActivity>
							<ElseActivity>
								<IfThenElse ResetWhenReinstalled="true">
									<IfCondition>
										<And Comment="Not eating or drinking AND stamina LessThanOrEqual 0">
											<Not>
												<Or Comment="Eat or Drink">
													<IfMannequinTag TagName="Drink"/>
													<IfMannequinTag TagName="Eat"/>
													<IfMannequinTag TagName="Potion"/>
												</Or>
											</Not>
											<IfStaminaValue Comment="LessThanOrEqual 0" ComparisonValue="0.00" ComparisonValueIsPercentage="false" ComparisonType="LessThanOrEqual"/>
										</And>
									</IfCondition>
									<ThenActivity>
										<SetAudioSwitchState ResetWhenReinstalled="true" RunOnRemoteClients="true" IgnoreDataChangesAfterInstall="false">
											<Default ForceStopFxOnDataChange="false" SwitchName="Voice" StateName="Vo_Exhausted" SetOnStop="false"/>
										</SetAudioSwitchState>
									</ThenActivity>
									<ElseActivity>
										<IfThenElse>
											<IfCondition ResetWhenReinstalled="true">
												<Or Comment="Eat or Drink">
													<IfMannequinTag TagName="Drink"/>
													<IfMannequinTag TagName="Eat"/>
													<IfMannequinTag TagName="Potion"/>
												</Or>
											</IfCondition>
											<ThenActivity>
												<SetAudioSwitchState ResetWhenReinstalled="true" RunOnRemoteClients="true" IgnoreDataChangesAfterInstall="false">
													<Default ForceStopFxOnDataChange="false" SwitchName="Navigation" StateName="Nav_Idle" SetOnStop="false"/>
												</SetAudioSwitchState>
											</ThenActivity>
										</IfThenElse>
									</ElseActivity>
								</IfThenElse>
							</ElseActivity>
						</IfThenElse>
					</ElseActivity>
				</IfThenElse>
				<IfThenElse Comment="Breathing audio switch. IF aiming, drinking or eating: switch to breathing" ResetWhenReinstalled="true">
					<IfCondition>
						<Or Comment="Aim or Eat or Drink">
							<IfMannequinTag TagName="Aim"/>
							<IfMannequinTag TagName="ChargedAim"/>
							<IfMannequinTag TagName="ChargedShakeAim"/>
							<IfMannequinTag TagName="Eat"/>
							<IfMannequinTag TagName="Drink"/>
							<IfMannequinTag TagName="Potion"/>
						</Or>
					</IfCondition>
					<ThenActivity>
						<SetAudioSwitchState ResetWhenReinstalled="true" RunOnRemoteClients="true" IgnoreDataChangesAfterInstall="false">
							<Default SwitchName="Voice" StateName="VO_Breathing"/>
						</SetAudioSwitchState>
					</ThenActivity>
				</IfThenElse>
				<IfThenElse Comment="OneTimeScript to initialize acceleration from idle and turning" ResetWhenReinstalled="true">
					<IfCondition>
						<Not Comment="">
							<IfMannequinTag TagName="OneTimeScriptDone"/>
						</Not>
					</IfCondition>
					<ThenActivity>
						<Group>
							<IfThenElse Comment="If we already have momentum, set tag appropriately" ResetWhenReinstalled="true">
								<IfCondition>
									<MoveSpeed Comment="GreaterThanOrEqual 1.65 Walk Speed" Value="1.65" Comparison="GreaterThanOrEqual"/>
								</IfCondition>
								<ThenActivity>
									<SetMannequinTag ResetWhenReinstalled="false">
										<Default TagName="Variant1" OnEnter="Set" OnExit="NoEffect" DelayTimer="0.00" DelayBlackboardKey=""/>
									</SetMannequinTag>
								</ThenActivity>
							</IfThenElse>
							<IfThenElse Comment="Detect 180s" ResetWhenReinstalled="false">
								<IfCondition>
									<IfExternalCondition ConditionName="180_Turn_Detected"/>
								</IfCondition>
								<ThenActivity>
									<SetMannequinTag ResetWhenReinstalled="false">
										<Default TagName="FirstAttack" OnEnter="Set" OnExit="NoEffect" DelayTimer="0.00" DelayBlackboardKey=""/>
									</SetMannequinTag>
								</ThenActivity>
							</IfThenElse>
							<SetMannequinTag ResetWhenReinstalled="false">
								<Default TagName="OneTimeScriptDone" OnEnter="Set" OnExit="NoEffect" DelayTimer="0.00" DelayBlackboardKey=""/>
							</SetMannequinTag>
						</Group>
					</ThenActivity>
				</IfThenElse>
				<IfThenElse Comment="Handle Acceleration from Idle Timer" ResetWhenReinstalled="true">
					<IfCondition>
						<Not>
							<IfMannequinTag TagName="Variant1"/>
						</Not>
					</IfCondition>
					<ThenActivity>
						<SetMannequinTag ResetWhenReinstalled="false">
							<Default TagName="Variant1" OnEnter="Set" OnExit="NoEffect" DelayTimer="0.50" DelayBlackboardKey=""/>
						</SetMannequinTag>
					</ThenActivity>
					<ElseActivity>
						<SetMannequinTag ResetWhenReinstalled="false">
							<Default TagName="Variant1" OnEnter="Set" OnExit="NoEffect" DelayTimer="0.00" DelayBlackboardKey=""/>
						</SetMannequinTag>
					</ElseActivity>
				</IfThenElse>
				<IfThenElse Comment="Camera lock on movement" ResetWhenReinstalled="true">
					<IfCondition>
						<And>
							<Not Comment="Camera Lock Active">
								<IfCameraLockActive/>
							</Not>
							<Not Comment="Aim Tag">
								<IfMannequinTag TagName="Aim"/>
							</Not>
							<Not Comment="HipAim">
								<IfMannequinTag TagName="HipAim"/>
							</Not>
							<Not Comment="ChargedAim tag">
								<IfMannequinTag TagName="ChargedAim"/>
							</Not>
							<Not Comment="Shoot tag">
								<IfMannequinTag TagName="Shoot"/>
							</Not>
							<Not Comment="Reload tag">
								<IfMannequinTag TagName="Reload"/>
							</Not>
							<Or>
								<Not Comment="Blocking">
									<IfMannequinTag TagName="Blocking"/>
								</Not>
								<And>
									<IfMannequinTag TagName="Blocking"/>
									<Not>
										<IfPlayerSetting PlayerSettingName="AttackFacing"/>
									</Not>
								</And>
							</Or>
							<Not Comment="Locked Strafe Movement">
								<IfPlayerSetting PlayerSettingName="LockedStrafeMovement"/>
							</Not>
							<Not>
								<IfExternalCondition ConditionName="CombatNav"/>
							</Not>
						</And>
					</IfCondition>
					<ThenActivity>
						<Group Comment="Free Run (NO Camera Lock)">
							<InputRelativeFacing>
								<Default Turnrate="0.50" UseExponentialTurnRate="true" RepeatInitialInput="false" UseClosestTarget="false" ForwardInput="ForwardNav" BackwardInput="moveback" LeftInput="moveleft" RightInput="moveright" AnalogInputOverride="LeftAnalog"/>
								<Conditional Name="NavStart" Turnrate="0.50" UseExponentialTurnRate="true" RepeatInitialInput="false" UseClosestTarget="false" ForwardInput="ForwardNav" BackwardInput="moveback" LeftInput="moveleft" RightInput="moveright" AnalogInputOverride="LeftAnalog"/>
							</InputRelativeFacing>
							<SetMannequinTag ResetWhenReinstalled="true">
								<Default TagName="ForwardNav" OnEnter="Set" OnExit="NoEffect" DelayTimer="0.00" DelayBlackboardKey=""/>
							</SetMannequinTag>
							<CharacterRelativeMovement>
								<Default ForwardInput="" BackwardInput="" LeftInput="" RightInput="" UpInput="" DownInput="" Forced="true" AlignVelocity="false" Speed="0.00,4.00,0.00" Accel="0.00,7.00,0.00" Decel="30.00,30.00,30.00" AnalogInputOverride="None"/>
								<Conditional Name="Wade" ForwardInput="ForwardNav" BackwardInput="moveback" LeftInput="moveleft" RightInput="moveright" UpInput="" DownInput="" Forced="true" AlignVelocity="false" Speed="0.00,1.65,0.00" Accel="0.00,10.00,0.00" Decel="10.00,10.00,10.00" AnalogInputOverride="LeftAnalog" UseAnalogInputToModulateSpeed="true" MinSpeed="1.40,1.40,0.00" MinAnalogInputMag="0.20" MaxAnalogInputMag="0.80"/>
								<Conditional Name="Stand_Walk" ForwardInput="ForwardNav" BackwardInput="moveback" LeftInput="moveleft" RightInput="moveright" UpInput="" DownInput="" Forced="true" AlignVelocity="false" Speed="0.00,1.65,0.00" Accel="0.00,25.00,0.00" Decel="25.00,25.00,25.00" AnalogInputOverride="LeftAnalog" UseAnalogInputToModulateSpeed="true" MinSpeed="1.40,1.40,1.40" MinAnalogInputMag="0.20" MaxAnalogInputMag="0.80"/>
								<Conditional Name="Crouch_Walk" ForwardInput="ForwardNav" BackwardInput="moveback" LeftInput="moveleft" RightInput="moveright" UpInput="" DownInput="" Forced="true" AlignVelocity="false" Speed="0.00,0.8250,0.00" Accel="0.00,2.5,0.00" Decel="2.50,2.50,0.00" AnalogInputOverride="LeftAnalog" UseAnalogInputToModulateSpeed="true" MinSpeed="0.80,0.80,0.00" MinAnalogInputMag="0.20" MaxAnalogInputMag="0.80"/>
								<Conditional Name="Prone_Walk" ForwardInput="ForwardNav" BackwardInput="moveback" LeftInput="moveleft" RightInput="moveright" UpInput="" DownInput="" Forced="true" AlignVelocity="false" Speed="0.00,0.1875,0.00" Accel="0.00,1.00,0.00" Decel="5.00,5.00,5.00" AnalogInputOverride="LeftAnalog" UseAnalogInputToModulateSpeed="true" MinSpeed="0.18,0.18,0.18" MinAnalogInputMag="0.20" MaxAnalogInputMag="0.80"/>
								<Conditional Name="Crouch" ForwardInput="ForwardNav" BackwardInput="moveback" LeftInput="moveleft" RightInput="moveright" UpInput="" DownInput="" Forced="true" AlignVelocity="false" Speed="0.00,1.65,0.00" Accel="0.00,10.00,0.00" Decel="10.00,10.00,10.00" AnalogInputOverride="LeftAnalog" UseAnalogInputToModulateSpeed="true" MinSpeed="0.80,0.80,0.00" MinAnalogInputMag="0.20" MaxAnalogInputMag="0.80"/>
								<Conditional Name="Prone" ForwardInput="ForwardNav" BackwardInput="moveback" LeftInput="moveleft" RightInput="moveright" UpInput="" DownInput="" Forced="true" AlignVelocity="false" Speed="0.00,.3750,0.00" Accel="0.00,1.00,0.00" Decel="5.00,5.00,5.00" AnalogInputOverride="LeftAnalog" UseAnalogInputToModulateSpeed="true" MinSpeed="0.18,0.18,0.00" MinAnalogInputMag="0.20" MaxAnalogInputMag="0.80"/>
								<Conditional Name="NavStart" ForwardInput="ForwardNav" BackwardInput="moveback" LeftInput="moveleft" RightInput="moveright" UpInput="" DownInput="" Forced="true" AlignVelocity="false" Speed="0.00,4.00,0.00" Accel="0.00,12.00,0.00" Decel="35.00,35.00,0.00" AnalogInputOverride="LeftAnalog" UseAnalogInputToModulateSpeed="true" MinSpeed="1.40,1.40,0.00" MinAnalogInputMag="0.20" MaxAnalogInputMag="0.80"/>
							</CharacterRelativeMovement>
						</Group>
					</ThenActivity>
					<ElseActivity>
						<Group Comment="Camera Lock Strafe Movement">
							<IfThenElse ResetWhenReinstalled="true" Comment="Check for Bow Accuracy">
								<IfCondition>
									<IfMannequinTag TagName="Bow"/>
								</IfCondition>
								<ThenActivity>
									<CameraRelativeMotion>
										<Default ForwardInput="ForwardNav" BackwardInput="moveback" LeftInput="moveleft" RightInput="moveright" UpInput="" DownInput="" ForcedMotion="false" UseInitialCameraRotation="false" AllowVerticalMotion="false" Speed="4,4,0" Accel="35.00,35.00,35.00" Decel="35.00,35.00,35.00" InheritVelocity="false" BackwardsMultiplier="0.7" AnalogInputOverride="LeftAnalog" UseAnalogInputToModulateSpeed="true" MinSpeed="1.40,1.40,0.00" MinAnalogInputMag="0.20" MaxAnalogInputMag="0.80" MovementAccuracyBonus="-12.0"/>
