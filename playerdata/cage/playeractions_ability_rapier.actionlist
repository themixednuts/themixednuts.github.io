				<And>
					<Or Comment="Backtick Check">
						<IfActiveAbilityMoveName AbilityName="Ability_Rapier_Slash"/>
						<And>
							<IfActiveAbilityMoveName AbilityName="Ability_Rapier_Slash" BackTicks="20"/>
