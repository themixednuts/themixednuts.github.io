<ActionList>
	<Actions>
		<Action Name="VoidGauntlet_Harvest" FragmentName="Ability_VoidGauntlet_Harvest">
			<ActivationCondition>
				<And>
					<And>
						<IfHealthPercentage Comparison="GreaterThanOrEqual" Percentage="0.2"/>
						<IfInput InputName="block_depressed"/>
						<Not>
							<IfActionStatus ActionName="VoidGauntlet_Blade_Active" Status="Installed"/>
						</Not>
						<Or Comment="Stance Check">
							<IfExternalCondition ConditionName="Trigger_From_Sheathed"/>
							<IfMannequinTagInItemSlot TagName="VoidGauntlet" SlotToCheckAlias="ACTIVE_MAIN_HAND_WEAPON"/>
						</Or>
					</And>
					<Not>
						<Or>
							<IfActionStatus ActionName="Sheathe_MainHand_1_UB" Status="Installed"/>
							<IfActionStatus ActionName="Sheathe_MainHand_2_UB" Status="Installed"/>
							<IfActionStatus ActionName="Sheathe_MainHand_3_UB" Status="Installed"/>
						</Or>
					</Not>
					<Not>
						<IfExternalCondition ConditionName="Throwable_Equipped"/>
					</Not>
					<Not>
						<IfMannequinTag TagName="FishingPole"/>
					</Not>
					<Not>
						<IfInput InputName="Placing_Building"/>
					</Not>
					<Not>
						<IfMannequinTag TagName="AttackAction"/>
					</Not>
					<Not>
						<IfMannequinTagInItemSlot TagName="Tool" SlotToCheckAlias="ACTIVE_MAIN_HAND_WEAPON"/>
					</Not>
					<IfCanResizeCharacterController ResizeType="Default"/>
					<Not>
						<IfWaterDepth Comparison="GreaterThan" WaterDepth="Shallow"/>
					</Not>
					<IfHasWeaponPrerequisites WeaponSlot="ACTIVE_MAIN_HAND_WEAPON"/>
					<Not>
						<HeightToGround ActivationHeight="0.5"/>
					</Not>
					<Not>
						<IfExternalCondition ConditionName="Prevent_Attack"/>
					</Not>
					<Not>
						<IfMannequinTag TagName="DeathsDoor"/>
					</Not>
				</And>
			</ActivationCondition>
			<DeactivationCondition>
				<IfFragmentDone UseHostActionFragment="true" ExitOffsetSeconds="0.0"/>
			</DeactivationCondition>
			<Activities>
				<IfThenElse ResetWhenReinstalled="false" Comment="Unsheathe active weapon and set tags from sheathed">
					<IfCondition>
						<IfExternalCondition ConditionName="Trigger_From_Sheathed"/>
					</IfCondition>
					<ThenActivity>
						<Group>
							<SheatheWeapon ResetWhenReinstalled="false">
								<Default SheatheOption="UnSheathe" SlotToSheathe="ACTIVE_MAIN_HAND_WEAPON" OnEnter="Set" OnExit="NoEffect"/>
							</SheatheWeapon>
							<SetMannequinTagFromSource ResetWhenReinstalled="false" Comment="Set Main Hand Tags">
								<Default TagSource="PaperdollSlot" SourceSlot="ACTIVE_MAIN_HAND_WEAPON" OnEnter="Set" OnExit="NoEffect"/>
							</SetMannequinTagFromSource>
							<SetMannequinTag ResetWhenReinstalled="false">
								<Default TagName="AutoUnsheathe" OnEnter="NoEffect" OnExit="Unset"/>
							</SetMannequinTag>
							<ChangeActionFragment ResetWhenReinstalled="false">
								<Default UseDefaultFragment="true"/>
							</ChangeActionFragment>
						</Group>
					</ThenActivity>
				</IfThenElse>
				<IfThenElse Comment="Toggle status effect" ResetWhenReinstalled="false">
					<IfCondition>
						<IfMannequinMarker MarkerName="DrainHealth"/>
					</IfCondition>
					<ThenActivity>
						<Group>
							<ModifyStatusEffects>
								<Default>
									<EffectsToApplyOnEnter>
										<EffectId EffectId="VoidGauntlet_Harvest_HealthDrain"/>
										<EffectId EffectId="VoidGauntlet_Harvest_ManaGain"/>
									</EffectsToApplyOnEnter>
									<EffectsToRemoveOnExit>
										<EffectId EffectId="VoidGauntlet_Harvest_HealthDrain"/>
										<EffectId EffectId="VoidGauntlet_Harvest_ManaGain"/>
										<EffectId EffectId="VoidGauntlet_Harvest_ReducedCostLowMana"/>
									</EffectsToRemoveOnExit>
								</Default>
							</ModifyStatusEffects>
							<IfThenElse Comment="Apply health drain reduction passive" ResetWhenReinstalled="false">
								<IfCondition>
									<And>
										<IfAbilityActive AbilityName="Passive_VoidGauntlet_HarvestReducedCostLowMana"/>
										<IfManaValue ComparisonType="LessThan" ComparisonValueIsPercentage="True" ComparisonValue="0.25"/>
									</And>
								</IfCondition>
								<ThenActivity>
									<ModifyStatusEffects>
										<Default>
											<EffectsToApplyOnEnter>
												<EffectId EffectId="VoidGauntlet_Harvest_ReducedCostLowMana"/>
											</EffectsToApplyOnEnter>
											<EffectsToRemoveOnExit>
												<EffectId EffectId="VoidGauntlet_Harvest_ReducedCostLowMana"/>
											</EffectsToRemoveOnExit>
										</Default>
									</ModifyStatusEffects>
								</ThenActivity>
							</IfThenElse>
						</Group>
					</ThenActivity>
				</IfThenElse>
				<IfThenElse Comment="Apply CDRWhileHarvest status effect if unlocked" ResetWhenReinstalled="false">
					<IfCondition>
						<IfMannequinMarker MarkerName="DrainHealth"/>
					</IfCondition>
					<ThenActivity>
						<IfThenElse ResetWhenReinstalled="false">
							<IfCondition>
								<IfAbilityActive AbilityName="Passive_VoidGauntlet_CDRWhileHarvest"/>
							</IfCondition>
							<ThenActivity>
								<ModifyStatusEffects>
									<Default>
										<EffectsToApplyOnEnter>
											<EffectId EffectId="VoidGauntlet_CDRWhileHarvest_Check"/>
										</EffectsToApplyOnEnter>
										<EffectsToRemoveOnExit>
											<EffectId EffectId="VoidGauntlet_CDRWhileHarvest_Check"/>
										</EffectsToRemoveOnExit>
									</Default>
								</ModifyStatusEffects>
							</ThenActivity>
						</IfThenElse>
					</ThenActivity>
				</IfThenElse>
				<ModifyStatusEffects>
					<Default>
						<EffectsToRemoveOnExit>
							<EffectId EffectId="VoidGauntlet_Harvest_HealthDrain"/>
							<EffectId EffectId="VoidGauntlet_Harvest_ManaGain"/>
							<EffectId EffectId="VoidGauntlet_Harvest_ReducedCostLowMana"/>
							<EffectId EffectId="VoidGauntlet_CDRWhileHarvest_Check"/>
						</EffectsToRemoveOnExit>
					</Default>
				</ModifyStatusEffects>
				<DisableInteractor>
					<Default OnEnter="Disable" OnExit="Enable"/>
				</DisableInteractor>
				<SetMannequinTag ResetWhenReinstalled="false">
					<Default TagName="AttackAction" OnEnter="Set" OnExit="Unset"/>
				</SetMannequinTag>
				<IfThenElse Comment="Stand if crouching or prone" ResetWhenReinstalled="false">
					<IfCondition>
						<Or>
							<IfMannequinTag TagName="Crouch"/>
							<IfMannequinTag TagName="Prone"/>
						</Or>
					</IfCondition>
					<ThenActivity>
						<Group>
							<SetMannequinTag ResetWhenReinstalled="false">
								<Default TagName="Stand" OnEnter="Set" OnExit="NoEffect"/>
							</SetMannequinTag>
							<SetMannequinTag ResetWhenReinstalled="false">
								<Default TagName="Stand_Desired" OnEnter="Set" OnExit="NoEffect"/>
							</SetMannequinTag>
						</Group>
					</ThenActivity>
				</IfThenElse>
				<TrackTimeInAction>
					<Default/>
				</TrackTimeInAction>
			</Activities>
		</Action>
		<Action Name="VoidGauntlet_Blade_Start" FragmentName="Ability_VoidGauntlet_Blade_Start">
			<ActivationCondition>
				<And>
					<And>
						<Or Comment="Backtick Check">
							<IfActiveAbilityMoveName AbilityName="Ability_VoidGauntlet_VoidBlade"/>
							<And>
								<IfActiveAbilityMoveName AbilityName="Ability_VoidGauntlet_VoidBlade" BackTicks="20"/>
								<IfActionStatus ActionName="UseBacktickInputs" Status="Installed"/>
							</And>
						</Or>
						<Not>
							<IfCooldownTimer AbilityID="Ability_VoidGauntlet_VoidBlade" SlotToCheckAlias="ACTIVE_MAIN_HAND_WEAPON"/>
						</Not>
						<IfManaCost ComparisonType="GreaterThanOrEqual" CostID="VoidGauntlet_VoidBlade" SlotToCheckAlias="ACTIVE_MAIN_HAND_WEAPON"/>
						<Not>
							<Or>
								<IfActionStatus ActionName="VoidGauntlet_Blade_Start" Status="Installed"/>
								<IfActionStatus ActionName="VoidGauntlet_Blade_Active" Status="Installed"/>
							</Or>
						</Not>
					</And>
					<And>
						<Not>
							<Or>
								<And>
									<Not>
										<IfItemInSlot SlotToCheckAlias="MAIN_HAND_OPTION_1" RequireSlotValid="true" RequireSlotActive="true"/>
									</Not>
									<IfInput InputName="Use_MainHandSlot_1" BackTicks="20"/>
								</And>
								<And>
									<Not>
										<IfItemInSlot SlotToCheckAlias="MAIN_HAND_OPTION_2" RequireSlotValid="true" RequireSlotActive="true"/>
									</Not>
									<IfInput InputName="Use_MainHandSlot_2" BackTicks="20"/>
								</And>
								<IfInput InputName="quickslot-weaponSwap" BackTicks="20"/>
								<IfActionStatus ActionName="Unsheathe_MainHand_1_UB" Status="Pending"/>
								<IfActionStatus ActionName="Unsheathe_MainHand_2_UB" Status="Pending"/>
								<IfActionStatus ActionName="Switch_MainHand_1_UB" Status="Pending"/>
								<IfActionStatus ActionName="Switch_MainHand_2_UB" Status="Pending"/>
							</Or>
						</Not>
						<Not>
							<IfExternalCondition ConditionName="Throwable_Equipped"/>
						</Not>
						<Not>
							<IfMannequinTag TagName="FishingPole"/>
						</Not>
						<Not>
							<IfInput InputName="Placing_Building"/>
						</Not>
						<Not>
							<IfMannequinTag TagName="AttackAction"/>
						</Not>
						<Not>
							<IfMannequinTagInItemSlot TagName="Tool" SlotToCheckAlias="ACTIVE_MAIN_HAND_WEAPON"/>
						</Not>
						<IfCanResizeCharacterController ResizeType="Default"/>
						<Not>
							<IfWaterDepth Comparison="GreaterThan" WaterDepth="Shallow"/>
						</Not>
						<IfHasWeaponPrerequisites WeaponSlot="ACTIVE_MAIN_HAND_WEAPON"/>
						<Not>
							<HeightToGround ActivationHeight="0.5"/>
						</Not>
						<Not>
							<IfEncumbered/>
						</Not>
						<Not>
							<IsSilenced/>
						</Not>
						<Not>
							<IfHasStatusEffect StatusEffect="PreventAttacksAndAbilities"/>
						</Not>
						<Or Comment="Stance Check">
							<IfExternalCondition ConditionName="Trigger_From_Sheathed"/>
							<IfMannequinTagInItemSlot TagName="VoidGauntlet" SlotToCheckAlias="ACTIVE_MAIN_HAND_WEAPON"/>
						</Or>
					</And>
				</And>
			</ActivationCondition>
			<DeactivationCondition>
				<IfFragmentDone UseHostActionFragment="true" ExitOffsetSeconds="0.0"/>
			</DeactivationCondition>
			<Activities>
				<ModifyStatusEffects>
					<Default>
						<EffectsToRemoveOnEnter>
							<EffectId EffectId="PotionHaste"/>
						</EffectsToRemoveOnEnter>
					</Default>
				</ModifyStatusEffects>
				<IfThenElse Comment="On Attack start set Stand_Desired if Standing" ResetWhenReinstalled="false">
					<IfCondition>
						<IfMannequinTag TagName="Stand"/>
					</IfCondition>
					<ThenActivity>
						<Group>
							<SetMannequinTag ResetWhenReinstalled="false">
								<Default TagName="Stand_Desired" OnEnter="Set" OnExit="NoEffect"/>
							</SetMannequinTag>
						</Group>
					</ThenActivity>
				</IfThenElse>
				<ClearInput>
					<Default InputName="attack_primary" OnEnter="Clear" OnExit="NoEffect"/>
				</ClearInput>
				<ClearInput>
					<Default InputName="attack_primary_hold" OnEnter="Clear" OnExit="NoEffect"/>
				</ClearInput>
				<ClearInput>
					<Default InputName="attack_primary_virtual" OnEnter="Clear" OnExit="NoEffect"/>
				</ClearInput>
				<ClearInput>
					<Default InputName="attack_shoot" OnEnter="Clear" OnExit="NoEffect"/>
				</ClearInput>
				<ClearInput>
					<Default InputName="aim_shoot_hold" OnEnter="Clear" OnExit="NoEffect"/>
				</ClearInput>
				<ClearInput>
					<Default InputName="aim_shoot_depressed" OnEnter="Clear" OnExit="NoEffect"/>
				</ClearInput>
				<ClearInput>
					<Default InputName="attack_alternate_depressed_virtual" OnEnter="Clear" OnExit="NoEffect"/>
				</ClearInput>
				<ClearInput>
					<Default InputName="rune_ability" OnEnter="Clear" OnExit="NoEffect"/>
				</ClearInput>
				<ClearInput>
					<Default InputName="ability1" OnEnter="NoEffect" OnExit="NoEffect"/>
					<Conditional Name="Unsheathe_Input_Pressed" InputName="ability1" OnEnter="NoEffect" OnExit="Clear"/>
				</ClearInput>
				<ClearInput>
					<Default InputName="ability2" OnEnter="NoEffect" OnExit="NoEffect"/>
					<Conditional Name="Unsheathe_Input_Pressed" InputName="ability2" OnEnter="NoEffect" OnExit="Clear"/>
				</ClearInput>
				<ClearInput>
					<Default InputName="ability3" OnEnter="NoEffect" OnExit="NoEffect"/>
					<Conditional Name="Unsheathe_Input_Pressed" InputName="ability3" OnEnter="NoEffect" OnExit="Clear"/>
				</ClearInput>
				<ClearInput>
					<Default InputName="attack_primary" OnEnter="NoEffect" OnExit="NoEffect"/>
					<Conditional Name="Unsheathe_Input_Pressed" InputName="attack_primary" OnEnter="NoEffect" OnExit="Clear"/>
				</ClearInput>
				<ClearInput>
					<Default InputName="attack_primary_hold" OnEnter="NoEffect" OnExit="NoEffect"/>
					<Conditional Name="Unsheathe_Input_Pressed" InputName="attack_primary_hold" OnEnter="NoEffect" OnExit="Clear"/>
				</ClearInput>
				<ClearInput>
					<Default InputName="attack_primary_virtual" OnEnter="NoEffect" OnExit="NoEffect"/>
					<Conditional Name="Unsheathe_Input_Pressed" InputName="attack_primary_virtual" OnEnter="NoEffect" OnExit="Clear"/>
				</ClearInput>
				<ClearInput>
					<Default InputName="attack_shoot" OnEnter="NoEffect" OnExit="NoEffect"/>
					<Conditional Name="Unsheathe_Input_Pressed" InputName="attack_shoot" OnEnter="NoEffect" OnExit="Clear"/>
				</ClearInput>
				<ClearInput>
					<Default InputName="aim_shoot_hold" OnEnter="NoEffect" OnExit="NoEffect"/>
					<Conditional Name="Unsheathe_Input_Pressed" InputName="aim_shoot_hold" OnEnter="NoEffect" OnExit="Clear"/>
				</ClearInput>
				<ClearInput>
					<Default InputName="aim_shoot_depressed" OnEnter="NoEffect" OnExit="NoEffect"/>
					<Conditional Name="Unsheathe_Input_Pressed" InputName="aim_shoot_depressed" OnEnter="NoEffect" OnExit="Clear"/>
				</ClearInput>
				<BlockEnteringStore/>
				<IfThenElse ResetWhenReinstalled="false" Comment="Unsheathe active weapon and set tags from sheathed">
					<IfCondition>
						<IfExternalCondition ConditionName="Trigger_From_Sheathed"/>
					</IfCondition>
					<ThenActivity>
						<Group>
							<SheatheWeapon ResetWhenReinstalled="false">
								<Default SheatheOption="UnSheathe" SlotToSheathe="ACTIVE_MAIN_HAND_WEAPON" OnEnter="Set" OnExit="NoEffect"/>
							</SheatheWeapon>
							<SetMannequinTagFromSource ResetWhenReinstalled="false" Comment="Set Main Hand Tags">
								<Default TagSource="PaperdollSlot" SourceSlot="ACTIVE_MAIN_HAND_WEAPON" OnEnter="Set" OnExit="NoEffect"/>
							</SetMannequinTagFromSource>
							<SetMannequinTag ResetWhenReinstalled="false">
								<Default TagName="AutoUnsheathe" OnEnter="NoEffect" OnExit="Unset"/>
							</SetMannequinTag>
							<ChangeActionFragment ResetWhenReinstalled="false">
								<Default UseDefaultFragment="true"/>
							</ChangeActionFragment>
						</Group>
					</ThenActivity>
				</IfThenElse>
				<ClearActiveAbilityMoveInput>
					<Default AbilityName="Ability_VoidGauntlet_VoidBlade" OnEnter="Clear" OnExit="NoEffect"/>
				</ClearActiveAbilityMoveInput>
				<IfThenElse ResetWhenReinstalled="false" Comment="Activate CoolDown after startup">
					<IfCondition>
						<Not>
							<IfExternalCondition ConditionName="Ability_Startup"/>
						</Not>
					</IfCondition>
					<ThenActivity>
						<Group>
							<SetCooldownTimer>
								<Default AbilityID="Ability_VoidGauntlet_VoidBlade" SlotToCheckAlias="ACTIVE_MAIN_HAND_WEAPON"/>
							</SetCooldownTimer>
							<PerformActiveAbility>
								<Default AbilityName="Ability_VoidGauntlet_VoidBlade"/>
							</PerformActiveAbility>
						</Group>
					</ThenActivity>
				</IfThenElse>
				<IfThenElse Comment="Toggle status effect" ResetWhenReinstalled="false">
					<IfCondition>
						<IfMannequinMarker MarkerName="ActivateBlade"/>
					</IfCondition>
					<ThenActivity>
						<ModifyStatusEffects>
							<Default>
								<EffectsToApplyOnEnter>
									<EffectId EffectId="VoidGauntlet_VoidBlade"/>
								</EffectsToApplyOnEnter>
							</Default>
						</ModifyStatusEffects>
					</ThenActivity>
				</IfThenElse>
				<ModifyStatusEffects>
					<Default/>
					<Conditional Name="VoidBlade_ApplyOnExit">
						<EffectsToApplyOnExit>
							<EffectId EffectId="VoidGauntlet_VoidBlade"/>
						</EffectsToApplyOnExit>
					</Conditional>
				</ModifyStatusEffects>
				<DisableInteractor>
					<Default OnEnter="Disable" OnExit="Enable"/>
				</DisableInteractor>
				<IfThenElse Comment="Character Facing Logic">
					<IfCondition>
						<Or>
							<IfCameraLockActive/>
							<IfPlayerSetting PlayerSettingName="AttackFacing"/>
						</Or>
					</IfCondition>
					<ThenActivity>
						<CameraRelativeFacing>
							<Default/>
							<Conditional Name="ApplyRotation" TurnRate="0.40" UseExponentialTurnRate="true"/>
							<Conditional Name="ApplyRotationFast" TurnRate="0.90" UseExponentialTurnRate="true"/>
						</CameraRelativeFacing>
					</ThenActivity>
					<ElseActivity>
						<InputRelativeFacing>
							<Default ForwardInput="ForwardNav" BackwardInput="moveback" LeftInput="moveleft" RightInput="moveright"/>
							<Conditional Name="ApplyRotation" TurnRate="960.00" ForwardInput="ForwardNav" BackwardInput="moveback" LeftInput="moveleft" RightInput="moveright" AnalogInputOverride="LeftAnalog"/>
							<Conditional Name="ApplyRotationFast" TurnRate="50000.00" ForwardInput="ForwardNav" BackwardInput="moveback" LeftInput="moveleft" RightInput="moveright" AnalogInputOverride="LeftAnalog"/>
						</InputRelativeFacing>
					</ElseActivity>
				</IfThenElse>
				<SetMannequinTag ResetWhenReinstalled="false">
					<Default TagName="AttackAction" OnEnter="Set" OnExit="Unset"/>
				</SetMannequinTag>
				<IfThenElse Comment="Stand if crouching or prone" ResetWhenReinstalled="false">
					<IfCondition>
						<Or>
							<IfMannequinTag TagName="Crouch"/>
							<IfMannequinTag TagName="Prone"/>
						</Or>
					</IfCondition>
					<ThenActivity>
						<Group>
							<SetMannequinTag ResetWhenReinstalled="false">
								<Default TagName="Stand" OnEnter="Set" OnExit="NoEffect"/>
							</SetMannequinTag>
							<SetMannequinTag ResetWhenReinstalled="false">
								<Default TagName="Stand_Desired" OnEnter="Set" OnExit="NoEffect"/>
							</SetMannequinTag>
						</Group>
					</ThenActivity>
				</IfThenElse>
			</Activities>
		</Action>
		<Action Name="VoidGauntlet_Blade_Active">
			<ActivationCondition>
				<And>
					<IfActionStatus ActionName="VoidGauntlet_Blade_Start" Status="Installed"/>
				</And>
			</ActivationCondition>
			<DeactivationCondition>
				<Or>
					<IfActionStatus ActionName="VoidGauntlet_Blade_Active" Status="Installed" ActiveTime="15.0f" ActiveTimeComparisonType="GreaterThanOrEqual"/>
					<Not>
						<IfMannequinTag TagName="VoidGauntlet"/>
					</Not>
					<Not>
						<IfMannequinTag TagName="TEMP_WeaponMaster"/>
					</Not>
				</Or>
			</DeactivationCondition>
			<Activities>
				<SetMannequinTag ResetWhenReinstalled="false">
					<Default TagName="TEMP_WeaponMaster" OnEnter="Set" OnExit="Unset"/>
				</SetMannequinTag>
				<ModifyStatusEffects>
					<Default RemoveNothingOnMigrate="true">
						<EffectsToRemoveOnExit>
							<EffectId EffectId="VoidGauntlet_VoidBlade"/>
						</EffectsToRemoveOnExit>
					</Default>
				</ModifyStatusEffects>
				<TrackTimeInAction>
					<Default/>
				</TrackTimeInAction>
				<IfThenElse ResetWhenReinstalled="false" Comment="Apply Fortify on start if upgrade unlocked">
					<IfCondition>
						<And>
							<IfAbilityActive AbilityName="Ability_VoidGauntlet_VoidBlade_Fortify"/>
						</And>
					</IfCondition>
					<ThenActivity>
						<ModifyStatusEffects>
							<Default>
								<EffectsToApplyOnEnter>
									<EffectId EffectId="VoidGauntlet_VoidBlade_Fortify"/>
								</EffectsToApplyOnEnter>
							</Default>
						</ModifyStatusEffects>
					</ThenActivity>
				</IfThenElse>
			</Activities>
		</Action>
		<Action Name="VoidGauntlet_Blade_FX_Toggle" RunOnRemoteClients="true">
			<ActivationCondition>
				<And>
					<Or>
						<IfActionStatus ActionName="VoidGauntlet_Blade_Active" Status="Installed"/>
					</Or>
					<Not>
						<Or>
							<IfFragmentPlaying UseHostActionFragment="false" FragmentName="Mantle_High"/>
							<IfFragmentPlaying UseHostActionFragment="false" FragmentName="Mantle_Low"/>
							<IfFragmentPlaying UseHostActionFragment="false" FragmentName="Ability_VoidGauntlet_NullChamber"/>
							<IfFragmentPlaying UseHostActionFragment="false" FragmentName="Ability_VoidGauntlet_Scream"/>
							<IfFragmentPlaying UseHostActionFragment="false" FragmentName="Ability_VoidGauntlet_DecayingOrb"/>
							<IfFragmentPlaying UseHostActionFragment="false" FragmentName="Ability_VoidGauntlet_DrainingTether"/>
							<IfFragmentPlaying UseHostActionFragment="false" FragmentName="Ability_VoidGauntlet_EssenceRupture"/>
							<IfFragmentPlaying UseHostActionFragment="false" FragmentName="Ability_VoidGauntlet_Harvest"/>
							<IfFragmentPlaying UseHostActionFragment="false" FragmentName="Dodge_Roll_Fast"/>
							<IfFragmentPlaying UseHostActionFragment="false" FragmentName="Attack_Start_Charge_UpperBody"/>
							<IfMannequinTag TagName="Potion"/>
							<IfMannequinTag TagName="WeaponOil"/>
							<IfMannequinTag TagName="Eat"/>
							<IfMannequinTag TagName="Drink"/>
							<IfMannequinTag TagName="Bandage"/>
							<IfMannequinTag TagName="Call"/>
							<IfMannequinMarker MarkerName="HideVoidBlade"/>
						</Or>
					</Not>
				</And>
			</ActivationCondition>
			<DeactivationCondition>
				<Or>
					<And>
						<Not>
							<IfActionStatus ActionName="VoidGauntlet_Blade_Active" Status="Installed"/>
						</Not>
						<Not>
							<IfMannequinTag TagName="AttackAction"/>
						</Not>
					</And>
					<Not>
						<IfMannequinTag TagName="VoidGauntlet"/>
					</Not>
					<Or>
						<IfFragmentPlaying UseHostActionFragment="false" FragmentName="Mantle_High"/>
						<IfFragmentPlaying UseHostActionFragment="false" FragmentName="Mantle_Low"/>
						<IfFragmentPlaying UseHostActionFragment="false" FragmentName="Ability_VoidGauntlet_NullChamber"/>
						<IfFragmentPlaying UseHostActionFragment="false" FragmentName="Ability_VoidGauntlet_Scream"/>
						<IfFragmentPlaying UseHostActionFragment="false" FragmentName="Ability_VoidGauntlet_DecayingOrb"/>
						<IfFragmentPlaying UseHostActionFragment="false" FragmentName="Ability_VoidGauntlet_DrainingTether"/>
						<IfFragmentPlaying UseHostActionFragment="false" FragmentName="Ability_VoidGauntlet_EssenceRupture"/>
						<IfFragmentPlaying UseHostActionFragment="false" FragmentName="Ability_VoidGauntlet_Harvest"/>
						<IfFragmentPlaying UseHostActionFragment="false" FragmentName="Dodge_Roll_Fast"/>
						<IfFragmentPlaying UseHostActionFragment="false" FragmentName="Attack_Start_Charge_UpperBody"/>
						<IfMannequinTag TagName="Potion"/>
