<Grid>
	<Aliases>
		<Alias Name="Idle"/>
		<Alias Name="Nav"/>
		<Alias Name="Fall"/>
		<Alias Name="Gather"/>
		<Alias Name="Sprint"/>
		<Alias Name="Build"/>
		<Alias Name="Attack"/>
		<Alias Name="Land"/>
		<Alias Name="Craft"/>
		<Alias Name="IdlePoseTrans"/>
		<Alias Name="SprintPoseTrans"/>
		<Alias Name="Dodge"/>
		<Alias Name="Reaction"/>
		<Alias Name="Sheathe"/>
		<Alias Name="TurnInPlace"/>
		<Alias Name="Death"/>
		<Alias Name="UpperBodyReaction"/>
		<Alias Name="AimPose"/>
		<Alias Name="Aim"/>
		<Alias Name="Emote"/>
		<Alias Name="Reload"/>
		<Alias Name="Cheat"/>
		<Alias Name="Inventory"/>
		<Alias Name="Use"/>
		<Alias Name="Respawn"/>
		<Alias Name="Offline"/>
		<Alias Name="Shoot"/>
		<Alias Name="Interact"/>
		<Alias Name="DeathsDoor"/>
		<Alias Name="DDrRcvr"/>
		<Alias Name="Traversal"/>
		<Alias Name="Jump"/>
		<Alias Name="Block"/>
		<Alias Name="Slide"/>
		<Alias Name="VFX"/>
		<Alias Name="FallingAttack"/>
		<Alias Name="Offhand"/>
		<Alias Name="IslandStart"/>
		<Alias Name="Cutscene"/>
		<Alias Name="FreeCam"/>
		<Alias Name="Stun"/>
		<Alias Name="Fishing"/>
		<Alias Name="UpperBodyAttack"/>
		<Alias Name="InventoryUpperBody"/>
		<Alias Name="Wade"/>
	</Aliases>
	<TransitionRules>
		<Rule From="*" To="*" Transition="Never"/>
		<Rule From="Idle" To="*" Transition="Always" ScopeBehavior="Moderate"/>
		<Rule From="Idle" To="Idle,SprintPoseTrans,FallingAttack" Transition="Never"/>
		<Rule From="Idle" To="TurnInPlace" ScopeBehavior="Moderate">
			<Condition>
				<Not>
					<IfMannequinMarker MarkerName="PreventTurnInPlace"/>
				</Not>
			</Condition>
		</Rule>
		<Rule From="Wade" To="*" Transition="Always" ScopeBehavior="Moderate"/>
		<Rule From="Nav" To="*" Transition="Always" ScopeBehavior="Moderate"/>
		<Rule From="Nav" To="Idle,SprintPoseTrans,TurnInPlace,FallingAttack" Transition="Never"/>
		<Rule From="Fall" To="*" Transition="Always" ScopeBehavior="Moderate"/>
		<Rule From="Fall" To="Idle,Nav,Fall,Gather,Sprint,Build,Craft,IdlePoseTrans,SprintPoseTrans,Aim,Shoot,Emote,Stun,UpperBodyAttack,Wade" Transition="Never"/>
		<Rule From="Fall" To="Dodge,Sheathe,TurnInPlace,Reload,Inventory,Use,Fishing,Block,Slide,Offhand" Transition="Never"/>
		<Rule From="Fall" To="Traversal,Jump" ScopeBehavior="Moderate">
			<Condition>
				<And>
					<IfTraversal MinHeight="0" MaxHeight="5" CheckHeight="false" CheckLength="false" TraversalType="mantle"/>
					<Not>
						<IsStunned/>
					</Not>
				</And>
			</Condition>
		</Rule>
		<Rule From="Fall" To="Attack" ScopeBehavior="Moderate">
			<Condition>
				<Or>
					<IfHomingOverrideTargetSet/>
					<And>
						<IfInput InputName="rune_ability" BackTicks="30"/>
						<IfAbilityActive AbilityName="Ability_Rune_HereticsFall" External="true"/>
					</And>
				</Or>
			</Condition>
		</Rule>
		<Rule From="Gather" To="*" Transition="Always" ScopeBehavior="Moderate"/>
		<Rule From="Gather" To="Idle,Build,Attack,Land,Craft,IdlePoseTrans,SprintPoseTrans,Sheathe,UpperBodyAttack,Wade" Transition="Never"/>
		<Rule From="Gather" To="TurnInPlace,Aim,Emote,Reload,Shoot,Interact,Block,FallingAttack,Offhand" Transition="Never"/>
		<Rule From="Gather" To="Nav,Sprint" ScopeBehavior="Aggressive">
			<Condition>
				<Not>
					<IfMannequinMarker MarkerName="PreventNav"/>
				</Not>
			</Condition>
		</Rule>
		<Rule From="Gather" To="Gather" ScopeBehavior="Moderate">
			<Condition>
				<IfMannequinMarker MarkerName="Gather_Loop"/>
			</Condition>
		</Rule>
		<Rule From="Gather" To="Inventory" ScopeBehavior="Moderate">
			<Condition>
				<IfInput InputName="Stop_Gathering"/>
			</Condition>
		</Rule>
		<Rule From="Gather" To="Use" ScopeBehavior="Aggressive"/>
		<Rule From="Sprint" To="*" Transition="Always" ScopeBehavior="Moderate"/>
		<Rule From="Sprint" To="Idle,IdlePoseTrans,TurnInPlace,FallingAttack" Transition="Never"/>
		<Rule From="Sprint" To="Fall,Dodge,Reload,Shoot,Emote" Transition="Always" ScopeBehavior="Aggressive"/>
		<Rule From="Sprint" To="Nav" ScopeBehavior="Moderate">
			<Condition>
				<Or>
					<Not>
						<IfCanSprint/>
					</Not>
					<IfMannequinTag TagName="Aim"/>
					<IfMannequinTag TagName="Shoot"/>
					<IfMannequinTag TagName="Throw"/>
					<IfMannequinTag TagName="Blocking"/>
					<And Comment="Loss of input">
						<Not>
							<IfAnalogInput AnalogInput="LeftAnalog" MinInputMagnitude="0.10" MaxInputMagnitude="1.00"/>
						</Not>
						<Not>
							<IfAnalogInput AnalogInput="None" DigitalForwardInput="ForwardNav" DigitalBackInput="moveback" DigitalRightInput="moveleft" DigitalLeftInput="moveright" MinInputMagnitude="0.10" MaxInputMagnitude="1.00"/>
						</Not>
					</And>
					<And Comment="Backward input during strafe sprint">
						<IfPlayerSetting PlayerSettingName="SprintStrafeMovement"/>
						<And>
							<Or Comment="LockedStrafeMovement or cameralock or combat mode">
								<IfPlayerSetting PlayerSettingName="LockedStrafeMovement"/>
								<IfCameraLockActive/>
								<And>
									<Not>
										<IfMannequinTag TagName="Tool"/>
									</Not>
									<Or>
										<IfMannequinTag TagName="1H_Melee"/>
										<IfMannequinTag TagName="2H_Melee"/>
										<IfMannequinTag TagName="1H_Ranged"/>
										<IfMannequinTag TagName="2H_Ranged"/>
										<IfMannequinTag TagName="Bow"/>
									</Or>
								</And>
							</Or>
							<Not Comment="Analog forward input">
								<IfAnalogInput AnalogInput="LeftAnalog" MinInputMagnitude="0.10" MaxInputMagnitude="1.00" RequireTargetAngleMatch="true" TargetAngle="0.00" TargetAngleTolerance="95.00"/>
							</Not>
							<Not Comment="Keyboard forward input">
								<IfAnalogInput AnalogInput="None" DigitalForwardInput="ForwardNav" DigitalBackInput="moveback" DigitalRightInput="moveleft" DigitalLeftInput="moveright" MinInputMagnitude="0.10" MaxInputMagnitude="1.00" RequireTargetAngleMatch="True" TargetAngle="0.00" TargetAngleTolerance="95.00"/>
							</Not>
						</And>
						<Not>
							<CameraCharacterAngle AngleThreshold="45.00"/>
						</Not>
					</And>
					<And Comment="Throw input">
						<IfMannequinTag TagName="Throwable"/>
						<Or>
							<IfActiveAbilityMoveName AbilityName="AimedThrow"/>
							<IfActiveAbilityMoveName AbilityName="Ability_Spear_Javelin"/>
						</Or>
					</And>
					<And Comment="Sprint toggle input check">
						<Not>
							<IfPlayerSetting PlayerSettingName="Sprint_InputMode_PressAndHold"/>
						</Not>
						<IfInput InputName="sprint_hold"/>
						<Not>
							<IfMannequinTag TagName="AutoRun"/>
						</Not>
					</And>
					<And Comment="Sprint Press-And-Hold input check">
						<IfPlayerSetting PlayerSettingName="Sprint_InputMode_PressAndHold"/>
						<Not>
							<IfInput InputName="sprint_hold"/>
						</Not>
						<Not>
							<IfMannequinTag TagName="AutoRun"/>
						</Not>
					</And>
					<IfMannequinMarker MarkerName="ForceWalk"/>
				</Or>
			</Condition>
		</Rule>
		<Rule From="Sprint" To="Block" ScopeBehavior="Aggressive">
			<Condition>
				<IfInput InputName="block"/>
			</Condition>
		</Rule>
		<Rule From="Sprint" To="Aim" ScopeBehavior="Aggressive">
			<Condition>
				<Or>
					<IfInput InputName="aim_draw"/>
					<IfInput InputName="aim_draw_press"/>
					<IfInput InputName="Ability3_Virtual" BackTicks="20"/>
					<IfInput InputName="Ability1_Virtual" BackTicks="20"/>
					<IfInput InputName="Ability2_Virtual" BackTicks="20"/>
					<IfInput InputName="attack_primary_virtual"/>
					<IfHasStatusEffect StatusEffect="Blunderbuss_MortarShotActiveTimer"/>
				</Or>
			</Condition>
		</Rule>
		<Rule From="Build" To="*" Transition="Always" ScopeBehavior="Moderate"/>
		<Rule From="Build" To="Gather,Land,Craft,IdlePoseTrans,SprintPoseTrans,Interact,FallingAttack,Traversal,Jump,Wade" Transition="Never"/>
		<Rule From="Build" To="Slide" Transition="Always" ScopeBehavior="Aggressive"/>
		<Rule From="Build" To="Attack,Sprint,Reload,Use,Fishing,Shoot,Block,Offhand,Dodge,Aim,Emote,Sheathe,TurnInPlace,UpperBodyAttack" ScopeBehavior="Aggressive">
			<Condition>
				<IfMannequinMarker MarkerName="CanTransition"/>
			</Condition>
		</Rule>
		<Rule From="Build" To="Build,Idle" ScopeBehavior="Moderate">
			<Condition>
				<IfMannequinMarker MarkerName="CanBuild"/>
			</Condition>
		</Rule>
		<Rule From="Attack" To="*" Transition="Always" ScopeBehavior="Moderate"/>
		<Rule From="Attack" To="Idle,Gather,Build,Land,Craft,SprintPoseTrans,TurnInPlace,Interact,FallingAttack,Wade" Transition="Never"/>
		<Rule From="Attack" To="Attack,UpperBodyAttack" Transition="Always" ScopeBehavior="Aggressive"/>
		<Rule From="Attack" To="Nav,Sprint" ScopeBehavior="Moderate">
			<Condition>
				<IfMannequinMarker MarkerName="CanTransition"/>
			</Condition>
		</Rule>
		<Rule From="Attack" To="IdlePoseTrans,Emote,Fishing,Fall,Inventory,InventoryUpperBody" ScopeBehavior="Aggressive">
			<Condition>
				<IfMannequinMarker MarkerName="CanTransition"/>
			</Condition>
		</Rule>
		<Rule From="Attack" To="Traversal,Jump" ScopeBehavior="Aggressive">
			<Condition>
				<Or>
					<IfMannequinMarker MarkerName="CanTransition"/>
					<IfMannequinMarker MarkerName="AllowMantle"/>
				</Or>
			</Condition>
		</Rule>
		<Rule From="Attack" To="Dodge" ScopeBehavior="Aggressive">
			<Condition>
				<Or>
					<IfMannequinMarker MarkerName="CanTransition"/>
					<IfMannequinMarker MarkerName="CanDodgeCancel"/>
				</Or>
			</Condition>
			<BufferCondition>
				<Or>
					<IfMannequinMarker MarkerName="CanDodgeBuffer"/>
				</Or>
			</BufferCondition>
		</Rule>
		<Rule From="Attack" To="Sheathe" ScopeBehavior="Aggressive">
			<Condition>
				<Or>
					<IfMannequinMarker MarkerName="CanTransition"/>
					<IfMannequinMarker MarkerName="CanCancel"/>
					<IfMannequinMarker MarkerName="CanThrow"/>
					<IfMannequinMarker MarkerName="CanUnsheathe"/>
				</Or>
			</Condition>
			<BufferCondition>
				<Or>
					<IfMannequinMarker MarkerName="CanBuffer"/>
					<IfMannequinMarker MarkerName="CanUnsheathe_Buffer"/>
				</Or>
			</BufferCondition>
		</Rule>
		<Rule From="Attack" To="Use" ScopeBehavior="Aggressive">
			<Condition>
				<Or>
					<IfMannequinMarker MarkerName="CanTransition"/>
					<IfMannequinMarker MarkerName="CanCancel"/>
					<IfMannequinMarker MarkerName="CanThrow"/>
					<IfMannequinMarker MarkerName="CanUnsheathe"/>
				</Or>
			</Condition>
			<BufferCondition>
				<Or>
					<IfMannequinMarker MarkerName="CanBuffer"/>
					<IfMannequinMarker MarkerName="CanUnsheathe_Buffer"/>
					<IfMannequinMarker MarkerName="CanDodgeBuffer"/>
					<IfMannequinMarker MarkerName="CanDodgeCancel"/>
				</Or>
			</BufferCondition>
		</Rule>
		<Rule From="Attack" To="Aim" ScopeBehavior="Aggressive">
			<Condition>
				<Or>
					<And>
						<IfMannequinMarker MarkerName="CanThrowCancel"/>
						<IfItemSheathed SlotToCheckAlias="ACTIVE_OFF_HAND_WEAPON"/>
					</And>
					<IfMannequinMarker MarkerName="CanTransition"/>
					<IfMannequinTag TagName="Fire"/>
					<IfMannequinTag TagName="Heal"/>
					<IfMannequinTag TagName="Ice"/>
					<IfMannequinTag TagName="VoidGauntlet"/>
					<IfMannequinMarker MarkerName="CanAimCancel"/>
				</Or>
			</Condition>
			<BufferCondition>
				<And>
					<Not>
						<IfMannequinTag TagName="Prone_Desired"/>
					</Not>
					<Not>
						<IfMannequinTag TagName="Crouch_Desired"/>
					</Not>
					<IfMannequinMarker MarkerName="CanThrowBuffer"/>
				</And>
			</BufferCondition>
		</Rule>
		<Rule From="Attack" To="Reload" ScopeBehavior="Moderate">
			<Condition>
				<IfMannequinMarker MarkerName="ForceReload"/>
			</Condition>
			<BufferCondition>
				<IfMannequinMarker MarkerName="CanBuffer"/>
			</BufferCondition>
		</Rule>
		<Rule From="Attack" To="Shoot" ScopeBehavior="Moderate">
			<Condition>
				<Or>
					<IfMannequinMarker MarkerName="CanTransition"/>
					<IfFragmentPlaying UseHostActionFragment="false" FragmentName="Attack_Start_Charge"/>
				</Or>
			</Condition>
			<BufferCondition>
				<IfMannequinMarker MarkerName="CanBuffer"/>
			</BufferCondition>
		</Rule>
		<Rule From="Attack" To="Block" ScopeBehavior="Aggressive">
			<Condition>
				<Or>
					<IfMannequinMarker MarkerName="CanTransition"/>
					<IfMannequinMarker MarkerName="CanBlockCancel"/>
					<And>
						<IfInput InputName="block"/>
						<Not>
							<IfMannequinMarker MarkerName="BlockDisabled"/>
						</Not>
						<IfMannequinMarker MarkerName="CanBlockCancelStartup"/>
					</And>
				</Or>
			</Condition>
		</Rule>
		<Rule From="Attack" To="Offhand" ScopeBehavior="Moderate">
			<Condition>
				<IfMannequinMarker MarkerName="CanDodgeCancel"/>
			</Condition>
			<BufferCondition>
				<IfMannequinMarker MarkerName="CanDodgeBuffer"/>
			</BufferCondition>
		</Rule>
		<Rule From="Land" To="*" Transition="Always" ScopeBehavior="Moderate"/>
		<Rule From="Land" To="Idle,Gather,Build,Land,Craft,IdlePoseTrans,SprintPoseTrans,Reaction,TurnInPlace,UpperbodyReaction,AimPose,Emote,Interact,FallingAttack" Transition="Never"/>
		<Rule From="Land" To="Nav,Fall,Sheathe,Reload,Use,Fishing,Inventory" ScopeBehavior="Moderate">
			<Condition>
				<IfMannequinMarker MarkerName="CanTransition"/>
			</Condition>
		</Rule>
		<Rule From="Land" To="Sprint" ScopeBehavior="Moderate">
			<Condition>
				<And>
					<IfMannequinMarker MarkerName="CanTransition"/>
					<Or>
						<Not>
							<IfMannequinMarker MarkerName="Land_To_Sprint"/>
						</Not>
						<IfMannequinTag TagName="OneTimeScriptDone"/>
					</Or>
				</And>
			</Condition>
		</Rule>
		<Rule From="Land" To="Traversal,Jump" ScopeBehavior="Moderate">
			<Condition>
				<And>
					<IfMannequinMarker MarkerName="Land_To_Jump"/>
				</And>
			</Condition>
		</Rule>
		<Rule From="Land" To="Attack,Offhand,Shoot" ScopeBehavior="Moderate">
			<Condition>
				<Or>
					<IfMannequinMarker MarkerName="CanAttackCancel"/>
					<IfMannequinMarker MarkerName="CanTransition"/>
				</Or>
			</Condition>
			<BufferCondition>
				<Or>
					<IfMannequinMarker MarkerName="CanAttackBuffer"/>
					<IfMannequinMarker MarkerName="CanBuffer"/>
				</Or>
			</BufferCondition>
		</Rule>
		<Rule From="Land" To="Dodge" ScopeBehavior="Moderate">
			<Condition>
				<Or>
					<IfMannequinMarker MarkerName="CanCancel"/>
					<IfMannequinMarker MarkerName="CanTransition"/>
				</Or>
			</Condition>
			<BufferCondition>
				<IfMannequinMarker MarkerName="CanBuffer"/>
			</BufferCondition>
		</Rule>
		<Rule From="Land" To="Aim,UpperBodyAttack,Block" ScopeBehavior="Aggressive">
			<Condition>
				<IfMannequinMarker MarkerName="CanTransition"/>
			</Condition>
			<BufferCondition>
				<IfMannequinMarker MarkerName="CanBuffer"/>
			</BufferCondition>
		</Rule>
		<Rule From="Craft" To="*" Transition="Always" ScopeBehavior="Moderate"/>
		<Rule From="Craft" To="Idle,Gather,Build,Attack,Land,Craft,SprintPoseTrans,Dodge,Sheathe,TurnInPlace,Aim,Emote,Reload,Inventory,InventoryUpperBody,Use,Fishing,Block,FallingAttack,Offhand,UpperBodyAttack,Wade" Transition="Never"/>
		<Rule From="Craft" To="Slide" Transition="Always" ScopeBehavior="Aggressive"/>
		<Rule From="IdlePoseTrans" To="*" Transition="Always" ScopeBehavior="Moderate"/>
		<Rule From="IdlePoseTrans" To="Idle,Land,SprintPoseTrans,TurnInPlace,Emote,Use,Fishing,FallingAttack" Transition="Never"/>
		<Rule From="IdlePoseTrans" To="Nav,Gather,Sprint,Build,Attack,UpperBodyAttack,Craft,IdlePoseTrans,Dodge,Traversal,Jump,Offhand,InventoryUpperBody" ScopeBehavior="Aggressive">
			<Condition>
				<IfMannequinMarker MarkerName="CanTransition"/>
			</Condition>
		</Rule>
		<Rule From="IdlePoseTrans" To="Sheathe,Reload" ScopeBehavior="Moderate">
			<Condition>
				<And>
					<Or>
						<Not>
							<IfMannequinTag TagName="Prone"/>
						</Not>
						<IfMannequinTag TagName="Rifle"/>
					</Or>
					<Or>
						<IfMannequinMarker MarkerName="CanTransition"/>
						<IfMannequinMarker MarkerName="CanTransition_Sheathe"/>
					</Or>
				</And>
			</Condition>
		</Rule>
		<Rule From="IdlePoseTrans" To="Aim" ScopeBehavior="Moderate">
			<Condition>
				<And>
					<Or>
						<And Comment="Not throwable weapon">
							<IfMannequinMarker MarkerName="CanTransition"/>
							<Not>
								<IfMannequinTag TagName="Throwable"/>
							</Not>
						</And>
						<And Comment="Throwable weapon">
							<IfMannequinMarker MarkerName="CanThrow"/>
							<IfMannequinTag TagName="Throwable"/>
						</And>
					</Or>
				</And>
			</Condition>
			<BufferCondition>
				<IfMannequinMarker MarkerName="CanTransition"/>
			</BufferCondition>
		</Rule>
		<Rule From="IdlePoseTrans" To="Block" ScopeBehavior="Aggressive">
			<Condition>
				<IfMannequinMarker MarkerName="CanTransition"/>
			</Condition>
		</Rule>
		<Rule From="SprintPoseTrans" To="*" Transition="Always" ScopeBehavior="Moderate"/>
		<Rule From="SprintPoseTrans" To="Idle,Land,IdlePoseTrans,SprintPoseTrans,TurnInPlace,Aim,Emote,Reload,Use,Fishing,Shoot,Interact,FallingAttack" Transition="Never"/>
		<Rule From="SprintPoseTrans" To="Nav,Gather,Build,Craft,Traversal,Jump,InventoryUpperBody" ScopeBehavior="Aggressive">
			<Condition>
				<IfMannequinMarker MarkerName="CanTransition"/>
			</Condition>
		</Rule>
		<Rule From="SprintPoseTrans" To="Sheathe" ScopeBehavior="Moderate">
			<Condition>
				<IfMannequinMarker MarkerName="CanUnsheathe"/>
			</Condition>
		</Rule>
		<Rule From="SprintPoseTrans" To="Sprint" ScopeBehavior="Moderate">
			<Condition>
				<IfMannequinMarker MarkerName="ResumeSprint"/>
			</Condition>
		</Rule>
		<Rule From="SprintPoseTrans" To="Attack,Dodge,Block,Offhand,UpperBodyAttack" ScopeBehavior="Moderate">
			<Condition>
				<IfMannequinMarker MarkerName="CanTransition"/>
			</Condition>
			<BufferCondition>
				<IfMannequinMarker MarkerName="CanBuffer"/>
			</BufferCondition>
		</Rule>
		<Rule From="Dodge" To="*" Transition="Always" ScopeBehavior="Moderate"/>
		<Rule From="Dodge" To="Idle,Gather,Build,Land,Craft,SprintPoseTrans,TurnInPlace,Emote,Interact,Wade" Transition="Never"/>
		<Rule From="Dodge" To="Nav,Sprint,IdlePoseTrans,Fishing,Block,Inventory,InventoryUpperBody,Sheathe" ScopeBehavior="Aggressive">
			<Condition>
				<IfMannequinMarker MarkerName="CanTransition"/>
			</Condition>
		</Rule>
		<Rule From="Dodge" To="Fall" ScopeBehavior="Aggressive">
			<Condition>
				<Or>
					<IfMannequinMarker MarkerName="CanAttackCancel"/>
					<IfFragmentPlaying FragmentName="Jump" UseHostActionFragment="false"/>
					<IfFragmentPlaying FragmentName="Jump_Diagonal" UseHostActionFragment="false"/>
				</Or>
			</Condition>
		</Rule>
		<Rule From="Dodge" To="Attack,Shoot,Offhand" ScopeBehavior="Aggressive">
			<Condition>
				<And>
					<IfMannequinMarker MarkerName="CanAttackCancel"/>
					<Not>
						<Or>
							<And>
								<Not>
									<IfItemInSlot SlotToCheckAlias="MAIN_HAND_OPTION_1" RequireSlotValid="true" RequireSlotActive="true"/>
								</Not>
								<IfInput InputName="Use_MainHandSlot_1" BackTicks="20"/>
							</And>
							<And>
								<Not>
									<IfItemInSlot SlotToCheckAlias="MAIN_HAND_OPTION_2" RequireSlotValid="true" RequireSlotActive="true"/>
								</Not>
								<IfInput InputName="Use_MainHandSlot_2" BackTicks="20"/>
							</And>
							<IfInput InputName="quickslot-weaponSwap" BackTicks="20"/>
							<IfActionStatus ActionName="Unsheathe_MainHand_1_UB" Status="Pending"/>
							<IfActionStatus ActionName="Unsheathe_MainHand_2_UB" Status="Pending"/>
							<IfActionStatus ActionName="Switch_MainHand_1_UB" Status="Pending"/>
							<IfActionStatus ActionName="Switch_MainHand_2_UB" Status="Pending"/>
						</Or>
					</Not>
				</And>
			</Condition>
			<BufferCondition>
				<IfMannequinMarker MarkerName="CanBufferAttack"/>
			</BufferCondition>
		</Rule>
		<Rule From="Dodge" To="Dodge" ScopeBehavior="Aggressive">
			<Condition>
				<Or>
					<IfMannequinMarker MarkerName="CanCancel"/>
					<IfMannequinMarker MarkerName="CanQuickStepCancel"/>
					<IfMannequinMarker MarkerName="CanDodgeAttackCancel"/>
				</Or>
			</Condition>
			<BufferCondition>
				<IfMannequinMarker MarkerName="CanBuffer"/>
			</BufferCondition>
		</Rule>
		<Rule From="Dodge" To="Use" ScopeBehavior="Aggressive">
			<Condition>
				<IfMannequinMarker MarkerName="CanAttackCancel"/>
			</Condition>
			<BufferCondition>
				<IfMannequinMarker MarkerName="CanBufferAttack"/>
			</BufferCondition>
		</Rule>
		<Rule From="Dodge" To="Aim,UpperBodyAttack" ScopeBehavior="Aggressive">
			<Condition>
				<And>
					<Not>
						<IfMannequinTag TagName="Shield_OH"/>
					</Not>
					<Not>
						<Or>
							<And>
								<Not>
									<IfItemInSlot SlotToCheckAlias="MAIN_HAND_OPTION_1" RequireSlotValid="true" RequireSlotActive="true"/>
								</Not>
								<IfInput InputName="Use_MainHandSlot_1" BackTicks="20"/>
							</And>
							<And>
								<Not>
									<IfItemInSlot SlotToCheckAlias="MAIN_HAND_OPTION_2" RequireSlotValid="true" RequireSlotActive="true"/>
								</Not>
								<IfInput InputName="Use_MainHandSlot_2" BackTicks="20"/>
							</And>
