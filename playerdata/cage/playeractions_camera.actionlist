			<IfMannequinTag TagName="Stand_Desired"/>
			<Or>
				<IfMannequinTag TagName="FromStand"/>
				<IfMannequinTag TagName="FromCrouch"/>
				<IfMannequinTag TagName="FromProne"/>
			</Or>
			<IfMannequinTag TagName="AimDownSights"/>
			<Not>
				<IfMannequinTag TagName="Zoom1Camera"/>
			</Not>
			<Not>
				<IfMannequinTag TagName="Zoom2Camera"/>
			</Not>
		</And>
		<And Name="Aim+Crouch+Trans">
			<IfMannequinTag TagName="Crouch_Desired"/>
			<Or>
				<IfMannequinTag TagName="FromStand"/>
				<IfMannequinTag TagName="FromCrouch"/>
				<IfMannequinTag TagName="FromProne"/>
			</Or>
			<IfMannequinTag TagName="AimDownSights"/>
			<Not>
				<IfMannequinTag TagName="Zoom1Camera"/>
			</Not>
			<Not>
				<IfMannequinTag TagName="Zoom2Camera"/>
			</Not>
		</And>
		<And Name="Aim+Prone+Trans">
			<IfMannequinTag TagName="Prone_Desired"/>
			<Or>
				<IfMannequinTag TagName="FromStand"/>
				<IfMannequinTag TagName="FromCrouch"/>
				<IfMannequinTag TagName="FromProne"/>
			</Or>
			<IfMannequinTag TagName="AimDownSights"/>
			<Not>
				<IfMannequinTag TagName="Zoom1Camera"/>
			</Not>
			<Not>
				<IfMannequinTag TagName="Zoom2Camera"/>
			</Not>
		</And>
		<And Name="InteriorCam">
			<IfMannequinTag TagName="Stand"/>
			<IsOverrideCamActive CameraName="CloseCamera"/>
			<Not>
				<IfMannequinTag TagName="AimDownSights"/>
			</Not>
		</And>
		<And Name="UI_Conversation">
			<IsOverrideCamActive CameraName="UI_Conversation"/>
