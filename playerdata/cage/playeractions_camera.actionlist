				<IfMannequinTag TagName="FromProne"/>
			</Not>
			<IfMannequinTag TagName="AimDownSights"/>
			<Not>
				<IfMannequinTag TagName="Zoom1Camera"/>
			</Not>
			<Not>
				<IfMannequinTag TagName="Zoom2Camera"/>
			</Not>
