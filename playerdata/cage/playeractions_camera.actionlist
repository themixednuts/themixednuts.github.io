				<Not>
					<Or>
						<IfMannequinTag TagName="Rifle"/>
						<IfMannequinTag TagName="Blunderbuss"/>
					</Or>
				</Not>
				<Not>
					<IfMannequinTag TagName="HeavyAim"/>
				</Not>
			</Or>
		</And>
		<And Name="Aim+Crouch">
			<Not>
				<IfMannequinTag TagName="Throwable"/>
			</Not>
			<Not>
				<IfMannequinTag TagName="Prone"/>
			</Not>
			<Or>
				<IfMannequinTag TagName="Crouch_Desired"/>
				<And>
					<IfMannequinTag TagName="Stand_Desired"/>
					<IfCanResizeCharacterController ResizeType="Crouch"/>
				</And>
			</Or>
			<Not>
				<IfMannequinTag TagName="FromStand"/>
			</Not>
			<Not>
				<IfMannequinTag TagName="FromCrouch"/>
			</Not>
			<Not>
				<IfMannequinTag TagName="FromProne"/>
			</Not>
			<IfMannequinTag TagName="AimDownSights"/>
			<Not>
				<IfMannequinTag TagName="Zoom1Camera"/>
			</Not>
			<Not>
				<IfMannequinTag TagName="Zoom2Camera"/>
			</Not>
			<Or>
				<Not>
					<Or>
						<IfMannequinTag TagName="Rifle"/>
						<IfMannequinTag TagName="Blunderbuss"/>
					</Or>
				</Not>
				<Not>
					<IfMannequinTag TagName="HeavyAim"/>
				</Not>
			</Or>
		</And>
		<And Name="Aim+Prone">
			<Or>
				<IfMannequinTag TagName="Prone_Desired"/>
				<And>
					<IfMannequinTag TagName="Stand_Desired"/>
					<IfCanResizeCharacterController ResizeType="Crawl"/>
				</And>
				<And>
					<IfMannequinTag TagName="Prone"/>
					<IfMannequinTag TagName="Crouch_Desired"/>
					<IfCanResizeCharacterController ResizeType="Crawl"/>
				</And>
			</Or>
			<Not>
				<IfMannequinTag TagName="FromStand"/>
			</Not>
			<Not>
				<IfMannequinTag TagName="FromCrouch"/>
			</Not>
			<Not>
				<IfMannequinTag TagName="FromProne"/>
			</Not>
			<IfMannequinTag TagName="AimDownSights"/>
			<Not>
				<IfMannequinTag TagName="Zoom1Camera"/>
			</Not>
			<Not>
				<IfMannequinTag TagName="Zoom2Camera"/>
			</Not>
			<Or>
				<Not>
					<Or>
						<IfMannequinTag TagName="Rifle"/>
						<IfMannequinTag TagName="Blunderbuss"/>
					</Or>
				</Not>
				<Not>
					<IfMannequinTag TagName="HeavyAim"/>
				</Not>
			</Or>
		</And>
