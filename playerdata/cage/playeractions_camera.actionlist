				<IfThenElse>
					<IfCondition>
						<And>
							<Or>
								<IfMannequinTag TagName="Aim"/>
								<IfMannequinTag TagName="ChargedAim"/>
								<IfMannequinTag TagName="HeavyAim"/>
								<IfMannequinTag TagName="1H_Ranged"/>
								<IfMannequinTag TagName="2H_Ranged"/>
								<IfMannequinTag TagName="Bow"/>
								<IfMannequinTag TagName="MagicGauntlet"/>
								<And>
									<Not>
										<IfCameraLockActive/>
									</Not>
									<IfMannequinTag TagName="AttackAction"/>
									<Not>
										<IfMannequinMarker MarkerName="CanTransition"/>
									</Not>
								</And>
								<IfIsPlacingBuilding/>
							</Or>
							<Not>
								<IfMannequinTag TagName="Heal"/>
							</Not>
						</And>
					</IfCondition>
					<ThenActivity>
						<DisableCameraLock>
							<Default OnEnter="Disable" OnExit="Enable"/>
						</DisableCameraLock>
					</ThenActivity>
				</IfThenElse>
				<IfThenElse Comment="Set Dynamic Camera Tag">
					<IfCondition>
						<Or>
							<And>
								<Not>
									<IfMannequinTag TagName="Zoom1Camera"/>
								</Not>
								<Not>
									<IfMannequinTag TagName="Zoom2Camera"/>
								</Not>
							</And>
							<And>
								<IfInput InputName="aim_draw_release"/>
								<Not>
									<And>
										<Or>
											<IfMannequinTag TagName="Rifle"/>
											<IfMannequinTag TagName="Blunderbuss"/>
										</Or>
										<IfMannequinTag TagName="HeavyAim"/>
									</And>
								</Not>
							</And>
							<IfInput InputName="toggleInventoryWindow"/>
							<IfInput InputName="toggleChatComponent"/>
							<IfInput InputName="toggleGuildComponent"/>
							<IfInput InputName="toggleJournalComponent"/>
							<IfInput InputName="toggleMenuComponent"/>
							<IfInput InputName="toggleMapComponent"/>
							<IfInput InputName="toggleSkillsComponent"/>
							<IfMannequinTag TagName="Bow"/>
						</Or>
					</IfCondition>
					<ThenActivity>
						<Group>
							<SetMannequinTag>
								<Default TagName="Dynamic" OnEnter="Set" OnExit="NoEffect"/>
							</SetMannequinTag>
							<ExecuteAudioTrigger ResetWhenReinstalled="true" RunOnRemoteClients="false" IgnoreDataChanges="false">
								<Default AudioTriggerName="Set_SniperFocus_00" ForceStopFxOnDataChange="false" ExecuteOnLocalPlayer="true" ExecuteOnRemotePlayer="true" AttachmentJointRtpcName="" AttachmentJoint="" AttachToEntity="true"/>
							</ExecuteAudioTrigger>
						</Group>
					</ThenActivity>
				</IfThenElse>
				<IfThenElse Comment="Set Zoom2Camera Camera Tag From Zoom1Camera or Dynamic">
					<IfCondition>
						<And>
							<Or>
								<IfMannequinTag TagName="Blunderbuss"/>
								<IfMannequinTag TagName="Rifle"/>
							</Or>
							<IfMannequinTag TagName="AimDownSights"/>
							<IfAbilityActive AbilityName="Ult_Passive_Musket_ZoomHeadshot"/>
							<Or>
								<IfMannequinTag TagName="Zoom1Camera"/>
								<Not>
									<IfAbilityActive AbilityName="Ult_Passive_Musket_ZoomHeadshot"/>
								</Not>
							</Or>
							<IfInput InputName="cam_zoom_in"/>
							<Not>
								<IfMannequinMarker MarkerName="NoCameraZoom"/>
							</Not>
						</And>
					</IfCondition>
					<ThenActivity>
						<Group>
							<SetMannequinTag>
								<Default TagName="Zoom2Camera" OnEnter="Set" OnExit="NoEffect"/>
							</SetMannequinTag>
							<ExecuteAudioTrigger ResetWhenReinstalled="true" RunOnRemoteClients="false" IgnoreDataChanges="false">
								<Default AudioTriggerName="Set_SniperFocus_100" ForceStopFxOnDataChange="false" ExecuteOnLocalPlayer="true" ExecuteOnRemotePlayer="true" AttachmentJointRtpcName="" AttachmentJoint="" AttachToEntity="true"/>
							</ExecuteAudioTrigger>
						</Group>
					</ThenActivity>
				</IfThenElse>
				<IfThenElse Comment="Set Zoom1Camera Camera Tag From Dynamic">
					<IfCondition>
						<And>
							<Or>
								<IfMannequinTag TagName="Rifle"/>
								<IfMannequinTag TagName="Blunderbuss"/>
							</Or>
							<IfMannequinTag TagName="AimDownSights"/>
							<IfAbilityActive AbilityName="Ult_Passive_Musket_ZoomHeadshot"/>
							<IfMannequinTag TagName="Dynamic"/>
							<IfInput InputName="cam_zoom_in"/>
							<Not>
								<IfMannequinMarker MarkerName="NoCameraZoom"/>
							</Not>
						</And>
					</IfCondition>
					<ThenActivity>
						<Group>
							<SetMannequinTag>
								<Default TagName="Zoom1Camera" OnEnter="Set" OnExit="NoEffect"/>
							</SetMannequinTag>
							<ExecuteAudioTrigger ResetWhenReinstalled="true" RunOnRemoteClients="false" IgnoreDataChanges="false">
								<Default AudioTriggerName="Set_SniperFocus_50" ForceStopFxOnDataChange="false" ExecuteOnLocalPlayer="true" ExecuteOnRemotePlayer="true" AttachmentJointRtpcName="" AttachmentJoint="" AttachToEntity="true"/>
							</ExecuteAudioTrigger>
						</Group>
					</ThenActivity>
				</IfThenElse>
				<IfThenElse Comment="Set Dynamic Camera Tag From Zoom1Camera or Zoom2Camera">
					<IfCondition>
						<And>
							<Or>
								<IfMannequinTag TagName="Rifle"/>
								<IfMannequinTag TagName="Blunderbuss"/>
							</Or>
							<IfMannequinTag TagName="AimDownSights"/>
							<Or>
								<And>
									<IfAbilityActive AbilityName="Ult_Passive_Musket_ZoomHeadshot"/>
									<IfMannequinTag TagName="Zoom1Camera"/>
								</And>
								<And>
									<Not>
										<IfAbilityActive AbilityName="Ult_Passive_Musket_ZoomHeadshot"/>
									</Not>
									<IfMannequinTag TagName="Zoom2Camera"/>
								</And>
							</Or>
							<IfInput InputName="cam_zoom_out"/>
							<Not>
								<IfMannequinMarker MarkerName="NoCameraZoom"/>
							</Not>
						</And>
					</IfCondition>
					<ThenActivity>
						<Group>
							<SetMannequinTag>
								<Default TagName="Dynamic" OnEnter="Set" OnExit="NoEffect"/>
							</SetMannequinTag>
							<ExecuteAudioTrigger ResetWhenReinstalled="true" RunOnRemoteClients="false" IgnoreDataChanges="false">
								<Default AudioTriggerName="Set_SniperFocus_00" ForceStopFxOnDataChange="false" ExecuteOnLocalPlayer="true" ExecuteOnRemotePlayer="true" AttachmentJointRtpcName="" AttachmentJoint="" AttachToEntity="true"/>
							</ExecuteAudioTrigger>
						</Group>
					</ThenActivity>
				</IfThenElse>
				<IfThenElse Comment="Set Zoom1Camera Camera Tag From Zoom2Camera">
					<IfCondition>
						<And>
							<Or>
								<IfMannequinTag TagName="Rifle"/>
								<IfMannequinTag TagName="Blunderbuss"/>
							</Or>
							<IfMannequinTag TagName="AimDownSights"/>
							<IfAbilityActive AbilityName="Ult_Passive_Musket_ZoomHeadshot"/>
							<IfMannequinTag TagName="Zoom2Camera"/>
							<IfInput InputName="cam_zoom_out"/>
							<Not>
								<IfMannequinMarker MarkerName="NoCameraZoom"/>
							</Not>
						</And>
					</IfCondition>
					<ThenActivity>
						<Group>
							<SetMannequinTag>
								<Default TagName="Zoom1Camera" OnEnter="Set" OnExit="NoEffect"/>
							</SetMannequinTag>
							<ExecuteAudioTrigger ResetWhenReinstalled="true" RunOnRemoteClients="false" IgnoreDataChanges="false">
								<Default AudioTriggerName="Set_SniperFocus_50" ForceStopFxOnDataChange="false" ExecuteOnLocalPlayer="true" ExecuteOnRemotePlayer="true" AttachmentJointRtpcName="" AttachmentJoint="" AttachToEntity="true"/>
							</ExecuteAudioTrigger>
						</Group>
					</ThenActivity>
				</IfThenElse>
			</Activities>
		</Action>
