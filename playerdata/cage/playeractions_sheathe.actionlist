<ActionList>
	<Actions>
		<Action Name="Unsheathe_MainHand_1_UB" FragmentName="Unsheathe_UpperBody">
			<ActivationCondition>
				<And>
					<Or Comment="Input Checks">
						<And Comment="Virtual Input Check">
							<Or>
								<IfInput InputName="Use_MainHandSlot_1" BackTicks="20" BackTickCancelInput="Use_MainHandSlot_2"/>
								<And Comment="Unsheathe if nothing equipped in alternate main hand or alternate main hand is active">
									<IfInput InputName="quickslot-weaponSwap" BackTicks="20"/>
									<Or>
										<Not>
											<IfItemInSlot SlotToCheckAlias="MAIN_HAND_OPTION_2" RequireSlotValid="true" RequireSlotActive="false"/>
										</Not>
										<And>
											<IfItemInSlot SlotToCheckAlias="MAIN_HAND_OPTION_2" RequireSlotValid="false" RequireSlotActive="true"/>
											<IfItemInSlot SlotToCheckAlias="MAIN_HAND_OPTION_2" RequireSlotValid="true" RequireSlotActive="false"/>
										</And>
									</Or>
								</And>
							</Or>
							<Or>
								<Not>
									<IfItemInSlot SlotToCheckAlias="MAIN_HAND_OPTION_1" RequireSlotValid="false" RequireSlotActive="true"/>
								</Not>
								<IfItemSheathed SlotToCheckAlias="MAIN_HAND_OPTION_1"/>
							</Or>
							<IfItemInSlot SlotToCheckAlias="MAIN_HAND_OPTION_1" RequireSlotValid="true" RequireSlotActive="false"/>
						</And>
						<And>
							<Or>
								<IfInput InputName="sheathe"/>
								<And>
									<IfMannequinTag TagName="AutoUnsheathe"/>
									<Not>
										<Or>
											<IfInput InputName="Use_MainHandSlot_2" BackTicks="20" BackTickCancelInput="Use_MainHandSlot_1"/>
											<IfInput InputName="quickslot-weaponSwap" BackTicks="20" BackTickCancelInput="Use_MainHandSlot_1"/>
										</Or>
									</Not>
									<Not>
										<IfMannequinMarker MarkerName="PreventUnsheathe"/>
									</Not>
									<Or>
										<Not>
											<IfMannequinTag TagName="Prone"/>
										</Not>
										<IfMannequinTag TagName="Rifle"/>
									</Or>
									<Not>
										<IfInput InputName="Apply_Resources_To_Structure"/>
									</Not>
									<Not>
										<IfMannequinTag TagName="SmokeAim"/>
									</Not>
									<Not>
										<IsRequestingInteraction>
											<Interactions>
												<Interaction InteractionId="ModifyStatusEffect"/>
											</Interactions>
										</IsRequestingInteraction>
									</Not>
								</And>
							</Or>
							<IfItemInSlot SlotToCheckAlias="MAIN_HAND_OPTION_1" RequireSlotValid="false" RequireSlotActive="true"/>
							<IfItemSheathed SlotToCheckAlias="MAIN_HAND_OPTION_1"/>
							<IfItemInSlot SlotToCheckAlias="MAIN_HAND_OPTION_1" RequireSlotValid="true" RequireSlotActive="false"/>
							<IfItemInSlotEquippable SlotToCheckAlias="MAIN_HAND_OPTION_1"/>
						</And>
					</Or>
					<Or>
						<Not Comment="Attempting to transition to prone">
							<And>
								<IfMannequinTag TagName="Prone_Desired"/>
								<Or>
									<IfMannequinTag TagName="Stand"/>
									<IfMannequinTag TagName="Crouch"/>
								</Or>
							</And>
						</Not>
						<And>
							<IfMannequinTag TagName="Prone"/>
							<IfMannequinTag TagName="Prone_Desired"/>
							<IfMannequinTag TagName="Rifle"/>
						</And>
					</Or>
					<Or Comment="Check if collision above">
						<And>
							<IfMannequinTag TagName="Prone"/>
							<IfCanResizeCharacterController ResizeType="Crouch"/>
						</And>
						<IfMannequinTag TagName="Crouch"/>
						<IfMannequinTag TagName="Stand"/>
					</Or>
					<Not>
						<IfIsLoadoutOpen/>
					</Not>
					<Not>
						<IfFragmentPlaying UseHostActionFragment="true"/>
					</Not>
					<Not>
						<IfInput InputName="Placing_Building"/>
					</Not>
					<Not Comment="Shooters Stance Check">
						<And>
							<Or>
								<IfMannequinTag TagName="Rifle"/>
								<IfMannequinTag TagName="Blunderbuss"/>
							</Or>
							<IfMannequinTag TagName="HeavyAim"/>
						</And>
					</Not>
					<IfItemInSlotEquippable SlotToCheckAlias="MAIN_HAND_OPTION_1"/>
					<Not>
						<IfWaterDepth Comparison="Equal" WaterDepth="Deep"/>
					</Not>
					<Not>
						<IfMannequinTag TagName="ChargeTimer"/>
					</Not>
					<Not>
						<IfExternalCondition ConditionName="Throwable_Equipped"/>
					</Not>
				</And>
			</ActivationCondition>
			<DeactivationCondition>
				<Or>
					<IfFragmentDone UseHostActionFragment="true" ExitOffsetSeconds="0"/>
					<And Comment="Prone exit cases">
						<IfMannequinTag TagName="Prone"/>
						<Or>
							<IfFragmentPlaying UseHostActionFragment="false" FragmentName="Idle_PostureTransition"/>
							<And Comment="Movement Exit">
								<IfMannequinMarker MarkerName="CanTransition_UpperBody"/>
								<Not>
									<IfFragmentPlaying UseHostActionFragment="false" FragmentName="Idle"/>
								</Not>
							</And>
						</Or>
						<Not>
							<Or>
								<IfMannequinTag TagName="Rifle"/>
							</Or>
						</Not>
					</And>
					<And Comment="Clear prone when attacking">
						<IfMannequinTag TagName="Prone_Desired"/>
						<Not>
							<Or>
								<IfMannequinTag TagName="Rifle"/>
							</Or>
						</Not>
					</And>
				</Or>
			</DeactivationCondition>
			<Activities>
				<ClearInput>
					<Default InputName="Use_MainHandSlot_1" OnEnter="Clear" OnExit="NoEffect"/>
				</ClearInput>
				<ClearInput>
					<Default InputName="Use_MainHandSlot_2" OnEnter="Clear" OnExit="NoEffect"/>
				</ClearInput>
				<ClearInput>
					<Default InputName="quickslot-weaponSwap" OnEnter="Clear" OnExit="NoEffect"/>
				</ClearInput>
				<ClearInput>
					<Default InputName="sheathe" OnEnter="Clear" OnExit="NoEffect"/>
				</ClearInput>
				<IfThenElse ResetWhenReinstalled="false" Comment="Unsheathe if allowed">
					<IfCondition>
						<IfExternalCondition ConditionName="Allow_Unsheathe"/>
					</IfCondition>
					<ThenActivity>
						<Group>
							<SetMannequinTagFromSource ResetWhenReinstalled="false" Comment="Unset Current Main Hand Weapon Tags on Enter">
								<Default TagSource="PaperdollSlot" SourceSlot="ACTIVE_MAIN_HAND_WEAPON" OnEnter="Unset" OnExit="NoEffect"/>
							</SetMannequinTagFromSource>
							<SheatheWeapon ResetWhenReinstalled="false">
								<Default SheatheOption="Sheathe" SlotToSheathe="ACTIVE_MAIN_HAND_WEAPON" OnEnter="Set" OnExit="NoEffect"/>
							</SheatheWeapon>
							<SetMannequinTagFromSource ResetWhenReinstalled="false" Comment="Set Main Hand 1 Tags">
								<Default TagSource="PaperdollSlot" SourceSlot="MAIN_HAND_OPTION_1" OnEnter="Set" OnExit="NoEffect"/>
							</SetMannequinTagFromSource>
							<IfThenElse ResetWhenReinstalled="false" Comment="Sheathe Offhand if New Main Hand is 2H Only or Magic Gauntlet">
								<IfCondition>
									<And>
										<Or>
											<IfExternalCondition ConditionName="MainHand_1_2H_Only"/>
											<IfExternalCondition ConditionName="Keep_Offhand_Sheathed"/>
										</Or>
										<IfItemInSlot SlotToCheckAlias="ACTIVE_OFF_HAND_WEAPON" RequireSlotValid="true" RequireSlotActive="false"/>
									</And>
								</IfCondition>
								<ThenActivity>
									<Group>
										<SetMannequinTagFromSource ResetWhenReinstalled="false">
											<Default TagSource="PaperdollSlot" SourceSlot="ACTIVE_OFF_HAND_WEAPON" OnEnter="Unset" OnExit="NoEffect"/>
										</SetMannequinTagFromSource>
										<SheatheWeapon ResetWhenReinstalled="false">
											<Default SheatheOption="Sheathe" SlotToSheathe="ACTIVE_OFF_HAND_WEAPON" OnEnter="Set" OnExit="NoEffect"/>
										</SheatheWeapon>
										<SetMannequinTag ResetWhenReinstalled="false">
											<Default TagName="Unarmed_OH" OnEnter="Set" OnExit="NoEffect"/>
										</SetMannequinTag>
									</Group>
								</ThenActivity>
							</IfThenElse>
							<SheatheWeapon ResetWhenReinstalled="false" Comment="Unsheathe current mainhand weapon">
								<Default SheatheOption="UnSheathe" SlotToSheathe="MAIN_HAND_OPTION_1" UpdateAttachment="false" OnEnter="Set" OnExit="NoEffect"/>
							</SheatheWeapon>
							<SetActiveWeapon ResetWhenReinstalled="false">
								<Default WeaponOption="main-hand-option-1" OnEnter="Set" OnExit="NoEffect"/>
							</SetActiveWeapon>
							<SetMannequinTagFromSource ResetWhenReinstalled="false" Comment="Set Off Hand Tags if valid">
								<Default OnEnter="NoEffect" OnExit="NoEffect"/>
								<Conditional Name="Keep_Offhand_Sheathed" TagSource="PaperdollSlot" SourceSlot="ACTIVE_OFF_HAND_WEAPON" OnEnter="NoEffect" OnExit="NoEffect"/>
								<Conditional Name="MainHand_1_2H_Only" TagSource="PaperdollSlot" SourceSlot="ACTIVE_OFF_HAND_WEAPON" OnEnter="NoEffect" OnExit="NoEffect"/>
								<Conditional Name="Has_Valid_Current_Offhand" TagSource="PaperdollSlot" SourceSlot="ACTIVE_OFF_HAND_WEAPON" OnEnter="Set" OnExit="NoEffect"/>
							</SetMannequinTagFromSource>
							<SetMannequinTag ResetWhenReinstalled="false" Comment="Set From_Unarmed_OH to enable unsheathe anim">
								<Default TagName="-" OnEnter="NoEffect" OnExit="NoEffect"/>
								<Conditional Name="Keep_Offhand_Sheathed" TagName="-" OnEnter="NoEffect" OnExit="NoEffect"/>
								<Conditional Name="MainHand_1_2H_Only" TagName="-" OnEnter="NoEffect" OnExit="NoEffect"/>
								<Conditional Name="Keep_Offhand_Unsheathed" TagName="-" OnEnter="NoEffect" OnExit="NoEffect"/>
								<Conditional Name="Has_Valid_Current_Offhand" TagName="From_Unarmed_OH" OnEnter="Set" OnExit="NoEffect"/>
							</SetMannequinTag>
							<SetMannequinTag ResetWhenReinstalled="false" Comment="Unset AutoUnsheathe">
								<Default TagName="AutoUnsheathe" OnEnter="Unset" OnExit="NoEffect"/>
							</SetMannequinTag>
							<IfThenElse ResetWhenReinstalled="false" Comment="Update current main hand attachment from Mannequin Marker">
								<IfCondition>
									<IfMannequinMarker MarkerName="Unsheathe_Weapon"/>
								</IfCondition>
								<ThenActivity>
									<Group>
										<AttachItemMesh>
											<Default Slot="ACTIVE_MAIN_HAND_WEAPON" Attach="true"/>
										</AttachItemMesh>
									</Group>
								</ThenActivity>
							</IfThenElse>
							<IfThenElse ResetWhenReinstalled="false" Comment="Unsheathe current offhand weapon if valid">
								<IfCondition>
									<And>
										<Not>
											<IfExternalCondition ConditionName="MainHand_1_2H_Only"/>
										</Not>
										<Not>
											<IfExternalCondition ConditionName="Keep_Offhand_Sheathed"/>
										</Not>
										<IfExternalCondition ConditionName="Has_Valid_Current_Offhand"/>
									</And>
								</IfCondition>
								<ThenActivity>
									<SheatheWeapon ResetWhenReinstalled="false">
										<Default SheatheOption="UnSheathe" SlotToSheathe="ACTIVE_OFF_HAND_WEAPON" UpdateAttachment="false" OnEnter="Set" OnExit="NoEffect"/>
									</SheatheWeapon>
								</ThenActivity>
							</IfThenElse>
							<IfThenElse ResetWhenReinstalled="false" Comment="Update current offhand weapon attachment from Mannequin Marker">
								<IfCondition>
									<And>
										<Not>
											<IfExternalCondition ConditionName="MainHand_1_2H_Only"/>
										</Not>
										<Not>
											<IfExternalCondition ConditionName="Keep_Offhand_Sheathed"/>
										</Not>
										<IfMannequinMarker MarkerName="Unsheathe_Offhand"/>
										<IfExternalCondition ConditionName="Has_Valid_Current_Offhand"/>
									</And>
								</IfCondition>
								<ThenActivity>
									<AttachItemMesh>
										<Default Slot="ACTIVE_OFF_HAND_WEAPON" Attach="true"/>
									</AttachItemMesh>
								</ThenActivity>
							</IfThenElse>
						</Group>
					</ThenActivity>
				</IfThenElse>
				<ClearMannequinTagGroup ResetWhenReinstalled="false">
					<Default TagGroupName="OffhandStanceTransition" OnEnter="NoEffect" OnExit="Clear"/>
				</ClearMannequinTagGroup>
				<SetMannequinTagFromSource ResetWhenReinstalled="false" Comment="Unset Current Main Hand Weapon Tags on exit when transitioning to interaction">
					<Default TagSource="PaperdollSlot" SourceSlot="MAIN_HAND_OPTION_1" OnEnter="NoEffect" OnExit="NoEffect"/>
					<Conditional Name="Clear_Weapon_Tags_OnExit" TagSource="PaperdollSlot" SourceSlot="MAIN_HAND_OPTION_1" OnEnter="NoEffect" OnExit="Unset"/>
				</SetMannequinTagFromSource>
				<SetMannequinTagFromSource ResetWhenReinstalled="false" Comment="Unset Current Off Hand Weapon Tags on exit when transitioning to interaction">
					<Default TagSource="PaperdollSlot" SourceSlot="MAIN_HAND_OPTION_1" OnEnter="NoEffect" OnExit="NoEffect"/>
					<Conditional Name="Clear_Weapon_Tags_OnExit_Offhand" TagSource="PaperdollSlot" SourceSlot="ACTIVE_OFF_HAND_WEAPON" OnEnter="NoEffect" OnExit="Unset"/>
				</SetMannequinTagFromSource>
				<SheatheWeapon ResetWhenReinstalled="false" Comment="Unsheathe Main hand On Exit for safety">
					<Default SheatheOption="UnSheathe" SlotToSheathe="MAIN_HAND_OPTION_1" OnEnter="NoEffect" OnExit="NoEffect"/>
					<Conditional Name="Unsheathe_MH1_OnExit" SheatheOption="UnSheathe" SlotToSheathe="MAIN_HAND_OPTION_1" OnEnter="NoEffect" OnExit="Set"/>
				</SheatheWeapon>
				<SheatheWeapon ResetWhenReinstalled="false" Comment="Unsheathe Off hand On Exit for safety">
					<Default OnEnter="NoEffect" OnExit="NoEffect"/>
					<Conditional Name="Prone" OnEnter="NoEffect" OnExit="NoEffect"/>
					<Conditional Name="Keep_Offhand_Sheathed" OnEnter="NoEffect" OnExit="NoEffect"/>
					<Conditional Name="MainHand_1_2H_Only" OnEnter="NoEffect" OnExit="NoEffect"/>
					<Conditional Name="Unsheathe_MH1_Offhand_OnExit" SheatheOption="UnSheathe" SlotToSheathe="ACTIVE_OFF_HAND_WEAPON" OnEnter="NoEffect" OnExit="Set"/>
				</SheatheWeapon>
				<ItemAudioTrigger ResetWhenReinstalled="false">
					<Default EventName="Unsheathe" ItemSlot="MAIN_HAND_OPTION_1" StopTriggerName="do_nothing"/>
				</ItemAudioTrigger>
				<IfThenElse ResetWhenReinstalled="false" Comment="Unsheath offhand sound">
					<IfCondition>
						<Not>
							<Or>
								<IfMannequinTagInItemSlot TagName="Rifle" SlotToCheckAlias="MAIN_HAND_OPTION_1"/>
								<IfMannequinTagInItemSlot TagName="Blunderbuss" SlotToCheckAlias="MAIN_HAND_OPTION_1"/>
								<IfMannequinTagInItemSlot TagName="Bow" SlotToCheckAlias="MAIN_HAND_OPTION_1"/>
								<IfMannequinTagInItemSlot TagName="2H_Melee" SlotToCheckAlias="MAIN_HAND_OPTION_1"/>
								<IfMannequinTagInItemSlot TagName="MagicStaff" SlotToCheckAlias="MAIN_HAND_OPTION_1"/>
							</Or>
						</Not>
					</IfCondition>
					<ThenActivity>
						<ItemAudioTrigger ResetWhenReinstalled="false">
							<Default EventName="Unsheathe" ItemSlot="ACTIVE_OFF_HAND_WEAPON"/>
						</ItemAudioTrigger>
					</ThenActivity>
				</IfThenElse>
				<IfThenElse ResetWhenReinstalled="false" Comment="Show prerequisite warning">
					<IfCondition>
						<And>
							<Not>
								<IfMannequinTag TagName="AutoUnsheathe"/>
							</Not>
							<Or>
								<And Comment="2H Weapon">
									<IfExternalCondition ConditionName="MainHand_1_2H_Only"/>
									<Not>
										<IfHasWeaponPrerequisites WeaponSlot="ACTIVE_MAIN_HAND_WEAPON"/>
									</Not>
								</And>
								<And Comment="1H Weapon with or without Shield">
									<Not>
										<IfExternalCondition ConditionName="MainHand_1_2H_Only"/>
									</Not>
									<Or>
										<And>
											<IfItemInSlot SlotToCheckAlias="ACTIVE_OFF_HAND_WEAPON" RequireSlotValid="true" RequireSlotActive="false"/>
											<Not>
												<IfHasWeaponPrerequisites WeaponSlot="ACTIVE_OFF_HAND_WEAPON"/>
											</Not>
										</And>
										<Not>
											<IfHasWeaponPrerequisites WeaponSlot="ACTIVE_MAIN_HAND_WEAPON"/>
										</Not>
									</Or>
								</And>
							</Or>
						</And>
					</IfCondition>
					<ThenActivity>
						<ShowUINotification>
							<Default type="Minor" text="@inv_does_not_meet_weapon_requirements"/>
						</ShowUINotification>
					</ThenActivity>
				</IfThenElse>
				<SetMannequinTag ResetWhenReinstalled="false" Comment="Set Unarmed if unsheathing is canceled and going to an interaction">
					<Default TagName="Unarmed" OnEnter="NoEffect" OnExit="NoEffect"/>
					<Conditional Name="Clear_Weapon_Tags_OnExit" TagName="Unarmed" OnEnter="NoEffect" OnExit="Set"/>
				</SetMannequinTag>
				<SetMannequinTag ResetWhenReinstalled="false" Comment="Set Unarmed_OH if unsheathing is canceled and going to an interaction">
					<Default TagName="Unarmed_OH" OnEnter="NoEffect" OnExit="NoEffect"/>
					<Conditional Name="Clear_Weapon_Tags_OnExit_Offhand" TagName="Unarmed_OH" OnEnter="NoEffect" OnExit="Set"/>
				</SetMannequinTag>
				<SheatheWeapon ResetWhenReinstalled="false" Comment="Sheathe Main hand if unsheathing is canceled and going to an interaction">
					<Default SheatheOption="Sheathe" SlotToSheathe="MAIN_HAND_OPTION_1" OnEnter="NoEffect" OnExit="NoEffect"/>
					<Conditional Name="Sheathe_MH1_OnExit" SheatheOption="Sheathe" SlotToSheathe="MAIN_HAND_OPTION_1" OnEnter="NoEffect" OnExit="Set"/>
				</SheatheWeapon>
				<SheatheWeapon ResetWhenReinstalled="false" Comment="Sheathe Offhand if unsheathing is canceled and going to an interaction">
					<Default OnEnter="NoEffect" OnExit="NoEffect"/>
					<Conditional Name="Sheathe_MH1_Offhand_OnExit" SheatheOption="Sheathe" SlotToSheathe="ACTIVE_OFF_HAND_WEAPON" OnEnter="NoEffect" OnExit="Set"/>
				</SheatheWeapon>
				<SetMannequinTag ResetWhenReinstalled="false" Comment="Unset AutoUnsheathe if unsheathing is canceled">
					<Default TagName="AutoUnsheathe" OnEnter="NoEffect" OnExit="Unset"/>
				</SetMannequinTag>
				<IfThenElse ResetWhenReinstalled="false" Comment="Show prerequisite warning">
					<IfCondition>
						<And>
							<IfMannequinTag TagName="Prone"/>
							<Not>
								<Or>
									<IfMannequinTag TagName="Rifle"/>
								</Or>
							</Not>
						</And>
					</IfCondition>
					<ThenActivity>
						<Group>
							<SetMannequinTag ResetWhenReinstalled="false" Comment="Unset AutoUnsheathe if unsheathing is canceled">
								<Default TagName="Crouch" OnEnter="Set" OnExit="NoEffect"/>
							</SetMannequinTag>
							<SetMannequinTag ResetWhenReinstalled="false" Comment="Unset AutoUnsheathe if unsheathing is canceled">
								<Default TagName="Crouch_Desired" OnEnter="Set" OnExit="NoEffect"/>
							</SetMannequinTag>
						</Group>
					</ThenActivity>
				</IfThenElse>
			</Activities>
		</Action>
		<Action Name="Unsheathe_MainHand_2_UB" FragmentName="Unsheathe_UpperBody">
			<ActivationCondition>
				<And>
					<Or Comment="Input Checks">
						<And Comment="Virtual Input Check">
							<Or>
								<IfInput InputName="Use_MainHandSlot_2" BackTicks="20" BackTickCancelInput="Use_MainHandSlot_1"/>
								<And Comment="Unsheathe if nothing equipped in alternate main hand or alternate main hand is active">
									<IfInput InputName="quickslot-weaponSwap" BackTicks="20"/>
									<Or>
										<Not>
											<IfItemInSlot SlotToCheckAlias="MAIN_HAND_OPTION_1" RequireSlotValid="true" RequireSlotActive="false"/>
										</Not>
										<And>
											<IfItemInSlot SlotToCheckAlias="MAIN_HAND_OPTION_1" RequireSlotValid="false" RequireSlotActive="true"/>
											<IfItemInSlot SlotToCheckAlias="MAIN_HAND_OPTION_1" RequireSlotValid="true" RequireSlotActive="false"/>
										</And>
									</Or>
								</And>
							</Or>
							<Or>
								<Not>
									<IfItemInSlot SlotToCheckAlias="MAIN_HAND_OPTION_2" RequireSlotValid="false" RequireSlotActive="true"/>
								</Not>
								<IfItemSheathed SlotToCheckAlias="MAIN_HAND_OPTION_2"/>
							</Or>
							<IfItemInSlot SlotToCheckAlias="MAIN_HAND_OPTION_2" RequireSlotValid="true" RequireSlotActive="false"/>
						</And>
						<And>
							<Or>
								<IfInput InputName="sheathe"/>
								<And>
									<IfMannequinTag TagName="AutoUnsheathe"/>
									<Not>
										<Or>
											<IfInput InputName="Use_MainHandSlot_1" BackTicks="20" BackTickCancelInput="Use_MainHandSlot_2"/>
											<IfInput InputName="quickslot-weaponSwap" BackTicks="20" BackTickCancelInput="Use_MainHandSlot_2"/>
										</Or>
									</Not>
									<Not>
										<IfMannequinMarker MarkerName="PreventUnsheathe"/>
									</Not>
									<Or>
										<Not>
											<IfMannequinTag TagName="Prone"/>
										</Not>
										<IfMannequinTag TagName="Rifle"/>
									</Or>
									<Not>
										<IfInput InputName="Apply_Resources_To_Structure"/>
									</Not>
									<Not>
										<IfMannequinTag TagName="SmokeAim"/>
									</Not>
									<Not>
										<IsRequestingInteraction>
											<Interactions>
												<Interaction InteractionId="ModifyStatusEffect"/>
											</Interactions>
										</IsRequestingInteraction>
									</Not>
								</And>
							</Or>
							<IfItemInSlot SlotToCheckAlias="MAIN_HAND_OPTION_2" RequireSlotValid="false" RequireSlotActive="true"/>
							<IfItemSheathed SlotToCheckAlias="MAIN_HAND_OPTION_2"/>
							<IfItemInSlot SlotToCheckAlias="MAIN_HAND_OPTION_2" RequireSlotValid="true" RequireSlotActive="false"/>
							<IfItemInSlotEquippable SlotToCheckAlias="MAIN_HAND_OPTION_2"/>
						</And>
					</Or>
					<Or>
						<Not Comment="Attempting to transition to prone">
							<And>
								<IfMannequinTag TagName="Prone_Desired"/>
								<Or>
									<IfMannequinTag TagName="Stand"/>
									<IfMannequinTag TagName="Crouch"/>
								</Or>
							</And>
						</Not>
						<And>
							<IfMannequinTag TagName="Prone"/>
							<IfMannequinTag TagName="Prone_Desired"/>
							<IfMannequinTag TagName="Rifle"/>
						</And>
					</Or>
					<Or Comment="Check if collision above">
						<And>
							<IfMannequinTag TagName="Prone"/>
							<IfCanResizeCharacterController ResizeType="Crouch"/>
						</And>
						<IfMannequinTag TagName="Crouch"/>
						<IfMannequinTag TagName="Stand"/>
					</Or>
					<Not>
						<IfIsLoadoutOpen/>
					</Not>
					<Not>
						<IfFragmentPlaying UseHostActionFragment="true"/>
					</Not>
					<Not>
						<IfInput InputName="Placing_Building"/>
					</Not>
					<Not Comment="Shooters Stance Check">
						<And>
							<Or>
								<IfMannequinTag TagName="Rifle"/>
								<IfMannequinTag TagName="Blunderbuss"/>
							</Or>
							<IfMannequinTag TagName="HeavyAim"/>
						</And>
					</Not>
					<IfItemInSlotEquippable SlotToCheckAlias="MAIN_HAND_OPTION_2"/>
					<Not>
						<IfWaterDepth Comparison="Equal" WaterDepth="Deep"/>
					</Not>
					<Not>
						<IfMannequinTag TagName="ChargeTimer"/>
					</Not>
					<Not>
						<IfExternalCondition ConditionName="Throwable_Equipped"/>
					</Not>
				</And>
			</ActivationCondition>
			<DeactivationCondition>
				<Or>
