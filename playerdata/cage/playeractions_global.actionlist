								</Group>
							</ElseActivity>
						</IfThenElse>
					</ElseActivity>
				</IfThenElse>
			</Activities>
		</Action>
		<Action Name="Exhausted">
			<ActivationCondition>
				<Or>
					<And>
						<Not>
							<IfActionStatus ActionName="Exhausted" Status="Installed"/>
						</Not>
						<IfStaminaValue ComparisonValue="0.00" ComparisonValueIsPercentage="true" ComparisonType="LessThanOrEqual"/>
					</And>
					<IfHasStatusEffect StatusEffect="ExhaustedState"/>
					<IfMannequinTag TagName="Exhausted"/>
				</Or>
			</ActivationCondition>
			<DeactivationCondition>
				<IfStaminaValue ComparisonValue="1.00" ComparisonValueIsPercentage="true" ComparisonType="Equal"/>
			</DeactivationCondition>
			<Activities>
				<SetMannequinTag ResetWhenReinstalled="false">
					<Default TagName="Exhausted" OnEnter="Set" OnExit="Unset"/>
				</SetMannequinTag>
				<SendUIEvent>
					<Default OnEnter="Send" EnterEventName="Hud.LocalPlayer.Vitals.StaminaLocked" OnExit="Send" ExitEventName="Hud.LocalPlayer.Vitals.StaminaLocked"/>
				</SendUIEvent>
				<SendUIEvent>
					<Default OnEnter="Send" EnterEventName="Cage.StaminaIsWinded" OnExit="Send" ExitEventName="Cage.StaminaIsWinded"/>
				</SendUIEvent>
				<ModifyStatusEffects>
					<Default>
						<EffectsToApplyOnEnter>
							<EffectId EffectId="ExhaustedState"/>
						</EffectsToApplyOnEnter>
						<EffectsToRemoveOnExit>
							<EffectId EffectId="ExhaustedState"/>
						</EffectsToRemoveOnExit>
						<EffectsToRemoveOnEnter>
							<EffectId EffectId="Status_Perk_Chest_OnBlockedHitTaken_StaminaRegen"/>
						</EffectsToRemoveOnEnter>
					</Default>
				</ModifyStatusEffects>
			</Activities>
		</Action>
		<Action Name="Hide_Name_Tag">
			<ActivationCondition>
				<And>
					<Or>
						<IfMannequinTag TagName="Crouch"/>
						<IfMannequinTag TagName="Prone"/>
						<IfHasStatusEffect StatusEffect="Bow_Smoke"/>
						<IfHasStatusEffect StatusEffect="Bow_Smoke_Long"/>
						<IfHasStatusEffect StatusEffect="IceMagic_Entombed"/>
					</Or>
					<Not>
						<IfInput InputName="Start_Gathering"/>
					</Not>
				</And>
			</ActivationCondition>
			<DeactivationCondition>
				<Or>
					<And>
						<Not>
							<IfMannequinTag TagName="Crouch"/>
						</Not>
						<Not>
							<IfMannequinTag TagName="Prone"/>
						</Not>
						<Not>
							<IfHasStatusEffect StatusEffect="Bow_Smoke"/>
						</Not>
						<Not>
							<IfHasStatusEffect StatusEffect="Bow_Smoke_Long"/>
						</Not>
						<Not>
							<IfHasStatusEffect StatusEffect="IceMagic_Entombed"/>
						</Not>
					</And>
					<IfInput InputName="Start_Gathering"/>
				</Or>
			</DeactivationCondition>
			<Activities>
				<HidePlayerNameTag RunOnRemoteClients="true">
					<Default OnEnter="PartiallyHidden"/>
					<Conditional Name="PlayDead" OnEnter="FullyHidden"/>
					<Conditional Name="Prone" OnEnter="MostlyHidden"/>
					<Conditional Name="Smoke" OnEnter="FullyHidden"/>
					<Conditional Name="IceTomb" OnEnter="FullyHidden"/>
				</HidePlayerNameTag>
			</Activities>
		</Action>
		<Action Name="Health_Desaturation">
			<ActivationCondition>
				<True/>
			</ActivationCondition>
			<Activities>
				<RunFxScript ResetWhenReinstalled="false" RunOnRemoteClients="false">
					<Default ScriptToRun="Desaturation_off" StopScriptOnExit="true" StopScriptOnDataChange="true" RunScriptOnSlotItem="false"/>
					<Conditional Name="Dead" ScriptToRun="Desaturation_dead" StopScriptOnExit="true" StopScriptOnDataChange="true" RunScriptOnSlotItem="false"/>
					<Conditional Name="DeathsDoor" ScriptToRun="Desaturation_deathsdoor" StopScriptOnExit="true" StopScriptOnDataChange="true" RunScriptOnSlotItem="false"/>
					<Conditional Name="Health_Low" ScriptToRun="Desaturation_hurt" StopScriptOnExit="true" StopScriptOnDataChange="true" RunScriptOnSlotItem="false"/>
				</RunFxScript>
			</Activities>
		</Action>
		<Action Name="AutoRun">
			<ActivationCondition>
				<And>
					<Or>
						<IfInput InputName="autorun"/>
						<IfActionStateFlag Flag="AutoRun"/>
					</Or>
					<Or>
						<Not>
							<IfMannequinMarker MarkerName="DisableAutorun"/>
						</Not>
						<IfAbilityActive AbilityName="PlayAction"/>
					</Or>
					<Not>
						<Or>
							<IfMannequinTag TagName="Aim"/>
							<IfMannequinTag TagName="HipAim"/>
							<IfMannequinTag TagName="AimDownSights"/>
							<And>
								<Not>
									<IfMannequinTag TagName="ContinueAutoRun"/>
								</Not>
								<Not>
									<IfHasStatusEffect StatusEffectCategory="AzothStaff"/>
								</Not>
								<IfMannequinTag TagName="AttackAction"/>
							</And>
							<IfFastTravelTeleporting/>
							<And>
								<Not>
									<IfStackConfigVar StackVar="javelin.enable-inventory-while-running"/>
								</Not>
								<IfIsLoadoutOpen/>
							</And>
							<IfIsGathering/>
							<IfIsInteractingWithStorage/>
							<IfInput InputName="Start_Crafting"/>
							<IfInput InputName="Start_Emote"/>
							<IfInCutscene/>
							<IsInStore/>
							<IsRequestingInteraction>
								<Interactions>
									<Interaction InteractionId="OpenTradingPost"/>
									<Interaction InteractionId="OpenPrefabRefinery"/>
									<Interaction InteractionId="OpenGenerator"/>
									<Interaction InteractionId="OpenTerritoryMarker"/>
									<Interaction InteractionId="OpenStorage"/>
									<Interaction InteractionId="OpenInteractScreen"/>
									<Interaction InteractionId="OpenStartClaimScreen"/>
									<Interaction InteractionId="OpenAttachment"/>
									<Interaction InteractionId="OpenPaint"/>
									<Interaction InteractionId="OpenLootStorage"/>
									<Interaction InteractionId="OpenRead"/>
									<Interaction InteractionId="OpenConversation"/>
									<Interaction InteractionId="OpenObjectiveProvider"/>
									<Interaction InteractionId="OpenArmory"/>
									<Interaction InteractionId="UseTurret"/>
									<Interaction InteractionId="OpenTerritoryInterface"/>
