									</ExecuteAudioTrigger>
									<ExecuteAudioTrigger ResetWhenReinstalled="true" RunOnRemoteClients="true" IgnoreDataChanges="false" Comment="Calf_left">
										<Default AudioTriggerName="Set_Switch_Posture_Prone" ForceStopFxOnDataChange="false" ExecuteOnLocalPlayer="true" ExecuteOnRemotePlayer="true" AttachmentJoint="Calf_left" AttachToEntity="true"/>
									</ExecuteAudioTrigger>
									<ExecuteAudioTrigger ResetWhenReinstalled="true" RunOnRemoteClients="true" IgnoreDataChanges="false" Comment="Calf_right">
										<Default AudioTriggerName="Set_Switch_Posture_Prone" ForceStopFxOnDataChange="false" ExecuteOnLocalPlayer="true" ExecuteOnRemotePlayer="true" AttachmentJoint="Calf_right" AttachToEntity="true"/>
									</ExecuteAudioTrigger>
									<ExecuteAudioTrigger ResetWhenReinstalled="true" RunOnRemoteClients="true" IgnoreDataChanges="false" Comment="Weapon joints">
										<Default AudioTriggerName="" ForceStopFxOnDataChange="false" ExecuteOnLocalPlayer="true" ExecuteOnRemotePlayer="true" AttachToEntity="true"/>
										<Conditional Name="Rifle_Unsheathed" AudioTriggerName="Set_Switch_Posture_Prone" ForceStopFxOnDataChange="false" ExecuteOnLocalPlayer="true" ExecuteOnRemotePlayer="true" AttachmentJoint="right_index_1" AttachToEntity="true"/>
										<Conditional Name="Rifle_Sheathed" AudioTriggerName="Set_Switch_Posture_Prone" ForceStopFxOnDataChange="false" ExecuteOnLocalPlayer="true" ExecuteOnRemotePlayer="true" AttachmentJoint="rifle_attach" AttachToEntity="true"/>
										<Conditional Name="Bow_Equipped" AudioTriggerName="Set_Switch_Posture_Prone" ForceStopFxOnDataChange="false" ExecuteOnLocalPlayer="true" ExecuteOnRemotePlayer="true" AttachmentJoint="quiver_attach" AttachToEntity="true"/>
									</ExecuteAudioTrigger>
								</Group>
							</ThenActivity>
							<ElseActivity>
								<Group Comment="Stand">
									<ExecuteAudioTrigger ResetWhenReinstalled="true" RunOnRemoteClients="true" IgnoreDataChanges="false" Comment="Dummy Object">
										<Default AudioTriggerName="Set_Switch_Posture_Stnd" ForceStopFxOnDataChange="false" ExecuteOnLocalPlayer="true" ExecuteOnRemotePlayer="false" AttachToEntity="false"/>
									</ExecuteAudioTrigger>
									<ExecuteAudioTrigger ResetWhenReinstalled="true" RunOnRemoteClients="true" IgnoreDataChanges="false" Comment="Default proxy">
										<Default AudioTriggerName="Set_Switch_Posture_Stnd" ForceStopFxOnDataChange="false" ExecuteOnLocalPlayer="true" ExecuteOnRemotePlayer="true" AttachToEntity="true"/>
									</ExecuteAudioTrigger>
									<ExecuteAudioTrigger ResetWhenReinstalled="true" RunOnRemoteClients="true" IgnoreDataChanges="false" Comment="Forearm_left">
										<Default AudioTriggerName="Set_Switch_Posture_Stnd" ForceStopFxOnDataChange="false" ExecuteOnLocalPlayer="true" ExecuteOnRemotePlayer="true" AttachmentJoint="Forearm_left" AttachToEntity="true"/>
									</ExecuteAudioTrigger>
									<ExecuteAudioTrigger ResetWhenReinstalled="true" RunOnRemoteClients="true" IgnoreDataChanges="false" Comment="Forearm_right">
										<Default AudioTriggerName="Set_Switch_Posture_Stnd" ForceStopFxOnDataChange="false" ExecuteOnLocalPlayer="true" ExecuteOnRemotePlayer="true" AttachmentJoint="Forearm_right" AttachToEntity="true"/>
									</ExecuteAudioTrigger>
									<ExecuteAudioTrigger ResetWhenReinstalled="true" RunOnRemoteClients="true" IgnoreDataChanges="false" Comment="Calf_left">
										<Default AudioTriggerName="Set_Switch_Posture_Stnd" ForceStopFxOnDataChange="false" ExecuteOnLocalPlayer="true" ExecuteOnRemotePlayer="true" AttachmentJoint="Calf_left" AttachToEntity="true"/>
									</ExecuteAudioTrigger>
									<ExecuteAudioTrigger ResetWhenReinstalled="true" RunOnRemoteClients="true" IgnoreDataChanges="false" Comment="Calf_right">
										<Default AudioTriggerName="Set_Switch_Posture_Stnd" ForceStopFxOnDataChange="false" ExecuteOnLocalPlayer="true" ExecuteOnRemotePlayer="true" AttachmentJoint="Calf_right" AttachToEntity="true"/>
									</ExecuteAudioTrigger>
									<ExecuteAudioTrigger ResetWhenReinstalled="true" RunOnRemoteClients="true" IgnoreDataChanges="false" Comment="Weapon joints">
										<Default AudioTriggerName="" ForceStopFxOnDataChange="false" ExecuteOnLocalPlayer="true" ExecuteOnRemotePlayer="true" AttachToEntity="true"/>
										<Conditional Name="Rifle_Unsheathed" AudioTriggerName="Set_Switch_Posture_Stnd" ForceStopFxOnDataChange="false" ExecuteOnLocalPlayer="true" ExecuteOnRemotePlayer="true" AttachmentJoint="right_index_1" AttachToEntity="true"/>
										<Conditional Name="Rifle_Sheathed" AudioTriggerName="Set_Switch_Posture_Stnd" ForceStopFxOnDataChange="false" ExecuteOnLocalPlayer="true" ExecuteOnRemotePlayer="true" AttachmentJoint="rifle_attach" AttachToEntity="true"/>
