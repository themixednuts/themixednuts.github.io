<ActionList>
	<Actions>
		<Action Name="Set_Reticle">
			<ActivationCondition>
				<Or>
					<And Comment="Bow">
						<IfMannequinTag TagName="Bow"/>
						<Not>
							<IfMannequinTag TagName="HeavyAim"/>
						</Not>
						<Not>
							<IfItemSheathed SlotToCheckAlias="ACTIVE_MAIN_HAND_WEAPON"/>
						</Not>
					</And>
					<And Comment="Rifle">
						<Or>
							<IfMannequinTag TagName="Rifle"/>
							<IfMannequinTag TagName="Blunderbuss"/>
						</Or>
						<Not>
							<IfItemSheathed SlotToCheckAlias="ACTIVE_MAIN_HAND_WEAPON"/>
						</Not>
					</And>
					<And Comment="2H_Ranged">
						<IfMannequinTag TagName="2H_Ranged"/>
						<IfMannequinTag TagName="AimDownSights"/>
						<Not>
							<IfItemSheathed SlotToCheckAlias="ACTIVE_MAIN_HAND_WEAPON"/>
						</Not>
					</And>
					<And Comment="1H_Ranged">
						<IfMannequinTag TagName="1H_Ranged"/>
						<IfMannequinTag TagName="AimDownSights"/>
						<Not>
							<IfItemSheathed SlotToCheckAlias="ACTIVE_MAIN_HAND_WEAPON"/>
						</Not>
					</And>
					<And Comment="Throwable">
						<IfMannequinTag TagName="Throwable"/>
						<IfMannequinTag TagName="AimDownSights"/>
						<Not>
							<IfItemSheathed SlotToCheckAlias="ACTIVE_MAIN_HAND_WEAPON"/>
						</Not>
					</And>
					<IfMannequinTag TagName="TurretCamera"/>
				</Or>
			</ActivationCondition>
			<Activities>
				<SetReticle>
					<Default OnEnter="Set" OnExit="Clear"/>
					<Conditional Name="Blunderbuss_MortarShot" OnEnter="Set" OnExit="Clear" OverrideReticleName="BlunderbussMortar" UseLoadedAmmoData="true"/>
					<Conditional Name="Blunderbuss_Reticle" OnEnter="Set" OnExit="Clear" UseLoadedAmmoData="true"/>
				</SetReticle>
			</Activities>
		</Action>
		<Action Name="Drown">
			<ActivationCondition>
				<And>
					<Or>
						<IfWaterDepth Comparison="Equal" WaterDepth="Deep"/>
						<And>
							<IfWaterDepth Comparison="Equal" WaterDepth="Shallow"/>
							<IfMannequinTag TagName="Crouch"/>
						</And>
					</Or>
					<Not>
						<Or>
							<IfDead Name="Dead" ServerOnly="false"/>
							<IfDeathsDoor Name="DeathsDoor" ServerOnly="false"/>
						</Or>
					</Not>
				</And>
			</ActivationCondition>
			<DeactivationCondition>
				<Or>
					<Not>
						<Or>
							<And>
								<IfWaterDepth Comparison="Equal" WaterDepth="Shallow"/>
								<IfMannequinTag TagName="Crouch"/>
							</And>
							<IfWaterDepth Comparison="Equal" WaterDepth="Deep"/>
						</Or>
					</Not>
					<IfDead Name="Dead" ServerOnly="false"/>
					<IfDeathsDoor Name="DeathsDoor" ServerOnly="false"/>
				</Or>
			</DeactivationCondition>
			<Activities>
				<ModifyStatusEffects>
					<Default>
						<EffectsToApplyOnEnter>
							<EffectId EffectId="Breath"/>
						</EffectsToApplyOnEnter>
						<EffectsToRemoveOnExit>
							<EffectId EffectId="Breath"/>
							<EffectId EffectId="Drowning"/>
						</EffectsToRemoveOnExit>
					</Default>
				</ModifyStatusEffects>
				<IfThenElse>
					<IfCondition>
						<IfWaterDepth Comparison="Equal" WaterDepth="Deep"/>
					</IfCondition>
					<ThenActivity>
						<DisableInteractor>
							<Default OnEnter="Disable" OnExit="Enable"/>
						</DisableInteractor>
					</ThenActivity>
				</IfThenElse>
			</Activities>
		</Action>
		<Action Name="OneOffInteraction">
			<ActivationCondition>
				<And>
					<CanInteract/>
					<IsRequestingInteraction>
						<Interactions>
							<Interaction InteractionId="CyclePermissions"/>
							<Interaction InteractionId="RemoveRespawn"/>
							<Interaction InteractionId="ToggleDoor"/>
							<Interaction InteractionId="SetSpawnPoint"/>
							<Interaction InteractionId="Dismantle"/>
							<Interaction InteractionId="TakeAll"/>
							<Interaction InteractionId="ToggleOnOff"/>
							<Interaction InteractionId="AddTerritoryClaimTime"/>
							<Interaction InteractionId="UpgradeBuildable"/>
							<Interaction InteractionId="CancelUpgradeBuildable"/>
