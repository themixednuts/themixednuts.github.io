							<Interactions>
								<Interaction InteractionId="GatherResource"/>
								<Interaction InteractionId="GatherWaterResource"/>
							</Interactions>
						</IsRequestingInteraction>
					</Not>
					<Not>
						<IfInput InputName="Apply_Resources_To_Structure" Comment="IfShouldBuild Virtual Input"/>
					</Not>
					<Not>
						<IfInput InputName="Start_Crafting" Comment="IfShouldCraft Virtual Input"/>
					</Not>
					<Not>
						<IfFastTravelTeleporting/>
					</Not>
				</And>
			</ActivationCondition>
			<DeactivationCondition>
				<Not>
					<Or>
						<IsRequestingInteraction>
							<Interactions>
								<Interaction InteractionId="CyclePermissions"/>
								<Interaction InteractionId="RemoveRespawn"/>
								<Interaction InteractionId="ToggleDoor"/>
								<Interaction InteractionId="SetSpawnPoint"/>
								<Interaction InteractionId="Dismantle"/>
								<Interaction InteractionId="TakeAll"/>
								<Interaction InteractionId="ToggleOnOff"/>
								<Interaction InteractionId="AddTerritoryClaimTime"/>
								<Interaction InteractionId="UpgradeBuildable"/>
								<Interaction InteractionId="CancelUpgradeBuildable"/>
								<Interaction InteractionId="IncreaseStructureSecurityLevel"/>
								<Interaction InteractionId="DecreaseStructureSecurityLevel"/>
								<Interaction InteractionId="TakeOverBuildable"/>
								<Interaction InteractionId="PurchaseClaim"/>
								<Interaction InteractionId="SetStructureSecurityLevel"/>
								<Interaction InteractionId="StructureDeposit"/>
								<Interaction InteractionId="StructureWithdraw"/>
								<Interaction InteractionId="AddToJournal"/>
								<Interaction InteractionId="SetStructureName"/>
								<Interaction InteractionId="InteractWithItemCost"/>
								<Interaction InteractionId="HousingQuickEnter"/>
								<Interaction InteractionId="HousingExit"/>
								<Interaction InteractionId="ObjectiveInteraction"/>
								<Interaction InteractionId="TeleportToOtherSideOfDoor"/>
								<Interaction InteractionId="UseArenaItem"/>
								<Interaction InteractionId="EnterDungeon"/>
								<Interaction InteractionId="BindToCamp"/>
								<Interaction InteractionId="ExitDungeon"/>
								<Interaction InteractionId="OpenMap"/>
								<Interaction InteractionId="PlayCinematicVideo"/>
								<Interaction InteractionId="InteractTeleporter"/>
								<Interaction InteractionId="TipPerformers"/>
							</Interactions>
						</IsRequestingInteraction>
					</Or>
				</Not>
			</DeactivationCondition>
			<Activities>
				<ExecuteInteraction/>
			</Activities>
		</Action>
		<Action Name="Audio_SwitchManager">
			<ActivationCondition>
				<True/>
			</ActivationCondition>
			<Activities>
				<IfThenElse ResetWhenReinstalled="true" Comment="Posture audio switch">
					<IfCondition>
						<Or Comment="Crouch without transitions or FromCrouch while transitioning to something else">
							<And>
								<IfMannequinTag TagName="Crouch"/>
								<Not>
									<IfMannequinTag TagName="FromStand"/>
								</Not>
								<Not>
									<IfMannequinTag TagName="FromProne"/>
								</Not>
							</And>
							<IfMannequinTag TagName="FromCrouch"/>
						</Or>
					</IfCondition>
					<ThenActivity>
						<Group Comment="Crouch">
							<ExecuteAudioTrigger ResetWhenReinstalled="true" RunOnRemoteClients="true" IgnoreDataChanges="false" Comment="Dummy Object">
								<Default AudioTriggerName="Set_Switch_Posture_Crch" ForceStopFxOnDataChange="false" ExecuteOnLocalPlayer="true" ExecuteOnRemotePlayer="false" AttachToEntity="false"/>
							</ExecuteAudioTrigger>
							<ExecuteAudioTrigger ResetWhenReinstalled="true" RunOnRemoteClients="true" IgnoreDataChanges="false" Comment="Default proxy">
								<Default AudioTriggerName="Set_Switch_Posture_Crch" ForceStopFxOnDataChange="false" ExecuteOnLocalPlayer="true" ExecuteOnRemotePlayer="true" AttachToEntity="true"/>
							</ExecuteAudioTrigger>
							<ExecuteAudioTrigger ResetWhenReinstalled="true" RunOnRemoteClients="true" IgnoreDataChanges="false" Comment="Forearm_left">
								<Default AudioTriggerName="Set_Switch_Posture_Crch" ForceStopFxOnDataChange="false" ExecuteOnLocalPlayer="true" ExecuteOnRemotePlayer="true" AttachmentJoint="Forearm_left" AttachToEntity="true"/>
							</ExecuteAudioTrigger>
							<ExecuteAudioTrigger ResetWhenReinstalled="true" RunOnRemoteClients="true" IgnoreDataChanges="false" Comment="Forearm_right">
								<Default AudioTriggerName="Set_Switch_Posture_Crch" ForceStopFxOnDataChange="false" ExecuteOnLocalPlayer="true" ExecuteOnRemotePlayer="true" AttachmentJoint="Forearm_right" AttachToEntity="true"/>
							</ExecuteAudioTrigger>
							<ExecuteAudioTrigger ResetWhenReinstalled="true" RunOnRemoteClients="true" IgnoreDataChanges="false" Comment="Calf_left">
								<Default AudioTriggerName="Set_Switch_Posture_Crch" ForceStopFxOnDataChange="false" ExecuteOnLocalPlayer="true" ExecuteOnRemotePlayer="true" AttachmentJoint="Calf_left" AttachToEntity="true"/>
							</ExecuteAudioTrigger>
							<ExecuteAudioTrigger ResetWhenReinstalled="true" RunOnRemoteClients="true" IgnoreDataChanges="false" Comment="Calf_right">
								<Default AudioTriggerName="Set_Switch_Posture_Crch" ForceStopFxOnDataChange="false" ExecuteOnLocalPlayer="true" ExecuteOnRemotePlayer="true" AttachmentJoint="Calf_right" AttachToEntity="true"/>
							</ExecuteAudioTrigger>
							<ExecuteAudioTrigger ResetWhenReinstalled="true" RunOnRemoteClients="true" IgnoreDataChanges="false" Comment="Weapon joints">
								<Default AudioTriggerName="" ForceStopFxOnDataChange="false" ExecuteOnLocalPlayer="true" ExecuteOnRemotePlayer="true" AttachToEntity="true"/>
								<Conditional Name="Rifle_Unsheathed" AudioTriggerName="Set_Switch_Posture_Crch" ForceStopFxOnDataChange="false" ExecuteOnLocalPlayer="true" ExecuteOnRemotePlayer="true" AttachmentJoint="right_index_1" AttachToEntity="true"/>
								<Conditional Name="Rifle_Sheathed" AudioTriggerName="Set_Switch_Posture_Crch" ForceStopFxOnDataChange="false" ExecuteOnLocalPlayer="true" ExecuteOnRemotePlayer="true" AttachmentJoint="rifle_attach" AttachToEntity="true"/>
								<Conditional Name="Bow_Equipped" AudioTriggerName="Set_Switch_Posture_Crch" ForceStopFxOnDataChange="false" ExecuteOnLocalPlayer="true" ExecuteOnRemotePlayer="true" AttachmentJoint="quiver_attach" AttachToEntity="true"/>
							</ExecuteAudioTrigger>
						</Group>
					</ThenActivity>
					<ElseActivity>
						<IfThenElse ResetWhenReinstalled="true">
							<IfCondition>
								<Or Comment="Prone without transitions or FromProne while transitioning to something else">
									<And>
										<IfMannequinTag TagName="Prone"/>
										<Not>
											<IfMannequinTag TagName="FromStand"/>
										</Not>
										<Not>
											<IfMannequinTag TagName="FromCrouch"/>
										</Not>
									</And>
									<IfMannequinTag TagName="FromProne"/>
								</Or>
							</IfCondition>
							<ThenActivity>
								<Group Comment="Prone">
									<ExecuteAudioTrigger ResetWhenReinstalled="true" RunOnRemoteClients="true" IgnoreDataChanges="false" Comment="Dummy Object">
										<Default AudioTriggerName="Set_Switch_Posture_Prone" ForceStopFxOnDataChange="false" ExecuteOnLocalPlayer="true" ExecuteOnRemotePlayer="false" AttachToEntity="false"/>
									</ExecuteAudioTrigger>
									<ExecuteAudioTrigger ResetWhenReinstalled="true" RunOnRemoteClients="true" IgnoreDataChanges="false" Comment="Default proxy">
										<Default AudioTriggerName="Set_Switch_Posture_Prone" ForceStopFxOnDataChange="false" ExecuteOnLocalPlayer="true" ExecuteOnRemotePlayer="true" AttachToEntity="true"/>
									</ExecuteAudioTrigger>
									<ExecuteAudioTrigger ResetWhenReinstalled="true" RunOnRemoteClients="true" IgnoreDataChanges="false" Comment="Forearm_left">
										<Default AudioTriggerName="Set_Switch_Posture_Prone" ForceStopFxOnDataChange="false" ExecuteOnLocalPlayer="true" ExecuteOnRemotePlayer="true" AttachmentJoint="Forearm_left" AttachToEntity="true"/>
									</ExecuteAudioTrigger>
