<ActionList>
	<Actions>
		<Action Name="Sprint_Start" FragmentName="Sprint_Start">
			<ActivationCondition>
				<And>
					<Not>
						<IfInput InputName="walk"/>
					</Not>
					<Not>
						<IfExternalCondition ConditionName="Throwable_Equipped"/>
					</Not>
					<Not>
						<Or>
							<IfHasStatusEffect StatusEffect="Sprint_Delay_Timer"/>
							<IfHasStatusEffect StatusEffect="Sprint_Delay_Combat_Timer_light"/>
							<IfHasStatusEffect StatusEffect="Sprint_Delay_Combat_Timer_Medium"/>
							<IfHasStatusEffect StatusEffect="Sprint_Delay_Combat_Timer_Heavy"/>
							<IfHasStatusEffect StatusEffect="ForceRun"/>
						</Or>
					</Not>
					<Not>
						<ActionConditionEquipLoadCategory Value="Overburdened"/>
					</Not>
					<Not>
						<IfFragmentPlaying UseHostActionFragment="false" FragmentName="Sprint"/>
					</Not>
					<Not>
						<IfMannequinTag TagName="AimDownSights"/>
					</Not>
					<Or Comment="Sprint input checks">
						<IfFragmentPlaying UseHostActionFragment="false" FragmentName="Sprint_PostureTransition"/>
						<And>
							<Or>
								<Not>
									<IfFragmentPlaying UseHostActionFragment="false" FragmentName="Sprint"/>
								</Not>
								<And Comment="Standing from crouch sprint">
									<IfMannequinTag TagName="Crouch"/>
									<IfMannequinTag TagName="Stand_Desired"/>
									<IfExternalCondition ConditionName="CombatNav"/>
								</And>
							</Or>
						</And>
						<IfMannequinTag TagName="AutoRun"/>
						<IfHasStatusEffect StatusEffect="PotionHaste"/>
					</Or>
					<Or Comment="Pressing any Forward direction key">
						<And Comment="Forward input during strafe sprint">
							<IfPlayerSetting PlayerSettingName="SprintStrafeMovement"/>
							<Or>
								<IfPlayerSetting PlayerSettingName="LockedStrafeMovement"/>
								<IfCameraLockActive/>
								<IfExternalCondition ConditionName="CombatNav"/>
							</Or>
							<IfExternalCondition ConditionName="Forward_180_Input"/>
						</And>
						<And Comment="Any input during non-strafe sprint movement">
							<Not>
								<IfCameraLockActive/>
							</Not>
							<Not>
								<IfExternalCondition ConditionName="CombatNav"/>
							</Not>
							<Or Comment="SprintStrafeMovement or LockedStrafeMovement">
								<Not>
									<IfPlayerSetting PlayerSettingName="SprintStrafeMovement"/>
								</Not>
								<Not>
									<IfPlayerSetting PlayerSettingName="LockedStrafeMovement"/>
								</Not>
							</Or>
							<IfExternalCondition ConditionName="Any_Nav_Input"/>
						</And>
					</Or>
					<IfCanSprint/>
					<IfCanNav/>
					<Not>
						<IfP2PTrading/>
					</Not>
					<Or Comment="Valid Tag states">
						<IfMannequinTag TagName="Idle"/>
						<IfMannequinTag TagName="AttackAction"/>
						<Or>
							<IfMannequinTag TagName="FromCrouch"/>
							<IfMannequinTag TagName="FromProne"/>
						</Or>
						<And>
							<IfFragmentPlaying UseHostActionFragment="false" FragmentName="Strafe"/>
							<IfMannequinTag TagName="BackNav"/>
						</And>
						<And>
							<IfMannequinTag TagName="Stand"/>
							<Or>
								<And>
									<IfExternalCondition ConditionName="CombatNav"/>
									<MoveSpeed Comparison="LessThan" Value="4"/>
								</And>
								<And>
									<Not>
										<IfExternalCondition ConditionName="CombatNav"/>
									</Not>
									<MoveSpeed Comparison="LessThan" Value="2.50"/>
								</And>
							</Or>
						</And>
						<And Comment="Dodging into sprint">
							<IfMannequinTag TagName="Dodging"/>
							<IfMannequinTag TagName="Stand_Desired"/>
						</And>
					</Or>
					<Not>
						<IfMannequinMarker MarkerName="PreventNav"/>
					</Not>
					<Not>
						<IfFragmentPlaying UseHostActionFragment="true"/>
					</Not>
					<Or Comment="Valid Action Check">
						<Not>
							<IfMannequinTag TagName="Sprint"/>
						</Not>
						<And Comment="Exiting water">
							<IfMannequinTag TagName="Sprint"/>
							<IfFragmentPlaying UseHostActionFragment="false" FragmentName="Strafe"/>
						</And>
						<And Comment="Standing from crouch sprint">
							<IfMannequinTag TagName="Crouch"/>
							<IfMannequinTag TagName="Stand_Desired"/>
						</And>
						<IfMannequinMarker MarkerName="ResumeSprint"/>
						<IfMannequinTag TagName="BackNav"/>
					</Or>
					<And Comment="If Wading, bypass Sprint Start">
						<Not>
							<IfExternalCondition ConditionName="Wade"/>
						</Not>
						<Not>
							<IfExternalCondition ConditionName="Wade+Crouch"/>
						</Not>
					</And>
					<Not>
						<IfEncumbered/>
					</Not>
					<Not>
						<IfMannequinMarker MarkerName="ForceStop"/>
					</Not>
					<Not>
						<IfMannequinMarker MarkerName="PreventSprint"/>
					</Not>
					<Not>
						<IfHasStatusEffect StatusEffect="SkipSprintStartTimer"/>
					</Not>
					<Not>
						<IfMannequinMarker MarkerName="ForceWalk"/>
					</Not>
					<Not>
						<IfInput InputName="Placing_Building"/>
					</Not>
					<Not Comment="Fix for bad TravelAngle rotation pop">
						<And>
							<IfFragmentPlaying UseHostActionFragment="false" FragmentName="Strafe"/>
							<IfMannequinTag TagName="BackNav"/>
						</And>
					</Not>
					<Not Comment="Fix for bad TravelAngle rotation pop">
						<IfFragmentPlaying UseHostActionFragment="false" FragmentName="Ability_Sword_ShieldRush"/>
					</Not>
					<Not>
						<IfMannequinTag TagName="HeavyAim"/>
					</Not>
				</And>
			</ActivationCondition>
			<DeactivationCondition>
				<Or>
					<Not>
						<IfAnalogInput AnalogInput="None" DigitalForwardInput="ForwardNav" DigitalBackInput="moveback" DigitalRightInput="moveright" DigitalLeftInput="moveleft" MinInputMagnitude="0.10" MaxInputMagnitude="1.00" RequireTargetAngleMatch="false"/>
					</Not>
					<IfP2PTrading/>
					<Not>
						<IfCanSprint/>
					</Not>
					<ActionConditionEquipLoadCategory Value="Overburdened"/>
					<IfEncumbered/>
					<IfMannequinMarker MarkerName="ForceStop"/>
					<IfInput InputName="walk"/>
					<IfMannequinMarker MarkerName="ForceWalk"/>
					<IfMannequinMarker MarkerName="PreventSprint"/>
					<IfInput InputName="Placing_Building"/>
				</Or>
			</DeactivationCondition>
			<Activities>
				<ApplyMoveSpeedMultiplier>
					<Default MultiplierType="SprintSpeedMod"/>
				</ApplyMoveSpeedMultiplier>
				<SetWeaponAccuracyBonus ResetWhenReinstalled="true">
					<Default StanceBonus="12.0" MovementBonus="0.0"/>
					<Conditional Name="Leviathon" StanceBonus="12.0" MovementBonus="0.0"/>
					<Conditional Name="LeviathonIn" StanceBonus="25.0" MovementBonus="0.0"/>
					<Conditional Name="AimStand" StanceBonus="25.0" MovementBonus="0.0"/>
					<Conditional Name="AimCrouch" StanceBonus="25.0" MovementBonus="0.0"/>
					<Conditional Name="AimProne" StanceBonus="25.0" MovementBonus="0.0"/>
					<Conditional Name="Crouch" StanceBonus="14.0" MovementBonus="0.0"/>
					<Conditional Name="Prone" StanceBonus="18.0" MovementBonus="0.0"/>
				</SetWeaponAccuracyBonus>
				<IfThenElse ResetWhenReinstalled="true" Comment="Equip Load speed modifier">
					<IfCondition>
						<IfMannequinTag TagName="Stand"/>
					</IfCondition>
					<ThenActivity>
						<ApplyEquipLoadMoveSpeedMultiplier/>
					</ThenActivity>
				</IfThenElse>
				<IfThenElse ResetWhenReinstalled="true" Comment="Check nav direction change over 80 and cancel inherit velocity">
					<IfCondition>
						<Not>
							<IfVelocityWithinInputAngle AngleTolerance="80.0"/>
						</Not>
					</IfCondition>
					<ThenActivity>
						<ClearMotion/>
					</ThenActivity>
				</IfThenElse>
				<IfThenElse ResetWhenReinstalled="true" Comment="If not eating or drinking then sprint">
					<IfCondition>
						<Not>
							<Or>
								<IfMannequinTag TagName="Drink"/>
								<IfMannequinTag TagName="Eat"/>
								<IfMannequinTag TagName="Potion"/>
							</Or>
						</Not>
					</IfCondition>
					<ThenActivity>
						<SetAudioSwitchState ResetWhenReinstalled="true" RunOnRemoteClients="true">
							<Default SwitchName="Navigation" StateName="Nav_Sprint" SetOnStop="false"/>
						</SetAudioSwitchState>
					</ThenActivity>
				</IfThenElse>
				<IfThenElse Comment="Targeting StickyLock Check">
					<IfCondition>
						<Not>
							<IfCameraStickyLockActive/>
						</Not>
					</IfCondition>
					<ThenActivity>
						<ForceCameraLock>
							<Default OnEnter="Disable" OnExit="NoEffect"/>
						</ForceCameraLock>
					</ThenActivity>
				</IfThenElse>
				<AIAudioTriggerActivity>
					<Default AudioTriggerName="Movement_Silent"/>
					<Conditional Name="Stand" AudioTriggerName="Sprint_Stand"/>
				</AIAudioTriggerActivity>
				<SetMannequinTag ResetWhenReinstalled="false">
					<Default TagName="Sprint" OnEnter="Set" OnExit="Unset"/>
				</SetMannequinTag>
				<SetMannequinTag ResetWhenReinstalled="false">
					<Default TagName="ForwardNav" OnEnter="Set" OnExit="Unset"/>
				</SetMannequinTag>
				<IfThenElse ResetWhenReinstalled="true">
					<IfCondition>
						<Or>
							<IfPlayerSetting PlayerSettingName="LockedStrafeMovement"/>
							<IfExternalCondition ConditionName="CombatNav"/>
						</Or>
					</IfCondition>
					<ThenActivity>
						<Group>
							<CameraRelativeFacing>
								<Default TurnRate="0.33" UseExponentialTurnRate="true" UseCameraDirOnEnterState="false" UseClosestTarget="false"/>
							</CameraRelativeFacing>
							<IfThenElse ResetWhenReinstalled="true">
								<IfCondition>
									<Or>
										<IfMannequinTag TagName="Blunderbuss"/>
										<Not>
											<True/>
										</Not>
									</Or>
								</IfCondition>
								<ThenActivity>
									<CameraRelativeMotion>
										<Default ForwardInput="ForwardNav" BackwardInput="moveback" LeftInput="moveleft" RightInput="moveright" ForcedMotion="false" UseInitialCameraRotation="false" AllowVerticalMotion="false" Speed="4,4,0" Accel="20,20,20" Decel="20,20,20" InheritVelocity="false" BackwardsMultiplier="0.55" AnalogInputOverride="LeftAnalog" UseAnalogInputToModulateSpeed="true" MinSpeed="1.4,1.4,0" MinAnalogInputMag="0.2" MaxAnalogInputMag="0.8" MovementAccuracyBonus="0.0"/>
										<Conditional Name="Wade+Crouch" ForwardInput="ForwardNav" BackwardInput="moveback" LeftInput="moveleft" RightInput="moveright" ForcedMotion="false" UseInitialCameraRotation="false" AllowVerticalMotion="false" Speed="1.75,1.75,0" Accel="20,20,20" Decel="20,20,20" InheritVelocity="false" BackwardsMultiplier="1.00" AnalogInputOverride="LeftAnalog" UseAnalogInputToModulateSpeed="true" MinSpeed="1.4,1.4,0" MinAnalogInputMag="0.2" MaxAnalogInputMag="0.8" MovementAccuracyBonus="0.0"/>
										<Conditional Name="Wade" ForwardInput="ForwardNav" BackwardInput="moveback" LeftInput="moveleft" RightInput="moveright" ForcedMotion="false" UseInitialCameraRotation="false" AllowVerticalMotion="false" Speed="2.5,2.5,0" Accel="20,20,20" Decel="20,20,20" InheritVelocity="false" BackwardsMultiplier="1.00" AnalogInputOverride="LeftAnalog" UseAnalogInputToModulateSpeed="true" MinSpeed="1.4,1.4,0" MinAnalogInputMag="0.2" MaxAnalogInputMag="0.8" MovementAccuracyBonus="0.0"/>
										<Conditional Name="TravelMode_Speed" ForwardInput="ForwardNav" BackwardInput="moveback" LeftInput="moveleft" RightInput="moveright" ForcedMotion="false" UseInitialCameraRotation="false" AllowVerticalMotion="false" Speed="5.5,5.5,0" Accel="20,20,20" Decel="20,20,20" InheritVelocity="false" BackwardsMultiplier="0.55" AnalogInputOverride="LeftAnalog" UseAnalogInputToModulateSpeed="true" MinSpeed="1.4,1.4,0" MinAnalogInputMag="0.2" MaxAnalogInputMag="0.8" MovementAccuracyBonus="0.0"/>
										<Conditional Name="Stand+Armed" ForwardInput="ForwardNav" BackwardInput="moveback" LeftInput="moveleft" RightInput="moveright" ForcedMotion="false" UseInitialCameraRotation="false" AllowVerticalMotion="false" Speed="5.5,5.5,0" Accel="10,10,10" Decel="20,20,20" InheritVelocity="false" BackwardsMultiplier="0.55" AnalogInputOverride="LeftAnalog" UseAnalogInputToModulateSpeed="true" MinSpeed="1.4,1.4,0" MinAnalogInputMag="0.2" MaxAnalogInputMag="0.8" MovementAccuracyBonus="0.0"/>
										<Conditional Name="Crouch" ForwardInput="ForwardNav" BackwardInput="moveback" LeftInput="moveleft" RightInput="moveright" ForcedMotion="false" UseInitialCameraRotation="false" AllowVerticalMotion="false" Speed="2.5,2.5,0" Accel="10,10,10" Decel="20,20,20" InheritVelocity="false" BackwardsMultiplier="0.55" AnalogInputOverride="LeftAnalog" UseAnalogInputToModulateSpeed="true" MinSpeed="1.4,1.4,0" MinAnalogInputMag="0.2" MaxAnalogInputMag="0.8" MovementAccuracyBonus="0.0"/>
										<Conditional Name="Prone" ForwardInput="ForwardNav" BackwardInput="moveback" LeftInput="moveleft" RightInput="moveright" ForcedMotion="false" UseInitialCameraRotation="false" AllowVerticalMotion="false" Speed="1.25,1.25,0" Accel="20,20,20" Decel="20,20,20" InheritVelocity="false" BackwardsMultiplier="0.55" AnalogInputOverride="LeftAnalog" UseAnalogInputToModulateSpeed="true" MinSpeed="1.4,1.4,0" MinAnalogInputMag="0.2" MaxAnalogInputMag="0.8" MovementAccuracyBonus="0.0"/>
										<Conditional Name="ServerValidatedCanSprint" ForwardInput="ForwardNav" BackwardInput="moveback" LeftInput="moveleft" RightInput="moveright" ForcedMotion="false" UseInitialCameraRotation="false" AllowVerticalMotion="false" Speed="5.5,5.5,0" Accel="20,20,20" Decel="20,20,20" InheritVelocity="false" BackwardsMultiplier="0.55" AnalogInputOverride="LeftAnalog" UseAnalogInputToModulateSpeed="true" MinSpeed="1.4,1.4,0" MinAnalogInputMag="0.2" MaxAnalogInputMag="0.8" MovementAccuracyBonus="0.0"/>
									</CameraRelativeMotion>
								</ThenActivity>
								<ElseActivity>
									<CameraRelativeMotion>
										<Default ForwardInput="ForwardNav" BackwardInput="moveback" LeftInput="moveleft" RightInput="moveright" ForcedMotion="false" UseInitialCameraRotation="false" AllowVerticalMotion="false" Speed="4,4,0" Accel="20,20,20" Decel="20,20,20" InheritVelocity="false" BackwardsMultiplier="0.55" AnalogInputOverride="LeftAnalog" UseAnalogInputToModulateSpeed="true" MinSpeed="1.4,1.4,0" MinAnalogInputMag="0.2" MaxAnalogInputMag="0.8" MovementAccuracyBonus="-20.0"/>
										<Conditional Name="Wade+Crouch" ForwardInput="ForwardNav" BackwardInput="moveback" LeftInput="moveleft" RightInput="moveright" ForcedMotion="false" UseInitialCameraRotation="false" AllowVerticalMotion="false" Speed="1.75,1.75,0" Accel="20,20,20" Decel="20,20,20" InheritVelocity="false" BackwardsMultiplier="1.00" AnalogInputOverride="LeftAnalog" UseAnalogInputToModulateSpeed="true" MinSpeed="1.4,1.4,0" MinAnalogInputMag="0.2" MaxAnalogInputMag="0.8" MovementAccuracyBonus="-7.0"/>
										<Conditional Name="Wade" ForwardInput="ForwardNav" BackwardInput="moveback" LeftInput="moveleft" RightInput="moveright" ForcedMotion="false" UseInitialCameraRotation="false" AllowVerticalMotion="false" Speed="2.5,2.5,0" Accel="20,20,20" Decel="20,20,20" InheritVelocity="false" BackwardsMultiplier="1.00" AnalogInputOverride="LeftAnalog" UseAnalogInputToModulateSpeed="true" MinSpeed="1.4,1.4,0" MinAnalogInputMag="0.2" MaxAnalogInputMag="0.8" MovementAccuracyBonus="-12.0"/>
										<Conditional Name="TravelMode_Speed" ForwardInput="ForwardNav" BackwardInput="moveback" LeftInput="moveleft" RightInput="moveright" ForcedMotion="false" UseInitialCameraRotation="false" AllowVerticalMotion="false" Speed="5.5,5.5,0" Accel="20,20,20" Decel="20,20,20" InheritVelocity="false" BackwardsMultiplier="0.55" AnalogInputOverride="LeftAnalog" UseAnalogInputToModulateSpeed="true" MinSpeed="1.4,1.4,0" MinAnalogInputMag="0.2" MaxAnalogInputMag="0.8" MovementAccuracyBonus="-20.0"/>
										<Conditional Name="Stand+Armed" ForwardInput="ForwardNav" BackwardInput="moveback" LeftInput="moveleft" RightInput="moveright" ForcedMotion="false" UseInitialCameraRotation="false" AllowVerticalMotion="false" Speed="5.5,5.5,0" Accel="10,10,10" Decel="20,20,20" InheritVelocity="false" BackwardsMultiplier="0.55" AnalogInputOverride="LeftAnalog" UseAnalogInputToModulateSpeed="true" MinSpeed="1.4,1.4,0" MinAnalogInputMag="0.2" MaxAnalogInputMag="0.8" MovementAccuracyBonus="-20.0"/>
										<Conditional Name="Crouch" ForwardInput="ForwardNav" BackwardInput="moveback" LeftInput="moveleft" RightInput="moveright" ForcedMotion="false" UseInitialCameraRotation="false" AllowVerticalMotion="false" Speed="2.5,2.5,0" Accel="10,10,10" Decel="20,20,20" InheritVelocity="false" BackwardsMultiplier="0.55" AnalogInputOverride="LeftAnalog" UseAnalogInputToModulateSpeed="true" MinSpeed="1.4,1.4,0" MinAnalogInputMag="0.2" MaxAnalogInputMag="0.8" MovementAccuracyBonus="-12.0"/>
										<Conditional Name="Prone" ForwardInput="ForwardNav" BackwardInput="moveback" LeftInput="moveleft" RightInput="moveright" ForcedMotion="false" UseInitialCameraRotation="false" AllowVerticalMotion="false" Speed="1.25,1.25,0" Accel="20,20,20" Decel="20,20,20" InheritVelocity="false" BackwardsMultiplier="0.55" AnalogInputOverride="LeftAnalog" UseAnalogInputToModulateSpeed="true" MinSpeed="1.4,1.4,0" MinAnalogInputMag="0.2" MaxAnalogInputMag="0.8" MovementAccuracyBonus="-5.0"/>
										<Conditional Name="ServerValidatedCanSprint" ForwardInput="ForwardNav" BackwardInput="moveback" LeftInput="moveleft" RightInput="moveright" ForcedMotion="false" UseInitialCameraRotation="false" AllowVerticalMotion="false" Speed="5.5,5.5,0" Accel="20,20,20" Decel="20,20,20" InheritVelocity="false" BackwardsMultiplier="0.55" AnalogInputOverride="LeftAnalog" UseAnalogInputToModulateSpeed="true" MinSpeed="1.4,1.4,0" MinAnalogInputMag="0.2" MaxAnalogInputMag="0.8" MovementAccuracyBonus="-20.0"/>
									</CameraRelativeMotion>
								</ElseActivity>
							</IfThenElse>
							<IfThenElse ResetWhenReinstalled="true" Comment="Set navigation tags">
								<IfCondition Comment="Set ForwardNav tag if pushing forward">
									<Or>
										<IfInput InputName="ForwardNav"/>
										<IfAnalogInput AnalogInput="LeftAnalog" MinInputMagnitude="0.10" MaxInputMagnitude="1.00" RequireTargetAngleMatch="true" UseCharacterRelativeAngle="false" TargetAngle="0" TargetAngleTolerance="30.0"/>
									</Or>
								</IfCondition>
								<ThenActivity>
									<SetMannequinTag ResetWhenReinstalled="true">
										<Default TagName="ForwardNav" OnEnter="Set" OnExit="NoEffect"/>
									</SetMannequinTag>
								</ThenActivity>
								<ElseActivity>
									<IfThenElse ResetWhenReinstalled="true">
										<IfCondition Comment="Set BackNav tag  if pushing back">
											<Or>
												<IfInput InputName="moveback"/>
												<IfAnalogInput AnalogInput="LeftAnalog" MinInputMagnitude="0.10" MaxInputMagnitude="1.00" RequireTargetAngleMatch="true" UseCharacterRelativeAngle="false" TargetAngle="180.0" TargetAngleTolerance="70.0"/>
											</Or>
										</IfCondition>
										<ThenActivity>
											<SetMannequinTag ResetWhenReinstalled="true">
												<Default TagName="BackNav" OnEnter="Set" OnExit="NoEffect"/>
											</SetMannequinTag>
										</ThenActivity>
										<ElseActivity>
											<IfThenElse ResetWhenReinstalled="true">
												<IfCondition Comment="Set LeftNav tag if pushing left">
													<Or>
														<IfInput InputName="moveleft"/>
														<IfAnalogInput AnalogInput="LeftAnalog" MinInputMagnitude="0.10" MaxInputMagnitude="1.00" RequireTargetAngleMatch="true" UseCharacterRelativeAngle="false" TargetAngle="270.0" TargetAngleTolerance="70.0"/>
													</Or>
												</IfCondition>
												<ThenActivity>
													<SetMannequinTag ResetWhenReinstalled="true">
														<Default TagName="LeftNav" OnEnter="Set" OnExit="NoEffect"/>
													</SetMannequinTag>
												</ThenActivity>
												<ElseActivity Comment="Set RightNav tag if all else fails">
													<SetMannequinTag ResetWhenReinstalled="true">
														<Default TagName="RightNav" OnEnter="Set" OnExit="NoEffect"/>
													</SetMannequinTag>
												</ElseActivity>
											</IfThenElse>
										</ElseActivity>
									</IfThenElse>
								</ElseActivity>
							</IfThenElse>
						</Group>
					</ThenActivity>
					<ElseActivity>
						<Group>
							<InputRelativeFacing>
								<Default TurnRate="0.50" UseExponentialTurnRate="true" RepeatInitialInput="false" UseClosestTarget="false" ForwardInput="ForwardNav" BackwardInput="moveback" LeftInput="moveleft" RightInput="moveright" AnalogInputOverride="LeftAnalog"/>
								<Conditional Name="Prone" TurnRate="0.20" UseExponentialTurnRate="true" RepeatInitialInput="false" UseClosestTarget="false" ForwardInput="ForwardNav" BackwardInput="moveback" LeftInput="moveleft" RightInput="moveright" AnalogInputOverride="LeftAnalog"/>
								<Conditional Name="180_Input" TurnRate="0.0" UseExponentialTurnRate="true" RepeatInitialInput="false" UseClosestTarget="false" ForwardInput="" BackwardInput="" LeftInput="" RightInput="" AnalogInputOverride="None"/>
							</InputRelativeFacing>
							<CharacterRelativeMovement>
								<Default Forced="true" AlignVelocity="true" Speed="0,5,0" Accel="20,20,0" Decel="20,20,0" AnalogInputOverride="LeftAnalog" UseAnalogInputToModulateSpeed="false"/>
								<Conditional Name="Wade+Crouch" Forced="true" AlignVelocity="true" Speed="0,1.75,0" Accel="20,20,0" Decel="20,20,0" AnalogInputOverride="LeftAnalog" UseAnalogInputToModulateSpeed="false"/>
								<Conditional Name="Wade" Forced="true" AlignVelocity="true" Speed="0,2.5,0" Accel="20,20,0" Decel="20,20,0" AnalogInputOverride="LeftAnalog" UseAnalogInputToModulateSpeed="false"/>
								<Conditional Name="Stand+Armed" Forced="true" AlignVelocity="true" Speed="0,5.5,0" Accel="10,10,0" Decel="20,20,0" AnalogInputOverride="LeftAnalog" UseAnalogInputToModulateSpeed="false"/>
								<Conditional Name="Crouch" Forced="true" AlignVelocity="true" Speed="0,2.5,0" Accel="10,10,0" Decel="20,20,0" AnalogInputOverride="LeftAnalog" UseAnalogInputToModulateSpeed="false"/>
								<Conditional Name="Prone" Forced="true" AlignVelocity="true" Speed="0,1.25,0" Accel="20,20,0" Decel="20,20,0" AnalogInputOverride="LeftAnalog" UseAnalogInputToModulateSpeed="false"/>
							</CharacterRelativeMovement>
						</Group>
					</ElseActivity>
				</IfThenElse>
				<ClearMannequinTagGroup ResetWhenReinstalled="true">
					<Default TagGroupName="NavDirection" OnEnter="NoEffect" OnExit="Clear"/>
				</ClearMannequinTagGroup>
			</Activities>
		</Action>
		<Action Name="Sprint_RightToLeft_180" FragmentName="Sprint_RightToLeft_180">
			<ActivationCondition>
				<And>
					<IfMannequinTag TagName="Sprint"/>
					<IfVelocityDirection CompareToCharacterFacing="true" Angle="90" AngleTolerance="22.5"/>
					<MoveSpeed Comparison="GreaterThanOrEqual" Value="1"/>
					<IfExternalCondition ConditionName="Input_Left"/>
					<Not>
						<IfFragmentPlaying UseHostActionFragment="false" FragmentName="Ability_Fire_Staff_FlameThrower"/>
					</Not>
					<Not>
						<IfFragmentPlaying UseHostActionFragment="false" FragmentName="Ability_IceMagic_WindChill_Aim"/>
					</Not>
					<Not>
						<IfFragmentPlaying UseHostActionFragment="true"/>
					</Not>
					<Not>
						<IfMannequinTag TagName="Prone"/>
					</Not>
					<Not>
						<IfMannequinTag TagName="Crouch"/>
					</Not>
					<IfPlayerSetting PlayerSettingName="SprintStrafeMovement"/>
					<Or>
						<IfPlayerSetting PlayerSettingName="LockedStrafeMovement"/>
