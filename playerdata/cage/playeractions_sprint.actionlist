														<IfInput InputName="ForwardNav"/>
														<IfAnalogInput AnalogInput="LeftAnalog" MinInputMagnitude="0.10" MaxInputMagnitude="1.00" RequireTargetAngleMatch="true" UseCharacterRelativeAngle="false" TargetAngle="0" TargetAngleTolerance="30.0"/>
													</Or>
												</IfCondition>
												<ThenActivity>
													<SetMannequinTag ResetWhenReinstalled="true">
														<Default TagName="ForwardNav" OnEnter="Set" OnExit="NoEffect"/>
													</SetMannequinTag>
												</ThenActivity>
												<ElseActivity Comment="Set BackNav tag if all else fails">
													<SetMannequinTag ResetWhenReinstalled="true">
														<Default TagName="BackNav" OnEnter="Set" OnExit="NoEffect"/>
													</SetMannequinTag>
												</ElseActivity>
											</IfThenElse>
										</ElseActivity>
									</IfThenElse>
								</ElseActivity>
							</IfThenElse>
						</Group>
					</ElseActivity>
				</IfThenElse>
				<ClearMannequinTagGroup ResetWhenReinstalled="true">
					<Default TagGroupName="NavDirection" OnEnter="NoEffect" OnExit="Clear"/>
				</ClearMannequinTagGroup>
				<SendUIEvent>
					<Default OnEnter="Send" EnterEventName="Cage.IsSprinting" OnExit="Send" ExitEventName="Cage.IsSprinting"/>
				</SendUIEvent>
			</Activities>
		</Action>
		<Action Name="Sprint_180_Turn" FragmentName="Sprint_180_Turn">
			<ActivationCondition>
				<And>
					<IfMannequinTag TagName="Sprint"/>
					<IfExternalCondition ConditionName="180_Input"/>
					<Not>
						<IfFragmentPlaying UseHostActionFragment="true"/>
					</Not>
					<Not>
						<IfMannequinTag TagName="Prone"/>
					</Not>
					<Or>
						<Not>
							<And>
								<IfPlayerSetting PlayerSettingName="SprintStrafeMovement"/>
								<Or>
									<IfPlayerSetting PlayerSettingName="LockedStrafeMovement"/>
									<IfCameraLockActive/>
									<IfExternalCondition ConditionName="CombatNav"/>
								</Or>
							</And>
						</Not>
					</Or>
				</And>
			</ActivationCondition>
			<DeactivationCondition>
				<Or>
					<IfMannequinMarker MarkerName="ForceStop"/>
					<IfAliasStatus GridAlias="Build" Status="Installed"/>
				</Or>
			</DeactivationCondition>
			<Activities>
				<AnimationDrivenMotion>
					<Default MotionMultiplier="1.00" RotationMultiplier="1.00" Decel="10,10,20" DisableGravity="false" InheritVelocity="true" InheritVelocityMaxSpeed="6"/>
				</AnimationDrivenMotion>
				<SetMannequinTag ResetWhenReinstalled="false">
					<Default TagName="Sprint" OnEnter="Set" OnExit="Unset"/>
				</SetMannequinTag>
			</Activities>
		</Action>
	</Actions>
	<Conditions>
		<And Name="Stand+Unarmed">
			<IfMannequinTag TagName="Stand"/>
			<IfMannequinTag TagName="Unarmed"/>
		</And>
		<And Name="Crouch+Unarmed">
			<IfMannequinTag TagName="Crouch"/>
			<IfMannequinTag TagName="Unarmed"/>
		</And>
		<And Name="Prone+Unarmed">
			<IfMannequinTag TagName="Prone"/>
			<IfMannequinTag TagName="Unarmed"/>
		</And>
		<IfMannequinTag Name="TravelMode_Stamina" TagName="Sprint_TravelMode"/>
		<And Name="TravelMode_Speed">
			<IfMannequinTag TagName="Sprint_TravelMode"/>
			<IfMannequinTag TagName="Unarmed"/>
			<IfCanSprint UseServer="True"/>
		</And>
		<And Name="180_Input">
			<Or Comment="Analog 180">
				<IfAnalogInput AnalogInput="LeftAnalog" MinInputMagnitude="0.20" MaxInputMagnitude="1.00" RequireTargetAngleMatch="true" UseCharacterRelativeAngle="true" TargetAngle="180" TargetAngleTolerance="45"/>
				<IfAnalogInput AnalogInput="None" DigitalForwardInput="ForwardNav" DigitalBackInput="moveback" DigitalLeftInput="moveleft" DigitalRightInput="moveright" MinInputMagnitude="0.20" MaxInputMagnitude="1.00" RequireTargetAngleMatch="true" UseCharacterRelativeAngle="true" TargetAngle="180" TargetAngleTolerance="45"/>
			</Or>
			<Or Comment="Stopping or going full speed">
				<IfMannequinMarker MarkerName="PreventNav"/>
				<And>
					<IfMannequinTag TagName="Stand"/>
					<MoveSpeed Comparison="GreaterThanOrEqual" Value="4.5"/>
				</And>
				<And>
					<IfMannequinTag TagName="Crouch"/>
					<MoveSpeed Comparison="GreaterThanOrEqual" Value="3.0"/>
				</And>
			</Or>
		</And>
		<IfCanSprint Name="ServerValidatedCanSprint" UseServer="True"/>
		<And Name="Wade+Crouch">
			<IfMannequinTag TagName="Crouch"/>
			<Or>
				<IfWaterDepth Comparison="GreaterThanOrEqual" WaterDepth="Shallow"/>
			</Or>
			<IfPlayerSetting PlayerSettingName="Swimming"/>
		</And>
		<And Name="Stand+Armed">
			<IfCanSprint UseServer="True"/>
			<IfMannequinTag TagName="Stand"/>
			<Or>
				<IfMannequinTag TagName="1H_Melee"/>
				<IfMannequinTag TagName="2H_Melee"/>
				<IfMannequinTag TagName="1H_Ranged"/>
				<IfMannequinTag TagName="2H_Ranged"/>
				<IfMannequinTag TagName="Bow"/>
			</Or>
		</And>
		<Or Name="Forward_180_Input">
			<IfAnalogInput AnalogInput="LeftAnalog" MinInputMagnitude="0.20" MaxInputMagnitude="1.00" RequireTargetAngleMatch="true" UseCharacterRelativeAngle="false" TargetAngle="0" TargetAngleTolerance="91"/>
			<IfAnalogInput AnalogInput="None" DigitalForwardInput="ForwardNav" DigitalBackInput="moveback" DigitalLeftInput="moveleft" DigitalRightInput="moveright" MinInputMagnitude="0.20" MaxInputMagnitude="1.00" RequireTargetAngleMatch="true" UseCharacterRelativeAngle="false" TargetAngle="0" TargetAngleTolerance="91"/>
		</Or>
		<Or Name="Input_Left">
			<IfAnalogInput AnalogInput="LeftAnalog" MinInputMagnitude="0.20" MaxInputMagnitude="1.00" RequireTargetAngleMatch="true" UseCharacterRelativeAngle="false" TargetAngle="270" TargetAngleTolerance="22.5"/>
			<IfAnalogInput AnalogInput="None" DigitalForwardInput="ForwardNav" DigitalBackInput="moveback" DigitalLeftInput="moveleft" DigitalRightInput="moveright" MinInputMagnitude="0.20" MaxInputMagnitude="1.00" RequireTargetAngleMatch="true" UseCharacterRelativeAngle="false" TargetAngle="270" TargetAngleTolerance="22.5"/>
		</Or>
		<Or Name="Input_Right">
			<IfAnalogInput AnalogInput="LeftAnalog" MinInputMagnitude="0.20" MaxInputMagnitude="1.00" RequireTargetAngleMatch="true" UseCharacterRelativeAngle="false" TargetAngle="90" TargetAngleTolerance="22.5"/>
			<IfAnalogInput AnalogInput="None" DigitalForwardInput="ForwardNav" DigitalBackInput="moveback" DigitalLeftInput="moveleft" DigitalRightInput="moveright" MinInputMagnitude="0.20" MaxInputMagnitude="1.00" RequireTargetAngleMatch="true" UseCharacterRelativeAngle="false" TargetAngle="90" TargetAngleTolerance="22.5"/>
		</Or>
		<Or Name="Input_Forward">
			<IfAnalogInput AnalogInput="LeftAnalog" MinInputMagnitude="0.20" MaxInputMagnitude="1.00" RequireTargetAngleMatch="true" UseCharacterRelativeAngle="false" TargetAngle="0" TargetAngleTolerance="22.5"/>
			<IfAnalogInput AnalogInput="None" DigitalForwardInput="ForwardNav" DigitalBackInput="moveback" DigitalLeftInput="moveleft" DigitalRightInput="moveright" MinInputMagnitude="0.20" MaxInputMagnitude="1.00" RequireTargetAngleMatch="true" UseCharacterRelativeAngle="false" TargetAngle="0" TargetAngleTolerance="22.5"/>
		</Or>
		<Or Name="Input_Backward">
			<IfAnalogInput AnalogInput="LeftAnalog" MinInputMagnitude="0.20" MaxInputMagnitude="1.00" RequireTargetAngleMatch="true" UseCharacterRelativeAngle="false" TargetAngle="180" TargetAngleTolerance="22.5"/>
			<IfAnalogInput AnalogInput="None" DigitalForwardInput="ForwardNav" DigitalBackInput="moveback" DigitalLeftInput="moveleft" DigitalRightInput="moveright" MinInputMagnitude="0.20" MaxInputMagnitude="1.00" RequireTargetAngleMatch="true" UseCharacterRelativeAngle="false" TargetAngle="180" TargetAngleTolerance="22.5"/>
		</Or>
		<Or Name="Input_ForwardLeft">
			<IfAnalogInput AnalogInput="LeftAnalog" MinInputMagnitude="0.20" MaxInputMagnitude="1.00" RequireTargetAngleMatch="true" UseCharacterRelativeAngle="false" TargetAngle="315" TargetAngleTolerance="22.5"/>
			<IfAnalogInput AnalogInput="None" DigitalForwardInput="ForwardNav" DigitalBackInput="moveback" DigitalLeftInput="moveleft" DigitalRightInput="moveright" MinInputMagnitude="0.20" MaxInputMagnitude="1.00" RequireTargetAngleMatch="true" UseCharacterRelativeAngle="false" TargetAngle="315" TargetAngleTolerance="22.5"/>
		</Or>
		<Or Name="Input_ForwardRight">
			<IfAnalogInput AnalogInput="LeftAnalog" MinInputMagnitude="0.20" MaxInputMagnitude="1.00" RequireTargetAngleMatch="true" UseCharacterRelativeAngle="false" TargetAngle="45" TargetAngleTolerance="22.5"/>
			<IfAnalogInput AnalogInput="None" DigitalForwardInput="ForwardNav" DigitalBackInput="moveback" DigitalLeftInput="moveleft" DigitalRightInput="moveright" MinInputMagnitude="0.20" MaxInputMagnitude="1.00" RequireTargetAngleMatch="true" UseCharacterRelativeAngle="false" TargetAngle="45" TargetAngleTolerance="22.5"/>
		</Or>
		<Or Name="Input_BackwardLeft">
			<IfAnalogInput AnalogInput="LeftAnalog" MinInputMagnitude="0.20" MaxInputMagnitude="1.00" RequireTargetAngleMatch="true" UseCharacterRelativeAngle="false" TargetAngle="225" TargetAngleTolerance="22.5"/>
			<IfAnalogInput AnalogInput="None" DigitalForwardInput="ForwardNav" DigitalBackInput="moveback" DigitalLeftInput="moveleft" DigitalRightInput="moveright" MinInputMagnitude="0.20" MaxInputMagnitude="1.00" RequireTargetAngleMatch="true" UseCharacterRelativeAngle="false" TargetAngle="225" TargetAngleTolerance="22.5"/>
		</Or>
		<Or Name="Input_BackwardRight">
			<IfAnalogInput AnalogInput="LeftAnalog" MinInputMagnitude="0.20" MaxInputMagnitude="1.00" RequireTargetAngleMatch="true" UseCharacterRelativeAngle="false" TargetAngle="135" TargetAngleTolerance="22.5"/>
			<IfAnalogInput AnalogInput="None" DigitalForwardInput="ForwardNav" DigitalBackInput="moveback" DigitalLeftInput="moveleft" DigitalRightInput="moveright" MinInputMagnitude="0.20" MaxInputMagnitude="1.00" RequireTargetAngleMatch="true" UseCharacterRelativeAngle="false" TargetAngle="135" TargetAngleTolerance="22.5"/>
		</Or>
		<Or Name="Any_Nav_Input">
			<IfAnalogInput AnalogInput="LeftAnalog" MinInputMagnitude="0.20" MaxInputMagnitude="1.00"/>
			<IfAnalogInput AnalogInput="None" DigitalForwardInput="ForwardNav" DigitalBackInput="moveback" DigitalLeftInput="moveleft" DigitalRightInput="moveright" MinInputMagnitude="0.20" MaxInputMagnitude="1.00"/>
		</Or>
		<Or Name="SprintDisabled">
			<Not>
				<IfCanSprint/>
			</Not>
			<IfMannequinTag TagName="Aim"/>
			<IfMannequinTag TagName="Shoot"/>
			<IfMannequinTag TagName="Throw"/>
			<IfMannequinTag TagName="Blocking"/>
			<IfMannequinMarker MarkerName="ForceWalk"/>
			<IfMannequinMarker MarkerName="ForceStop"/>
			<IfMannequinMarker MarkerName="PreventNav"/>
			<ActionConditionEquipLoadCategory Value="Overburdened"/>
			<IfEncumbered/>
			<IfInput InputName="walk"/>
		</Or>
	</Conditions>
</ActionList>

