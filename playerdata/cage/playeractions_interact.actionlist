													</SheatheWeapon>
													<SetMannequinTagFromSource ResetWhenReinstalled="false" Comment="Unset Current Main Hand Weapon Tags">
														<Default TagSource="PaperdollSlot" SourceSlot="ACTIVE_OFF_HAND_WEAPON" OnEnter="Unset" OnExit="NoEffect"/>
													</SetMannequinTagFromSource>
													<SetMannequinTag ResetWhenReinstalled="false">
														<Default TagName="Unarmed_OH" OnEnter="Set" OnExit="NoEffect"/>
													</SetMannequinTag>
												</Group>
											</ThenActivity>
										</IfThenElse>
									</Group>
								</ElseActivity>
							</IfThenElse>
						</Group>
					</ThenActivity>
				</IfThenElse>
				<SetMannequinTag ResetWhenReinstalled="false">
					<Default TagName="KeepWeaponSheathed" OnEnter="NoEffect" OnExit="Unset"/>
				</SetMannequinTag>
				<SetMannequinTag ResetWhenReinstalled="false">
					<Default TagName="AttackAction" OnEnter="Set" OnExit="Unset"/>
				</SetMannequinTag>
				<SetMannequinTag ResetWhenReinstalled="false">
					<Default TagName="DisableAim" OnEnter="Set" OnExit="Unset"/>
				</SetMannequinTag>
				<ModifyStatusEffects>
					<Default>
						<EffectsToRemoveOnExit>
							<EffectId EffectId="Dungeon_AdmiralCannonBall"/>
							<EffectId EffectId="Sandworm_CannonBall"/>
						</EffectsToRemoveOnExit>
					</Default>
				</ModifyStatusEffects>
			</Activities>
		</Action>
	</Actions>
	<Conditions>
		<IfInput Name="Continue_Crafting" InputName="Start_Crafting"/>
		<Not Name="Stop_ApplyingResources">
			<IfInput InputName="Apply_Resources_To_Structure" Comment="IfShouldBuild Virtual Input"/>
		</Not>
		<True Name="LoopCrafting"/>
		<IfInteractionSuccess Name="UsingTurret" InteractionId="UseTurret"/>
		<Or Name="ExitInteraction" Comment="Exit interactable interactions with specific inputs">
			<IfInput InputName="moveforward_onpress"/>
			<IfInput InputName="moveback_onpress"/>
			<IfInput InputName="moveleft_onpress"/>
			<IfInput InputName="moveright_onpress"/>
			<IfInput InputName="ui_interact"/>
		</Or>
		<And Name="MassRezUpgrade">
			<IfAbilityActive AbilityName="ProtectorMassRez"/>
		</And>
		<And Name="MassRezUpgrade_Ready">
			<IfInteractionSuccess InteractionId="RevivePlayer"/>
			<IfAbilityActive AbilityName="ProtectorMassRez"/>
		</And>
		<And Name="NoMassRezUpgrade">
			<Or>
				<IfHasStatusEffect StatusEffect="MassRezTimer"/>
				<Not>
					<IfAbilityActive AbilityName="ProtectorMassRez"/>
				</Not>
			</Or>
		</And>
		<And Name="MassRezOutro">
			<IfInteractionSuccess InteractionId="RevivePlayer"/>
		</And>
		<Or Name="AzothStaffBarrier_Cancel">
			<IfInput InputName="aim_draw_press"/>
			<IfInput InputName="toggleInventoryWindow"/>
			<IfInput InputName="Player_LoadoutOpen"/>
			<IfInput InputName="Player_InteractingStorage"/>
		</Or>
	</Conditions>
</ActionList>

