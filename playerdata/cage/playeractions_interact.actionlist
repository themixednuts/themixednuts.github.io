<ActionList>
	<Actions>
		<Action Name="CommittedInteraction" FragmentName="Crafting_Station_Interaction">
			<ActivationCondition>
				<And>
					<CanInteract/>
					<IsRequestingInteraction>
						<Interactions>
							<Interaction InteractionId="OpenTradingPost"/>
							<Interaction InteractionId="OpenPrefabRefinery"/>
							<Interaction InteractionId="OpenGenerator"/>
							<Interaction InteractionId="OpenTerritoryMarker"/>
							<Interaction InteractionId="OpenStorage"/>
							<Interaction InteractionId="OpenInteractScreen"/>
							<Interaction InteractionId="OpenStartClaimScreen"/>
							<Interaction InteractionId="OpenAttachment"/>
							<Interaction InteractionId="OpenPaint"/>
							<Interaction InteractionId="OpenLootStorage"/>
							<Interaction InteractionId="OpenRead"/>
							<Interaction InteractionId="OpenConversation"/>
							<Interaction InteractionId="OpenObjectiveProvider"/>
							<Interaction InteractionId="OpenArmory"/>
							<Interaction InteractionId="UseTurret"/>
							<Interaction InteractionId="OpenTerritoryInterface"/>
							<Interaction InteractionId="OpenTerritoryProgression"/>
							<Interaction InteractionId="HousingPurchase"/>
							<Interaction InteractionId="HousingEnter"/>
							<Interaction InteractionId="OpenRaidSetup"/>
							<Interaction InteractionId="OpenTurretSelection"/>
							<Interaction InteractionId="OpenInnScreen"/>
							<Interaction InteractionId="OpenArenaMenu"/>
							<Interaction InteractionId="InteractWithHousingItem"/>
							<Interaction InteractionId="OpenDungeonMenu"/>
							<Interaction InteractionId="OpenSummoningSelection"/>
							<Interaction InteractionId="OpenCampSkins"/>
							<Interaction InteractionId="OpenEventShop"/>
							<Interaction InteractionId="PlayCinematicVideo"/>
						</Interactions>
					</IsRequestingInteraction>
					<Not>
						<IsRequestingInteraction>
							<Interactions>
								<Interaction InteractionId="GatherResource"/>
								<Interaction InteractionId="GatherWaterResource"/>
							</Interactions>
						</IsRequestingInteraction>
					</Not>
					<Not>
						<IfInput InputName="Apply_Resources_To_Structure" Comment="IfShouldBuild Virtual Input"/>
					</Not>
					<Not>
						<IfInput InputName="Start_Crafting" Comment="IfShouldCraft Virtual Input"/>
					</Not>
					<Not>
						<IfInput InputName="Player_InteractingStorage" Comment="IfIsInteractingWithStorage Virtual Input"/>
					</Not>
				</And>
			</ActivationCondition>
			<DeactivationCondition>
				<Or>
					<And>
						<IsRequestingInteraction>
							<Interactions>
								<Interaction InteractionId="UseTurret"/>
							</Interactions>
						</IsRequestingInteraction>
						<Or>
							<IfInput InputName="ui_interact_press_only"/>
							<IfInput InputName="ui_cancel"/>
							<IfInput InputName="Stop_Committed_Interaction"/>
						</Or>
						<IfMannequinMarker MarkerName="AllowTurretExit"/>
					</And>
					<Not>
						<IsRequestingInteraction>
							<Interactions>
								<Interaction InteractionId="OpenTradingPost"/>
								<Interaction InteractionId="OpenPrefabRefinery"/>
								<Interaction InteractionId="OpenGenerator"/>
								<Interaction InteractionId="OpenTerritoryMarker"/>
								<Interaction InteractionId="OpenStorage"/>
								<Interaction InteractionId="OpenInteractScreen"/>
								<Interaction InteractionId="OpenStartClaimScreen"/>
								<Interaction InteractionId="OpenAttachment"/>
								<Interaction InteractionId="OpenPaint"/>
								<Interaction InteractionId="OpenLootStorage"/>
								<Interaction InteractionId="OpenRead"/>
								<Interaction InteractionId="OpenConversation"/>
								<Interaction InteractionId="UseTurret"/>
								<Interaction InteractionId="OpenObjectiveProvider"/>
								<Interaction InteractionId="OpenArmory"/>
								<Interaction InteractionId="OpenTerritoryInterface"/>
								<Interaction InteractionId="OpenTerritoryProgression"/>
								<Interaction InteractionId="HousingPurchase"/>
								<Interaction InteractionId="HousingEnter"/>
								<Interaction InteractionId="OpenRaidSetup"/>
								<Interaction InteractionId="OpenTurretSelection"/>
								<Interaction InteractionId="OpenInnScreen"/>
								<Interaction InteractionId="OpenArenaMenu"/>
								<Interaction InteractionId="InteractWithHousingItem"/>
								<Interaction InteractionId="OpenDungeonMenu"/>
								<Interaction InteractionId="OpenSummoningSelection"/>
								<Interaction InteractionId="OpenEventShop"/>
								<Interaction InteractionId="PlayCinematicVideo"/>
							</Interactions>
						</IsRequestingInteraction>
					</Not>
				</Or>
			</DeactivationCondition>
			<Activities>
				<SetMannequinTag ResetWhenReinstalled="true">
					<Default TagName="Blocking" OnEnter="UnSet" OnExit="NoEffect"/>
				</SetMannequinTag>
				<SetMannequinTag ResetWhenReinstalled="true">
					<Default TagName="AimDownSights" OnEnter="UnSet" OnExit="NoEffect"/>
				</SetMannequinTag>
				<SetMannequinTag ResetWhenReinstalled="true">
					<Default TagName="Aim" OnEnter="UnSet" OnExit="NoEffect"/>
				</SetMannequinTag>
				<ExecuteInteraction>
					<Default/>
					<Conditional Name="UsingTurret" ClearInteractionOnExit="true" ClearInteractionOnUnavailable="true"/>
				</ExecuteInteraction>
				<ModifyStatusEffects>
					<Default>
						<EffectsToRemoveOnEnter>
							<EffectId EffectId="PotionHaste"/>
						</EffectsToRemoveOnEnter>
					</Default>
				</ModifyStatusEffects>
				<IfThenElse ResetWhenReinstalled="false" Comment="Use Idle fragment in conversation">
					<IfCondition>
						<IsRequestingInteraction>
							<Interactions>
								<Interaction InteractionId="OpenConversation"/>
							</Interactions>
						</IsRequestingInteraction>
					</IfCondition>
					<ThenActivity>
						<ChangeActionFragment ResetWhenReinstalled="false">
							<Default UseDefaultFragment="false" FragmentName="Idle"/>
						</ChangeActionFragment>
					</ThenActivity>
				</IfThenElse>
				<InteractionAlignment>
					<Default/>
					<Conditional Name="UsingTurret" IsDynamic="true"/>
				</InteractionAlignment>
				<IfThenElse ResetWhenReinstalled="false" Comment="Instant Sheathe-All + AutoUnsheathe Logic v2.2">
					<IfCondition>
						<Or>
							<IfItemInSlot SlotToCheckAlias="ACTIVE_MAIN_HAND_WEAPON" RequireSlotValid="true" RequireSlotActive="false"/>
							<IfItemInSlot SlotToCheckAlias="ACTIVE_OFF_HAND_WEAPON" RequireSlotValid="true" RequireSlotActive="false"/>
						</Or>
					</IfCondition>
					<ThenActivity>
						<Group>
							<IfThenElse ResetWhenReinstalled="false" Comment="Sheathe Logic">
								<IfCondition>
									<And Comment="Weapon slots empty or already sheathed">
										<Or>
											<Not>
												<IfItemInSlot SlotToCheckAlias="ACTIVE_MAIN_HAND_WEAPON" RequireSlotValid="true" RequireSlotActive="false"/>
											</Not>
											<IfItemSheathed SlotToCheckAlias="ACTIVE_MAIN_HAND_WEAPON"/>
										</Or>
										<Or>
											<Not>
												<IfItemInSlot SlotToCheckAlias="ACTIVE_OFF_HAND_WEAPON" RequireSlotValid="true" RequireSlotActive="false"/>
											</Not>
											<IfItemSheathed SlotToCheckAlias="ACTIVE_OFF_HAND_WEAPON"/>
										</Or>
										<Not>
											<And>
												<Not>
													<IfItemInSlot SlotToCheckAlias="ACTIVE_MAIN_HAND_WEAPON" RequireSlotValid="true" RequireSlotActive="false"/>
												</Not>
												<Not>
													<IfItemInSlot SlotToCheckAlias="ACTIVE_OFF_HAND_WEAPON" RequireSlotValid="true" RequireSlotActive="false"/>
												</Not>
											</And>
										</Not>
										<Not>
											<IfMannequinTag TagName="AutoUnsheathe"/>
										</Not>
									</And>
								</IfCondition>
								<ThenActivity>
									<SetMannequinTag ResetWhenReinstalled="false">
										<Default TagName="KeepWeaponSheathed" OnEnter="Set" OnExit="NoEffect"/>
									</SetMannequinTag>
								</ThenActivity>
								<ElseActivity>
									<Group Comment="Sheathe All Weapons">
										<IfThenElse ResetWhenReinstalled="false" Comment="Sheathe Current Main Hand Weapon">
											<IfCondition>
												<And>
													<IfItemInSlot SlotToCheckAlias="ACTIVE_MAIN_HAND_WEAPON" RequireSlotValid="true" RequireSlotActive="false"/>
													<Not>
														<IfItemSheathed SlotToCheckAlias="ACTIVE_MAIN_HAND_WEAPON"/>
													</Not>
												</And>
											</IfCondition>
											<ThenActivity>
												<Group>
													<SheatheWeapon ResetWhenReinstalled="false">
														<Default SheatheOption="Sheathe" SlotToSheathe="ACTIVE_MAIN_HAND_WEAPON" OnEnter="Set" OnExit="NoEffect"/>
													</SheatheWeapon>
													<SetMannequinTagFromSource ResetWhenReinstalled="false" Comment="Unset Current Main Hand Weapon Tags">
														<Default TagSource="PaperdollSlot" SourceSlot="ACTIVE_MAIN_HAND_WEAPON" OnEnter="Unset" OnExit="NoEffect"/>
													</SetMannequinTagFromSource>
													<ClearMannequinTagGroup ResetWhenReinstalled="false">
														<Default TagGroupName="Reloading" OnEnter="Clear" OnExit="NoEffect"/>
													</ClearMannequinTagGroup>
													<SetMannequinTag ResetWhenReinstalled="false">
														<Default TagName="Unarmed" OnEnter="Set" OnExit="NoEffect"/>
													</SetMannequinTag>
													<SetMannequinTag ResetWhenReinstalled="false">
														<Default TagName="AutoUnsheathe" OnEnter="Set" OnExit="NoEffect"/>
													</SetMannequinTag>
												</Group>
											</ThenActivity>
											<ElseActivity Comment="Clear Weapon Tag">
												<SetMannequinTagFromSource ResetWhenReinstalled="false" Comment="Unset Current Main Hand Weapon Tags">
													<Default TagSource="PaperdollSlot" SourceSlot="ACTIVE_MAIN_HAND_WEAPON" OnEnter="Unset" OnExit="NoEffect"/>
												</SetMannequinTagFromSource>
											</ElseActivity>
										</IfThenElse>
										<IfThenElse ResetWhenReinstalled="false" Comment="Sheathe Offhand-Weapon">
											<IfCondition>
												<And>
													<IfItemInSlot SlotToCheckAlias="ACTIVE_OFF_HAND_WEAPON" RequireSlotValid="true" RequireSlotActive="false"/>
													<Not>
														<IfItemSheathed SlotToCheckAlias="ACTIVE_OFF_HAND_WEAPON"/>
													</Not>
												</And>
											</IfCondition>
											<ThenActivity>
												<Group>
													<SheatheWeapon ResetWhenReinstalled="false">
														<Default SheatheOption="Sheathe" SlotToSheathe="ACTIVE_OFF_HAND_WEAPON" OnEnter="Set" OnExit="NoEffect"/>
													</SheatheWeapon>
													<SetMannequinTagFromSource ResetWhenReinstalled="false" Comment="Unset Current Main Hand Weapon Tags">
														<Default TagSource="PaperdollSlot" SourceSlot="ACTIVE_OFF_HAND_WEAPON" OnEnter="Unset" OnExit="NoEffect"/>
													</SetMannequinTagFromSource>
													<SetMannequinTag ResetWhenReinstalled="false">
														<Default TagName="Unarmed_OH" OnEnter="Set" OnExit="NoEffect"/>
													</SetMannequinTag>
												</Group>
											</ThenActivity>
										</IfThenElse>
									</Group>
								</ElseActivity>
							</IfThenElse>
						</Group>
					</ThenActivity>
				</IfThenElse>
				<SetMannequinTag ResetWhenReinstalled="false">
					<Default TagName="KeepWeaponSheathed" OnEnter="NoEffect" OnExit="Unset"/>
				</SetMannequinTag>
				<SetMannequinTag ResetWhenReinstalled="false">
					<Default TagName="DisableAim" OnEnter="Set" OnExit="Unset" DelayTimer="0.0"/>
				</SetMannequinTag>
				<SetMannequinTagFromSource ResetWhenReinstalled="false">
					<Default TagSource="Interaction" SourceSlot="" OnEnter="Set" OnExit="NoEffect"/>
				</SetMannequinTagFromSource>
				<IfThenElse ResetWhenReinstalled="false" Comment="Set 'Craft' when crafting">
					<IfCondition>
						<And>
							<IfInput InputName="Start_Crafting" Comment="IfShouldCraft Virtual Input"/>
							<Not>
								<IfInput InputName="Player_InteractingStorage" Comment="IfIsInteractingWithStorage Virtual Input"/>
							</Not>
						</And>
					</IfCondition>
					<ThenActivity>
						<Group>
							<Craft/>
							<SetMannequinTagFromSource ResetWhenReinstalled="false">
								<Default TagSource="InteractingCraftingStation" SourceSlot="" OnEnter="Set" OnExit="NoEffect"/>
							</SetMannequinTagFromSource>
						</Group>
					</ThenActivity>
				</IfThenElse>
				<IfThenElse ResetWhenReinstalled="false" Comment="Set posture tag as needed">
					<IfCondition>
						<IfMannequinMarker MarkerName="Set_Crouch"/>
					</IfCondition>
					<ThenActivity>
						<SetMannequinTag ResetWhenReinstalled="false">
							<Default TagName="Crouch" OnEnter="Set" OnExit="NoEffect" DelayTimer="0.0"/>
						</SetMannequinTag>
					</ThenActivity>
					<ElseActivity>
						<IfThenElse ResetWhenReinstalled="false" Comment="">
							<IfCondition>
								<IfMannequinMarker MarkerName="Set_Prone"/>
							</IfCondition>
							<ThenActivity>
								<SetMannequinTag ResetWhenReinstalled="false">
									<Default TagName="Prone" OnEnter="Set" OnExit="NoEffect" DelayTimer="0.0"/>
								</SetMannequinTag>
							</ThenActivity>
							<ElseActivity>
								<SetMannequinTag ResetWhenReinstalled="false">
									<Default TagName="Stand" OnEnter="Set" OnExit="NoEffect" DelayTimer="0.0"/>
								</SetMannequinTag>
							</ElseActivity>
						</IfThenElse>
					</ElseActivity>
				</IfThenElse>
				<ClearMannequinTagGroup ResetWhenReinstalled="false">
					<Default TagGroupName="CraftingItem" OnEnter="NoEffect" OnExit="Clear"/>
				</ClearMannequinTagGroup>
				<IfThenElse ResetWhenReinstalled="false" Comment="Set Turret Stuff">
					<IfCondition>
						<IfInteractionSuccess InteractionId="UseTurret"/>
					</IfCondition>
					<ThenActivity>
						<Group>
							<SetMannequinTag ResetWhenReinstalled="false">
								<Default TagName="TurretCamera" OnEnter="Set" OnExit="Unset" DelayTimer="0.0"/>
							</SetMannequinTag>
							<DisableCollision>
								<Default disableActorToActorCollision="false" disableTerrainCollision="true"/>
							</DisableCollision>
							<AnimationDrivenMotion>
								<Default MotionMultiplier="1.00" RotationMultiplier="0.00" Decel="0.0,0.0,0.0" DisableGravity="true" InheritVelocity="false"/>
							</AnimationDrivenMotion>
						</Group>
					</ThenActivity>
				</IfThenElse>
				<SetMannequinTagFromSource ResetWhenReinstalled="false" Comment="Unset Current Main Hand Weapon Tags">
					<Default TagSource="PaperdollSlot" SourceSlot="ACTIVE_MAIN_HAND_WEAPON" OnEnter="Unset" OnExit="NoEffect"/>
				</SetMannequinTagFromSource>
				<SetMannequinTagFromSource ResetWhenReinstalled="false" Comment="Unset Current Off Hand Weapon Tags">
					<Default TagSource="PaperdollSlot" SourceSlot="ACTIVE_OFF_HAND_WEAPON" OnEnter="Unset" OnExit="NoEffect"/>
				</SetMannequinTagFromSource>
				<ClearInput>
					<Default InputName="attack_primary" OnEnter="NoEffect" OnExit="Clear"/>
				</ClearInput>
				<ClearInput>
					<Default InputName="attack_primary_hold" OnEnter="NoEffect" OnExit="Clear"/>
				</ClearInput>
				<ClearInput>
					<Default InputName="attack_primary_virtual" OnEnter="NoEffect" OnExit="Clear"/>
				</ClearInput>
				<ClearInput>
					<Default InputName="attack_primary" OnEnter="NoEffect" OnExit="Clear"/>
				</ClearInput>
				<ClearInput>
					<Default InputName="attack_shoot" OnEnter="NoEffect" OnExit="Clear"/>
				</ClearInput>
				<ClearInput>
					<Default InputName="aim_shoot_hold" OnEnter="NoEffect" OnExit="Clear"/>
				</ClearInput>
				<ClearInput>
					<Default InputName="aim_shoot_depressed" OnEnter="NoEffect" OnExit="Clear"/>
				</ClearInput>
				<ClearInput>
					<Default InputName="attack_alternate_depressed_virtual" OnEnter="NoEffect" OnExit="Clear"/>
				</ClearInput>
				<ClearInput>
					<Default InputName="rune_ability" OnEnter="NoEffect" OnExit="Clear"/>
				</ClearInput>
			</Activities>
		</Action>
		<Action Name="CraftingStationInteraction" FragmentName="Crafting_Station_CraftLoop">
			<ActivationCondition>
				<And>
					<CanInteract/>
					<IsRequestingInteraction>
						<Interactions>
							<Interaction InteractionId="OpenCrafting"/>
