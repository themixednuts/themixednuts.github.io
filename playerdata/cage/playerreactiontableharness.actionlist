		</And>
		<And Name="blocked">
			<IfDamaged FailifDamageSettoNoReaction="true" IgnoreDeflectDamage="false" RequireCriticalHit="false" RequireDamageDepletedStamina="false" RequireDamageWasBlocked="true" RequireDeflectDamage="false" RequireHitVolumeName="" RequireRangedAttack="false"/>
		</And>
		<IfDamaged FailifDamageSettoNoReaction="true" IgnoreDeflectDamage="false" Name="last_hit_depleted_stamina" RequireCriticalHit="false" RequireDamageDepletedStamina="true" RequireDamageWasBlocked="false" RequireDeflectDamage="false" RequireHitVolumeName="" RequireRangedAttack="false"/>
		<Not Name="No_Damage">
			<IfDamaged FailifDamageSettoNoReaction="false" IgnoreDeflectDamage="false" RequireCriticalHit="false" RequireDamageDepletedStamina="false" RequireDamageWasBlocked="false" RequireDeflectDamage="false" RequireHitVolumeName="" RequireRangedAttack="false"/>
		</Not>
		<And Name="Stagger_Resisted">
			<Or>
				<IfGritValue RequireGritActive="true" ComparisonValue="0.00" ComparisonValueIsPercentage="false" ComparisonType="GreaterThan"/>
				<IfHasStatusEffect StatusEffectCategory="Uninterruptible"/>
			</Or>
			<Not>
				<IsStunned/>
			</Not>
		</And>
		<And Name="allow_additive">
			<Not>
				<IfMannequinTag TagName="AttackAction"/>
			</Not>
			<Not>
				<IfMannequinTag TagName="AimDownSights"/>
			</Not>
			<Not>
				<IfMannequinTag TagName="HipAim"/>
			</Not>
			<Not>
				<IfMannequinTag TagName="Reaction"/>
			</Not>
		</And>
		<IfCanBreakReaction Name="Minor_Reaction" NumberofReactions="1"/>
		<IfDamaged FailifDamageSettoNoReaction="true" IgnoreDeflectDamage="false" Name="Critical_Hit" RequireCriticalHit="true" RequireDamageDepletedStamina="false" RequireDamageWasBlocked="false" RequireDeflectDamage="false" RequireHitVolumeName="" RequireRangedAttack="false"/>
